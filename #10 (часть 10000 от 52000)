            if(strval(inputtext) <= 0 || !strlen(inputtext)) return SPD(playerid,3254,DIALOG_STYLE_INPUT,"{4F94CD}Перевод средств","{FFFFFF}Введите сумму денег которую\nхотите перевести на введенный Вами счёт:","Далее","Назад");
