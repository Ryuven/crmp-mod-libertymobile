    case 590:
        {
            if(!response) return 1;
            new itemID = GetPVarInt(playerid, "DrugsMenuDialog");
            switch(listitem) {
            case 0 : { // Использовать
                    OnPlayerUseDrug(playerid, itemID, 1);
                }
            case 1 : { // Передать
                    if(!PI[playerid][p_drugs][itemID]) return SendClientMessage(playerid, COLOR_WHITE, "* Этот слот пуст!");
                    format(stringer1, sizeof stringer1, "Передать %s", Drugs[GetPVarInt(playerid, "DrugsMenuDialog")+1][d_name]);
                    SPD(playerid, 591, DIALOG_STYLE_INPUT, stringer1, "\nВведите количество грамм\n", "Выбрать", "Выйти");
                }
            case 2 : { // Выбросить
                    if(!PI[playerid][p_drugs][itemID]) return SendClientMessage(playerid, COLOR_WHITE, "* Этот слот пуст!");
                    format(stringer1, sizeof stringer1, "Выбросить %s", Drugs[itemID+1][d_name]);
                    SPD(playerid, 593, DIALOG_STYLE_INPUT, stringer1, "Введите количество которое хотите выбросить", "Выбрать","Выйти");
                }
            }
            return 1;
        }
    case 591:
        {
            if(!response) return pc_cmd_drugs(playerid, "");
            new itemID = GetPVarInt(playerid, "DrugsMenuDialog"), level;
            if(!PI[playerid][p_drugs][itemID]) return SendClientMessage(playerid, COLOR_WHITE, "* Этот слот пуст!");
            format(stringer1, sizeof stringer1, "Передать %s", Drugs[itemID+1][d_name]);
            if(!strlen(inputtext)) {
                SPD(playerid, 591, DIALOG_STYLE_INPUT, stringer1, "\nВведите количество грамм\n", "Выбрать", "Выйти");
                return 1;
            }
            level = strval(inputtext);
            if(level < 0 || level > PI[playerid][p_drugs][itemID]) {
                format(stringer1, sizeof stringer1, "* Количество от 1 до %d!", PI[playerid][p_drugs][itemID]);
                SendClientMessage(playerid, COLOR_WHITE, stringer1);
                return 1;
            }
            SetPVarInt(playerid, "TransferDrugsQuantity", level);
            SPD(playerid, 592, DIALOG_STYLE_INPUT, stringer1, "\nВведите ID игрока\n", "Выбрать", "Выйти");
        }