        {
            if(response)
            {
                switch(listitem)
                {
                case 0:
                    {
                        if(closewarnzz[playerid] == true)
                        {
                            SendClientMessage(playerid, COLOR_LIGHTBLUE, "{EEAC11}[Успешно]{FFFFFF} Вы включили отображение уведомлений об убийстве в Зеленой Зоне !");
