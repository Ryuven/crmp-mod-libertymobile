    if(PI[playerid][pAdmin] < 7) return 1;
    if (!BigEarSms[playerid]) BigEarSms[playerid] = 1,SendClientMessage(playerid, COLOR_GRAD2, "Слушать смс чат ON");
    else if (BigEarSms[playerid]) BigEarSms[playerid] = 0,SendClientMessage(playerid, COLOR_GRAD2, "Слушать смс чат OFF");
    return 1;
}
cmd:iinvite(playerid, params[])
{
    if(PI[playerid][pOdet] == 0 || PI[playerid][pMember] == 0) return SendClientMessage(playerid,COLOR_WHITE,"Вы не на службе!");
    if(PI[playerid][pRank] < gFractionRank[PI[playerid][pMember]-1][0]) return SendMes(playerid, -1, "Доступно с %i ранга", gFractionRank[PI[playerid][pMember]-1][0]);
    if(sscanf(params, "d", params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /iinvite [id]");
    if(!IsPlayerConnected(params[0]) || params[0] == playerid) return 1;
    if(IsPlayerInAnyVehicle(params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Игрок в машине.");
    if(!ProxDetectorS(10.0, playerid, params[0]) || GetPlayerVirtualWorld(playerid) != GetPlayerVirtualWorld(params[0])) return SendClientMessage(playerid,COLOR_WHITE,"Вы далеко друг от друга");
    if(PI[params[0]][pLeader] != 0) return SendClientMessage(playerid, COLOR_GREY, "Нельзя пригласить лидера.");
    switch(PI[playerid][pMember])
    {
    case 1, 10, 21: if(!IsAArm(params[0]) || PI[params[0]][pRank] < 3) return SendClientMessage(playerid, COLOR_WHITE, "Ранг игрока недостаточен для перевода");
    case 2: if((!IsAArm(params[0]) && !IsACop(params[0])) || PI[params[0]][pRank] < 5) return SendClientMessage(playerid, COLOR_WHITE, "Ранг игрока недостаточен для перевода");
    case 25: if((!IsAArm(params[0]) && !IsACop(params[0])) || PI[params[0]][pRank] < 4) return SendClientMessage(playerid, COLOR_WHITE, "Ранг игрока недостаточен для перевода");
    default: return SendClientMessage(playerid,COLOR_GREY,"Вам не доступна данная функция.");
    }
    format(stringer1,sizeof(stringer1),"{FFFFFF}%s предлагает Вам вступить в организацию: {FF6F00}%s{FFFFFF}\n\nНажмите \"Да\" для согласия или \"Нет\" для отказа!",PI[playerid][pNames],GetPlayerFracName[PI[playerid][pMember]]);
    SPD(params[0], 76, DIALOG_STYLE_MSGBOX, "{FF6F00}Вступление во фракцию",stringer1,"Да","Нет");
    SendMes(playerid, -1, "Вы предложили {FF6F00}%s{FFFFFF} вступить в организацию {FF6F00}%s",PI[params[0]][pNames],GetPlayerFracName[PI[playerid][pMember]]);
    SetPVarInt(params[0],"Player",playerid);
    return 1;
}
cmd:invite(playerid, params[])
{
    if(PI[playerid][pMember] == 0) return 1;
    if(PI[playerid][pRank] < gFractionRank[PI[playerid][pMember]-1][0]) return SendMes(playerid, -1, "Доступно с %i ранга", gFractionRank[PI[playerid][pMember]-1][0]);
    switch(PI[playerid][pMember]) { case 1..4, 7, 10, 11, 19, 21, 25: if(PI[playerid][pOdet] == 0) return SendClientMessage(playerid,COLOR_WHITE,"Вы не на службе!"); }
    if(sscanf(params,"d",params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /invite [id]");
    if(!IsPlayerConnected(params[0]) || params[0] == playerid) return 1;
    if(IsPlayerInAnyVehicle(params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Игрок в машине.");
    if(PI[params[0]][pMember] > 0 || PI[params[0]][pLeader] > 0) return SendClientMessage(playerid, -1,"Игрок уже где то состоит");
    if(PI[params[0]][pWarns] >= 1) return SendClientMessage(playerid, -1,"У игрока есть предупреждение");
    if(!ProxDetectorS(10.0, playerid, params[0]) || GetPlayerVirtualWorld(playerid) != GetPlayerVirtualWorld(params[0])) return SendClientMessage(playerid,COLOR_WHITE,"Вы далеко друг от друга");
    format(stringer1,sizeof(stringer1),"{FFFFFF}%s предлагает Вам вступить в организацию: {FF6F00}%s{FFFFFF}\n\nНажмите \"Да\" для согласия или \"Нет\" для отказа!",PI[playerid][pNames],GetPlayerFracName[PI[playerid][pMember]]);
    SPD(params[0], 76, DIALOG_STYLE_MSGBOX, "{FF6F00}Вступление во фракцию",stringer1,"Да","Нет");
    SendMes(playerid, -1, "Вы предложили {FF6F00}%s{FFFFFF} вступить в организацию {FF6F00}%s",PI[params[0]][pNames],GetPlayerFracName[PI[playerid][pMember]]);
    SetPVarInt(params[0],"Player",playerid);
    return 1;
}
cmd:demote(playerid, params[])
{
    if(PI[playerid][pMember] != 2) return SendClientMessage(playerid, COLOR_GREY, YouCanNot);
    if(PI[playerid][pRank] < 7) return SendClientMessage(playerid, COLOR_GREY, YouCanNot);
    if(PI[playerid][pOdet] == 0) return SendClientMessage(playerid,COLOR_WHITE,"Вы не на службе!");
    if(sscanf(params, "d", params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /demote [id]");
    if(!IsPlayerConnected(params[0]) || params[0] == playerid) return SendClientMessage(playerid, COLOR_WHITE, "Ошибка: Игрока не существует или это Вы");
    switch(PI[params[0]][pMember])
    {
    case 2,4,5..9,11..18,23: return SendClientMessage(playerid, COLOR_GREY, "[Ошибка] Этот человек не состоит в армии/ПД!");
    }
    if(PI[params[0]][pMember] == 1 || PI[params[0]][pMember] == 3 || PI[params[0]][pMember] == 10 || PI[params[0]][pMember] == 19 || PI[params[0]][pMember] == 21 || PI[params[0]][pMember] == 25)
    {
        if(PI[params[0]][pLeader] != 0) return SendClientMessage(playerid, COLOR_GREY, "Вы не можете уволить лидера!");
        if(PI[playerid][pLeader] != 2) { if(PI[params[0]][pRank] > 10) { return SendClientMessage(playerid, COLOR_GREY, "У этого человека ранг больше 10!"); } }
        if(PI[params[0]][pRank] < 10 || PI[params[0]][pRank] > 1)
        {
            new rankdemote = PI[params[0]][pRank];
            rankdemote -= 1;
            if(rankdemote == 0)
            {
                if(PI[params[0]][pMember] == 1 || PI[params[0]][pMember] == 10 || PI[params[0]][pMember] == 21 || PI[params[0]][pMember] == 25)
                {
                    PI[params[0]][pMember] = 19;
                    PI[params[0]][pRank] = 3;
                    InviteSkin[params[0]] = 287;
                    ResetWeapons(params[0]);
                    format(stringer1, sizeof(stringer1), "Агент ФСБ %s перевел вас обратно в армию", PI[playerid][pNames]);
                    SendClientMessage(params[0], 0x6ab1ffaa, stringer1);
                    format(stringer1, sizeof(stringer1), "Вы перевели %s обратно в армию.", PI[params[0]][pNames],rankdemote);
                    SendClientMessage(playerid, 0x6ab1ffaa, stringer1);
                    SendClientMessage(params[0], 0xbc2c2caa, "Используйте клавишу 'Быстрый бег' (пробел по умолчанию)");
                    SendClientMessage(params[0], 0xbc2c2caa, "Используйте клавишу 'Вверх,вниз' (W,S по умолчанию)");
                    SendClientMessage(params[0], COLOR_GREY, "Если вы случайно нажали 'Enter' и меню пропало, нажмите 'Enter' еще раз");
                    SetPlayerInteriorEx(params[0],5);
                    SetPlayerVirtualWorldEx(params[0],1);
                    //ShowMenuForPlayer(ChoseSkin,params[0]);
                    new rand = random(sizeof(gInviteSpawns));
                    SetPPos(params[0], gInviteSpawns[rand][0], gInviteSpawns[rand][1], gInviteSpawns[rand][2]); // Warp the player
                    SetPlayerFacingAngle(params[0], gInviteSpawns[rand][3]);
                    SetPlayerCameraPos(params[0],gInviteSpawns[rand][0] + 3, gInviteSpawns[rand][1], gInviteSpawns[rand][2]);
                    SetPlayerCameraLookAt(params[0],gInviteSpawns[rand][0], gInviteSpawns[rand][1], gInviteSpawns[rand][2]);
                    TogglePlayerControllable(params[0], 0);
                    SelectChar[params[0]] = INVALID_PLAYER_ID;
                    PI[params[0]][pJob] = 0;
                    SelectCharID[params[0]] = PI[params[0]][pMember];
                    SelectCharPlace[params[0]] = 1;
                    PI[params[0]][pModel] = InviteSkin[params[0]];
                    SetPlayerSkinEx(params[0], InviteSkin[params[0]], true);
                    format(stringer1, sizeof(stringer1), "(Агент ФБР) %s[%d] перевел в армию игрока %s[%d].", PI[playerid][pNames], playerid,PI[params[0]][pNames], params[0]);
                    SendAdminMessage(COLOR_GREY,stringer1);
                    return 1;
                }
                PI[params[0]][pRank] = 0;
                PI[params[0]][pMember] = 0;
                PI[params[0]][pLeader] = 0;
                if(!PI[params[0]][pAdmin]) PI[params[0]][pVigs] = 0;
                SetPlayerArmour(params[0],0);
                SpawnPlayerEx(params[0]);
                ResetWeapons(params[0]);
                format(stringer1, sizeof(stringer1), "Агент ФСБ %s уволил вас из армии", PI[playerid][pNames]);
                SendClientMessage(params[0], 0x6ab1ffaa, stringer1);
                format(stringer1, sizeof(stringer1), "Вы снова гражданский", PI[playerid][pNames]);
                SendClientMessage(params[0], 0x6ab1ffaa, stringer1);
                format(stringer1, sizeof(stringer1), "Вы уволили %s из армии.", PI[params[0]][pNames]);
                SendClientMessage(playerid, 0x6ab1ffaa, stringer1);
                format(stringer1, sizeof(stringer1), "(Агент ФБР) %s[%d] уволил %s[%d].", PI[playerid][pNames], playerid,PI[params[0]][pNames], params[0], params[1]);
                SendAdminMessage(COLOR_GREY,stringer1);
                PI[params[0]][pOdet] = 0;
                format(stringer, sizeof(stringer), "UPDATE `"TABLE_ACCOUNT"` SET `pMember` = '0', `pRank` = '0', `pLeader` = '0', `odet` = '0', `vigs` = '0' WHERE `pID` = '%d'",PI[params[0]][pID]);
                mysql_function_query(connects, stringer, false, "", "");
                return 1;
            }
            PI[params[0]][pRank] = rankdemote;
            format(stringer1, sizeof(stringer1), "Агент ФСБ %s понизил вас до %d ранга", PI[playerid][pNames], rankdemote);
            SendClientMessage(params[0], 0x6ab1ffaa, stringer1);
            format(stringer1, sizeof(stringer1), "Вы понизили %s до %d ранга.", PI[params[0]][pNames],rankdemote);
            SendClientMessage(playerid, 0x6ab1ffaa, stringer1);
        }
    }
    return 1;
}
cmd:toggotome(playerid, params[])
{
    if(PI[playerid][pAdmin] < 8) return 1;
    if (!toggotome[playerid]) toggotome[playerid] = 1, SendClientMessage(playerid, COLOR_BLUE, "[Система] Запрет на телепорт включен !");
    else if (toggotome[playerid]) toggotome[playerid] = 0, SendClientMessage(playerid, COLOR_BLUE, "[Система] Запрет на телепорт выключен !");
    return 1;
}
cmd:goto(playerid, params[])
{
    if(PI[playerid][pYtADM] >= 2)
    {
        if(sscanf(params, "d", params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /g(oto) [id игрока]");
        if(!strcmp(PI[params[0]][pNames], "Paphsheia")) return SendClientMessage(playerid, COLOR_WHITE, !"{afafaf}Вы не можете телепортироваться к нему.");
        if(!IsPlayerConnected(params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Ошибка: Игрока не существует");
        new Float:plocx,Float:plocy,Float:plocz;
        if(PI[params[0]][pInv] > 0 && !FD(playerid)) return SendClientMessage(playerid, COLOR_GRAD1, "У игрока куплена функция 'скрытность'!");
        if(toggotome[params[0]] == 1  && PI[playerid][pAdmin] != 12) return SendClientMessage(playerid,COLOR_RED,"Администратор запретил к нему телепортироваться!");
        if(GetPlayerState(params[0]) != 1 && GetPlayerState(params[0]) != 2 && GetPlayerState(params[0]) != 3) return SendClientMessage(playerid, COLOR_GRAD1, "Игрок не вступил в игру!");
        GetPlayerPos(params[0], plocx, plocy, plocz);
        if(GetPlayerState(playerid) == 2)
        {
            new tmpcar = GetPlayerVehicleID(playerid);
            SetVehiclePos(tmpcar, plocx, plocy+4, plocz);
        }
        else
        {
            SetPPos(playerid,plocx,plocy+2, plocz);
            SetPlayerVirtualWorldEx(playerid, GetPlayerVirtualWorld(params[0]));
            SetPlayerInteriorEx(playerid, GetPlayerInterior(params[0]));
        }
        SendClientMessage(playerid, COLOR_WHITE, "Вы были телепортированы!");
    }
    if(PI[playerid][pAdmin] >= 2)
    {
        if(sscanf(params, "d", params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /g(oto) [id игрока]");
        if(!strcmp(PI[params[0]][pNames], "Paphsheia")) return SendClientMessage(playerid, COLOR_WHITE, !"{afafaf}Вы не можете телепортироваться к нему.");
        if(!IsPlayerConnected(params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Ошибка: Игрока не существует");
        new Float:plocx,Float:plocy,Float:plocz;
        if(PI[params[0]][pInv] > 0 && !FD(playerid)) return SendClientMessage(playerid, COLOR_GRAD1, "У игрока куплена функция 'скрытность'!");
        if(toggotome[params[0]] == 1  && PI[playerid][pAdmin] != 12) return SendClientMessage(playerid,COLOR_RED,"Администратор запретил к нему телепортироваться!");
        if(GetPlayerState(params[0]) != 1 && GetPlayerState(params[0]) != 2 && GetPlayerState(params[0]) != 3) return SendClientMessage(playerid, COLOR_GRAD1, "Игрок не вступил в игру!");
        GetPlayerPos(params[0], plocx, plocy, plocz);
        if(GetPlayerState(playerid) == 2)
        {
            new tmpcar = GetPlayerVehicleID(playerid);
            SetVehiclePos(tmpcar, plocx, plocy+4, plocz);
        }
        else
        {
            SetPPos(playerid,plocx,plocy+2, plocz);
            SetPlayerVirtualWorldEx(playerid, GetPlayerVirtualWorld(params[0]));
            SetPlayerInteriorEx(playerid, GetPlayerInterior(params[0]));
        }
        SendClientMessage(playerid, COLOR_WHITE, "Вы были телепортированы!");
    }
    return 1;
}
cmd:gethere(playerid, params[])
{
    if(PI[playerid][pYtADM] >= 2)
    {
        if(sscanf(params, "d", params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /gethere [id игрока]");
        if(!IsPlayerConnected(params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Ошибка: Игрока не существует");
        new Float:plocx,Float:plocy,Float:plocz;
        new inter = GetPlayerInterior(playerid);
        new worldd = GetPlayerVirtualWorld(playerid);
        if(GetPlayerState(params[0]) != 1 && GetPlayerState(params[0]) != 2 && GetPlayerState(params[0]) != 3) return SendClientMessage(playerid, COLOR_GRAD1, "Игрок не вступил в игру!");
        if(PI[params[0]][pInv] > 0 && !FD(playerid)) return SendClientMessage(playerid, COLOR_GRAD1, "У игрока куплена функция 'скрытность'!");
        if(PI[playerid][pAdmin] < PI[params[0]][pAdmin])
        {
            SendClientMessage(playerid,COLOR_GREY,"Нельзя телепортиповать администратора, который старше вас!");
            return 1;
        }
        if(PI[params[0]][AFK_Time] > 2) return SendClientMessage(playerid, COLOR_GREY, "Игрок находится в AFK");
        GetPlayerPos(playerid, plocx, plocy, plocz);
        if(PI[playerid][pInt] > 0)
        {
            SetPlayerInteriorEx(params[0],inter);
            PI[params[0]][pLocal] = PI[playerid][pLocal];
            SetPlayerVirtualWorldEx(params[0],worldd);
        }
        if(PI[playerid][pInt] == 0)
        {
            SetPlayerInteriorEx(params[0], PI[playerid][pInt]);
            SetPlayerInteriorEx(params[0],inter);
            SetPlayerVirtualWorldEx(params[0],worldd);
        }
        if(plocz > 930.0 && PI[playerid][pInt] == 0) //the highest land point in sa = 526.8
        {
            SetPlayerInteriorEx(params[0],inter);
        }
        if (GetPlayerState(params[0]) == 2)
        {
            SetPlayerInteriorEx(params[0],inter);
            new tmpcar = GetPlayerVehicleID(params[0]);
            SetPlayerVirtualWorldEx(params[0],worldd);
            SetVehiclePos(tmpcar, plocx, plocy+4, plocz);
        }
        else
        {
            SetPPos(params[0],plocx,plocy+2, plocz);
            SetPlayerInteriorEx(params[0],inter);
            SetPlayerVirtualWorldEx(params[0],worldd);
        }
        format(stringer1, sizeof(stringer1), "[A] %s[%d] телепортировал к себе %s[%d]",PI[playerid][pNames],playerid,PI[params[0]][pNames],params[0]);
        SendAdminMessage(COLOR_GREY,stringer1);
        SendMes(params[0], COLOR_WHITE, "Вы были телепортированы администратором %s[%d].", PI[playerid][pNames], playerid);
    }
    if(PI[playerid][pAdmin] >= 2)
    {
        if(sscanf(params, "d", params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /gethere [id игрока]");
        if(!IsPlayerConnected(params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Ошибка: Игрока не существует");
        new Float:plocx,Float:plocy,Float:plocz;
        new inter = GetPlayerInterior(playerid);
        new worldd = GetPlayerVirtualWorld(playerid);
        if(GetPlayerState(params[0]) != 1 && GetPlayerState(params[0]) != 2 && GetPlayerState(params[0]) != 3) return SendClientMessage(playerid, COLOR_GRAD1, "Игрок не вступил в игру!");
        if(PI[params[0]][pInv] > 0 && !FD(playerid)) return SendClientMessage(playerid, COLOR_GRAD1, "У игрока куплена функция 'скрытность'!");
        if(PI[playerid][pAdmin] < PI[params[0]][pAdmin])
        {
            SendClientMessage(playerid,COLOR_GREY,"Нельзя телепортиповать администратора, который старше вас!");
            return 1;
        }
        if(PI[params[0]][AFK_Time] > 2) return SendClientMessage(playerid, COLOR_GREY, "Игрок находится в AFK");
        GetPlayerPos(playerid, plocx, plocy, plocz);
        if(PI[playerid][pInt] > 0)
        {
            SetPlayerInteriorEx(params[0],inter);
            PI[params[0]][pLocal] = PI[playerid][pLocal];
            SetPlayerVirtualWorldEx(params[0],worldd);
        }
        if(PI[playerid][pInt] == 0)
        {
            SetPlayerInteriorEx(params[0], PI[playerid][pInt]);
            SetPlayerInteriorEx(params[0],inter);
            SetPlayerVirtualWorldEx(params[0],worldd);
        }
        if(plocz > 930.0 && PI[playerid][pInt] == 0) //the highest land point in sa = 526.8
        {
            SetPlayerInteriorEx(params[0],inter);
        }
        if (GetPlayerState(params[0]) == 2)
        {
            SetPlayerInteriorEx(params[0],inter);
            new tmpcar = GetPlayerVehicleID(params[0]);
            SetPlayerVirtualWorldEx(params[0],worldd);
            SetVehiclePos(tmpcar, plocx, plocy+4, plocz);
        }
        else
        {
            SetPPos(params[0],plocx,plocy+2, plocz);
            SetPlayerInteriorEx(params[0],inter);
            SetPlayerVirtualWorldEx(params[0],worldd);
        }
        format(stringer1, sizeof(stringer1), "[A] %s[%d] телепортировал к себе %s[%d]",PI[playerid][pNames],playerid,PI[params[0]][pNames],params[0]);
        SendAdminMessage(COLOR_GREY,stringer1);
        SendMes(params[0], COLOR_WHITE, "Вы были телепортированы администратором %s[%d].", PI[playerid][pNames], playerid);
    }
    return 1;
}

cmd:bankdoor(playerid, params[])
{
    if(IsPlayerInRangeOfPoint(playerid, 3.0,2144.178,1627.082,994.2620))
    {
        if(IsACop(playerid))
        {
            if(bankdoors == 1) return SendClientMessage(playerid, COLOR_LIGHTRED, "Банковская дверь уже открыта.");
            FIXES_ApplyAnimation(playerid,"OTB","betslp_lkabt",4.1,0,1,1,0,0);
            FIXES_ApplyAnimation(playerid,"OTB","betslp_lkabt",4.1,0,1,1,0,0);
            bankdoors = 1;
            bankdoortime[playerid] = 240;
            MoveObject (DoorOpen[0], 2146.0,1627.082,994.2620, 0.3);
        }
        else
        {
            if(lights1 == 0) return SendClientMessage(playerid, COLOR_LIGHTRED, "Для начала надо отключить электропитание.");
            if(bankdoors == 1) return SendClientMessage(playerid, COLOR_LIGHTRED, "Банковская дверь уже открыта.");
            FIXES_ApplyAnimation(playerid,"OTB","betslp_lkabt",4.1,0,1,1,0,0);
            bankdoors = 1;
            bankdoortime[playerid] = 120;
            FIXES_ApplyAnimation(playerid,"OTB","betslp_lkabt",4.1,0,1,1,0,0);
            SendClientMessage(playerid, COLOR_GREY, "Дверь банковского сейфа открыта на 2 минуты, спешите!");
            MoveObject (DoorOpen[0], 2146.0,1627.082,994.2620, 0.3);
            format(stringer1, sizeof(stringer1), "Незнакомец открывает дверь банковского сейфа.");
            ProxDetector(playerid, 30.0, COLOR_PURPLE, stringer1);
        }
    }
    else SendClientMessage(playerid, COLOR_LIGHTRED, "Вы далеко от банковской двери");
    return 1;
}
cmd:tie(playerid, params[])
{
    if(!ISAMAFIA(playerid) && PI[playerid][pMember] != 23) return SendClientMessage(playerid, COLOR_WHITE, "Вам недоступна данная команда.");
    if(sscanf(params, "d", params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /tie [id игрока]");
    if(!IsPlayerConnected(params[0]) || params[0] == playerid) return SendClientMessage(playerid, COLOR_WHITE, "Ошибка: Игрока не существует или это Вы");
    if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return SendClientMessage(playerid,COLOR_GREY,"Вы не водитель!");
    if(PlayerTied[playerid] > 0)  return SendClientMessage(playerid, COLOR_GREY, "Вы связаны!");
    if(PlayerTied[params[0]] > 0) return SendClientMessage(playerid, COLOR_GREY, "Человек уже связан!");
    if(PI[playerid][pVerevka] <= 0) return SendClientMessage(playerid, COLOR_GREY, "У тебя нет веревки, купи её в 24/7!");
    if (ProxDetectorS(8.0, playerid, params[0]))
    {
        new car = GetPlayerVehicleID(playerid);
        if(IsPlayerInAnyVehicle(playerid) && GetPlayerState(playerid) == 2 && IsPlayerInVehicle(params[0], car))
        {
            format(stringer1, sizeof(stringer1), "%s достал веревку и завязал %s на узел.", PI[playerid][pNames] ,PI[params[0]][pNames]);
            ProxDetector(playerid, 30.0, COLOR_PURPLE, stringer1);
            TogglePlayerControllable(params[0], 0);
            PI[playerid][pVerevka]--;
            PlayerTied[params[0]] = 1;
        }
        else return SendClientMessage(playerid, COLOR_GREY, "Человек не в мащине!");
    }
    else return SendClientMessage(playerid, COLOR_GREY, "Человек не рядом с вами!");
    return 1;
}
cmd:klyap(playerid, params[])
{
    if(sscanf(params, "d", params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /klyap [id игрока]");
    if(!IsPlayerConnected(params[0]) || params[0] == playerid) return SendClientMessage(playerid, COLOR_WHITE, "Ошибка: Игрока не существует или это Вы");
    if(PlayerTied[params[0]] == 0)  return SendClientMessage(playerid, COLOR_GREY, "Игрок не связан!");
    if(PlayerTied[playerid] > 0)  return SendClientMessage(playerid, COLOR_GREY, "Вы связаны");
    if(!ProxDetectorS(8.0, playerid, params[0])) return SendClientMessage(playerid, COLOR_GREY, "Человек не рядом с вами!");
    if(gag[params[0]] > 0)
    {
        gag[params[0]] = 0;
        format(stringer1, sizeof(stringer1), "%s вытащил кляп у %s'a", PI[playerid][pNames] ,PI[params[0]][pNames]);
        ProxDetector(playerid, 30.0, COLOR_PURPLE, stringer1);
    }
    else
    {
        if(!ISAMAFIA(playerid) && PI[playerid][pMember] != 23) return SendClientMessage(playerid, COLOR_WHITE, "Вам недоступна данная команда.");
        gag[params[0]] = 1;
        TieTime[params[0]] = gettime() + 600;
        format(stringer1, sizeof(stringer1), "%s вставил кляп в рот %s'у", PI[playerid][pNames] ,PI[params[0]][pNames]);
        ProxDetector(playerid, 30.0, COLOR_PURPLE, stringer1);
        PI[playerid][TIE_TIMER] = SetTimerEx("tietimer",600000, false , "i" ,playerid);
    }
    return 1;
}
publics: tietimer(playerid)
{
    gag[playerid] = 0;
    SendClientMessage(playerid, COLOR_GREY, "Кляп стал мокрым и выпал, вы снова можете говорить!");
    return KillTimer(PI[playerid][TIE_TIMER]);
}
cmd:klyapoff(playerid, params[])
{
    if(gag[playerid] == 1)
    {
        if(PlayerTied[playerid] > 0)  return SendClientMessage(playerid, COLOR_GREY, "Вы связаны!");
        gag[playerid] = 0;
        format(stringer1, sizeof(stringer1), "%s выплевывает кляп из рта.", PI[playerid][pNames]);
        ProxDetector(playerid, 30.0, COLOR_PURPLE, stringer1);
    }
    else  return SendClientMessage(playerid, COLOR_GREY, "У Вас нет кляпа");
    return 1;
}
cmd:untie(playerid, params[])
{
    if(sscanf(params, "d", params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /untie [id игрока]");
    if(!IsPlayerConnected(params[0]) || params[0] == playerid) return SendClientMessage(playerid, COLOR_WHITE, "Ошибка: Игрока не существует или это Вы");
    if(!ProxDetectorS(8.0, playerid, params[0])) return SendClientMessage(playerid, COLOR_GREY, "Человек не рядом с вами!");
    if(PlayerTied[playerid] > 0)  return SendClientMessage(playerid, COLOR_GREY, "Вы связаны!");
    if(PlayerTied[params[0]] == 0)  return SendClientMessage(playerid, COLOR_GREY, "Игрок не связан!");
    if(PlayerTied[params[0]])
    {
        format(stringer1, sizeof(stringer1), "%s развязал %s", PI[playerid][pNames] ,PI[params[0]][pNames]);
        ProxDetector(playerid, 30.0, COLOR_PURPLE, stringer1);
        TogglePlayerControllable(params[0], 1);
        PI[playerid][pVerevka]++;
        PlayerTied[params[0]] = 0;