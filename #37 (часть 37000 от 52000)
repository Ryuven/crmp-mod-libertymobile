cmd:limit(playerid, params[])
{
    if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return SendClientMessage(playerid,COLOR_GREY,"Вы должны быть за рулем");
    if(sscanf(params, "d", params[0])) return SendClientMessage(playerid, -1, "• "SERVER"[Подсказка] {ffffff}Используйте /limit [скорость]");
    if(!params[0])
    {
        if(PI[playerid][pLimit])
        {
            PI[playerid][pLimit] = 0;
            SendClientMessage(playerid,COLOR_GREEN,"Ограничение скорости снято");
        }
        else SendClientMessage(playerid,COLOR_GREY,"• "SERVER"[Подсказка] {ffffff}Используйте /limit [скорость]");
        return 1;
    }
    if(params[0] < 10 || params[0] > 150) return SendClientMessage(playerid,COLOR_GREY,"Ограничение должно быть от 10 до 150");
    PI[playerid][pLimit] = params[0];
    SendClientMessage(playerid,COLOR_GREEN,"Установлено ограничение скорости");
    return 1;
}
cmd:light(playerid)
{
    if(GetPlayerVehicleID(playerid) == INVALID_VEHICLE_ID) return 1;
    if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return 1;
    GetVehicleParamsEx(GetPlayerVehicleID(playerid),engine,lights,alarm,doors,bonnet,boot,objective);

    if(!lights)
    {
        SetVehicleParamsEx(GetPlayerVehicleID(playerid), engine,VEHICLE_PARAMS_ON,alarm,doors,bonnet,boot,objective);
        GameTextForPlayer ( playerid, "~g~LIGHT ON", 2000, 4 );
    }
    else
    {
        SetVehicleParamsEx(GetPlayerVehicleID(playerid), engine, VEHICLE_PARAMS_OFF,alarm,doors,bonnet,boot,objective);
        GameTextForPlayer ( playerid, "~r~LIGHT OFF", 2000, 4 );
    }
    return 1;
}
cmd:en(playerid, params[])
{
    if(GetPlayerVehicleID(playerid) == INVALID_VEHICLE_ID) return 1;
    if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return 1;
    if(zavodis[playerid] == 0)
    {
        new idd = GetPlayerVehicleID(playerid);
        if(GetVehicleData(idd, V_FUEL) <= 0) return SendClientMessage(playerid,COLOR_RED,"В машине нет бензина");
        GetVehicleParamsEx(GetPlayerVehicleID(playerid),engine,lights,alarm,doors,bonnet,boot,objective);
        SetVehicleParamsEx(GetPlayerVehicleID(playerid) ,VEHICLE_PARAMS_ON,lights,alarm,doors,bonnet,boot,objective);
        zavodis[playerid] = 1;
        GameTextForPlayer ( playerid, "~g~ENGINE ON", 2000, 4 );
    }
    else if(zavodis[playerid] == 1)
    {
        GetVehicleParamsEx(GetPlayerVehicleID(playerid),engine,lights,alarm,doors,bonnet,boot,objective);
        SetVehicleParamsEx(GetPlayerVehicleID(playerid),VEHICLE_PARAMS_OFF,lights,alarm,doors,bonnet,boot,objective);
        zavodis[playerid] = 0;
        GameTextForPlayer ( playerid, "~r~ENGINE OFF", 2000, 4 );
    }
    return 1;
}
cmd:togphone(playerid)
{
    if(PI[playerid][pDonateRank] >= 1 || PI[playerid][pAdmin] >= 1 || PI[playerid][pLeader] >= 1)
    {
        if(!PhoneOnline[playerid]) PhoneOnline[playerid] = 1,SendClientMessage(playerid, COLOR_WHITE, "Вы выключили телефон");
        else if(PhoneOnline[playerid]) PhoneOnline[playerid] = 0,SendClientMessage(playerid, COLOR_WHITE, "Вы включили телефон");
    }
    else return SendClientMessage(playerid, COLOR_GREY, "Вы не VIP/Админ/Лидер!");
    return 1;
}
cmd:divorce(playerid, params[])
{
    if(!IsPlayerInRangeOfPoint(playerid, 10.0, 1879.8152,1170.2408,30.0091)) return SendClientMessage(playerid, COLOR_GREY, "Вы не в церкви!");
    if(PI[playerid][pMarried] < 1) return SendClientMessage(playerid, COLOR_GREY, "Вы не женаты (замужем)!");
    SendClientMessage(playerid, COLOR_GREY, "Вы развелись, ваш(а) супруг(а) должна сделать тоже самое");
    PI[playerid][pMarried] = 0;
    return 1;
}
cmd:propose(playerid, params[])
{
    if(!IsPlayerInRangeOfPoint(playerid, 10.0, 1879.8152,1170.2408,30.0091)) return SendClientMessage(playerid, COLOR_GREY, "Вы не в церкви!");
    if(PI[playerid][pCash] < 100000) return SendClientMessage(playerid, COLOR_GREY, "Вам нужно 100 000 на свадьбу!");
    if(PI[playerid][pMarried] > 0) return SendClientMessage(playerid, COLOR_GREY, "Вы уже женаты/замужем!");
    if(sscanf(params, "d", params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /propose [id]");
    if(!IsPlayerConnected(params[0]) || params[0] == playerid) return SendClientMessage(playerid, COLOR_WHITE, "Ошибка: Игрока не существует или это Вы");
    if(PI[params[0]][pMarried] > 0) return SendClientMessage(playerid, COLOR_GREY, "Человек уже состоит в браке!");
    if(!ProxDetectorS(8.0, playerid, params[0])) return SendClientMessage(playerid, COLOR_GREY, "Человек не рядом с вами!");
    format(stringer1, sizeof(stringer1), "Вы предложили руку и сердце %s.", PI[params[0]][pNames]);
    SendClientMessage(playerid, 0x6ab1ffaa, stringer1);
    format(stringer1, sizeof(stringer1), "%s предлагает вам руку и сердце. (( Введите: /accept marriage, чтобы принять )).", PI[playerid][pNames]);
    SendClientMessage(params[0], COLOR_BLUE, stringer1);
    ProposeOffer[params[0]] = playerid;
    return 1;
}
