cmd:hotdog(playerid, params[])
{
    if(PI[playerid][pJob] != 3) return SendClientMessage(playerid, COLOR_WHITE, "Вы не продавец ХотДогов!");
    if(GetVehicleModel(GetPlayerVehicleID(playerid)) != 588) return SendClientMessage(playerid,COLOR_GREY,"Вы не в машине продажи хотдого!");
    if(sscanf(params, "dd", params[0],params[1])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /hotdog [id игрока] [цена]");
    if(params[1] < 10 || params[1] > 999) return SendClientMessage(playerid, COLOR_GREY, "Цена должна быть: от 10 до 999!");
    if(!IsPlayerConnected(params[0]) || params[0] == playerid) return SendClientMessage(playerid, COLOR_WHITE, "Ошибка: Игрока не существует или это Вы");
    if(!ProxDetectorS(8.0, playerid, params[0])) return SendClientMessage(playerid, COLOR_GREY, "Человек не рядом с вами");
    format(stringer1, sizeof(stringer1), "Вы предложили %s ХотДог за %d руб.",PI[params[0]][pNames],params[1]);
    SendClientMessage(playerid, 0x6ab1ffaa, stringer1);
    format(stringer1, sizeof(stringer1), "%s предлагает Вам ХотДог за %d руб.. ((  Введите: /accept hotdog для покупки ХотДога ))",PI[playerid][pNames],params[1]);
    SendClientMessage(params[0], 0x6ab1ffaa, stringer1);
    HotdogOffer[params[0]] = playerid;
    HotdogPrice[params[0]] = params[1];
    return 1;
}
cmd:setmats(playerid, params[])
{
    if(PI[playerid][pAdmin] < 8) return 1;
    if(sscanf(params, "dd", params[0],params[1]))
    {
        SendClientMessage(playerid, COLOR_WHITE, "Введите: /setmats [номер] [количество]");
        SendClientMessage(playerid, COLOR_WHITE, "[ГОС]: 1 - Армия LV, 2 - Армия SF, 3 - ФБР, 4 - ЛСПД, 5 - СФПД, 6 - ЛВПД");
        SendClientMessage(playerid, COLOR_WHITE, "[МАФИИ]: 7 - ЯКИ, 8 - ЛКН, 9 - РМ");
        SendClientMessage(playerid, COLOR_WHITE, "[ВСЕ]: 10");
        return 1;
    }
    switch(params[0])
    {
    case 1: { armmatbi = params[1]; format(stringer1, sizeof(stringer1), "Маты армии 51 изменены на: %d", params[1]); }
    case 2: { armmatsf = params[1]; format(stringer1, sizeof(stringer1), "Маты армии Авианосец изменены на: %d", params[1]); }
    case 3: { fbimats = params[1]; format(stringer1, sizeof(stringer1), "Маты ФСБ изменены на: %d", params[1]); }
    case 4: { lspdmatbi = params[1]; format(stringer1, sizeof(stringer1), "Маты МВД Г. Арзамас изменены на: %d", params[1]); }
    case 5: { sfpdmats = params[1]; format(stringer1, sizeof(stringer1), "Маты SFPD изменены на: %d", params[1]); }
    case 6: { lvpdmats = params[1]; format(stringer1, sizeof(stringer1), "Маты LVPD изменены на: %d", params[1]); }
    case 7: { yakuzamats = params[1]; format(stringer1, sizeof(stringer1), "Маты Якудза изменены на: %d", params[1]); }
    case 8: { lcnmats = params[1]; format(stringer1, sizeof(stringer1), "Маты ЛКН изменены на: %d", params[1]); }
    case 9: { rmmats = params[1]; format(stringer1, sizeof(stringer1), "Маты РМ изменены на: %d", params[1]); }
    case 10:
        {
            armmatbi = params[1];
            armmatsf = params[1];
            lspdmatbi = params[1];
            fbimats = params[1];
            yakuzamats = params[1];
            rmmats = params[1];
            lcnmats = params[1];
            sfpdmats = params[1];
            lvpdmats = params[1];
            format(stringer1, sizeof(stringer1), "Маты ВСЕМ фракциям изменены на %d", params[1]);
        }
    default: format(stringer1, sizeof(stringer1), "[Ошибка] Неверный номер!");
    }
    SendClientMessage(playerid, COLOR_GRAD1, stringer1);
    SaveOther();
    return 1;
}
cmd:setstat(playerid, params[])
{
    if(!FD(playerid)) return 1;
    if(sscanf(params, "ddd", params[0],params[1],params[2]))
    {
        SendClientMessage(playerid, COLOR_WHITE, "Введите: /setstat [id] [номер] [количество]");
        SendClientMessage(playerid, COLOR_GRAD2, "[1] Уровень | [2] Законка |[3] Маты | [4] Скин | [5] Убийств");
        SendClientMessage(playerid, COLOR_GRAD2, "[6] Номер телефона | [7] Exp | [8] Ключ от дома | [9] Номер бизнеса");
        SendClientMessage(playerid, COLOR_GRAD2, "[10] VIP [1-3] | [11] Работа игрока | [12] Аптечки | [13] Деньги в банке");
        SendClientMessage(playerid, COLOR_GRAD2, "[14] Мобильный | [15] Деньги | [16] Варны | 17] Аптечки");
        SendClientMessage(playerid, COLOR_GRAD2, "[18] Член орг. | [19] Скилл бокса | [20] Время бокса");
        SendClientMessage(playerid, COLOR_GRAD2, "[21] Бокс | [22] конг Фу | [23] КикБокс | [24] Уважение | [25] Бег");
        SendClientMessage(playerid, COLOR_GRAD2, "[26] Машина | [27] Машина | [28] Наркозависимость");
        SendClientMessage(playerid, COLOR_GRAD2, "[29] Фрак. скин | [30] Муж/Жена | [31] Процы | [32] Время банка");
        SendClientMessage(playerid, COLOR_GRAD2, "[33] Доступ к /ban | [34] Доступ к /warn");
        return 1;
    }
    if(!IsPlayerConnected(params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Ошибка: Игрока не существует");
    switch(params[1])
    {
    case 1: { PI[params[0]][pLevel] = params[2]; SendMes(playerid,COLOR_GRAD1, "LVL игрока изменён на: %d", params[2]); }
    case 2: { PI[params[0]][pZakonp] = params[2]; SendMes(playerid,COLOR_GRAD1, "Законка: %d ", params[2]); }
    case 3: { PI[params[0]][pMats] = params[2]; SendMes(playerid,COLOR_GRAD1, "Маты: %d ", params[2]); }
    case 4: { PI[params[0]][pChar] = params[2]; SendMes(playerid,COLOR_GRAD1, "Скин игрока установлен на: %d", params[2]); }
    case 5: { PI[params[0]][pKills] = params[2]; SendMes(playerid,COLOR_GRAD1, "Убийства: %d", params[2]); }
    case 6: { PI[params[0]][pPnumber] = params[2]; SendMes(playerid,COLOR_GRAD1, "Номер телефона игрока изменён на: %d", params[2]); }
    case 7: { PI[params[0]][pExp] = params[2]; SendMes(playerid,COLOR_GRAD1, "EXP игрока изменены на: %d", params[2]); }
    case 8: { PI[params[0]][pPhousekey] = params[2]; SendMes(playerid,COLOR_GRAD1, "Дом: %d", params[2]); }
    case 9: { SetPVarInt(params[0], "PlayerBizz", params[2]); SendMes(playerid,COLOR_GRAD1, "Биз: %d", params[2]); }
    case 10: { PI[params[0]][pDonateRank] = params[2]; SendMes(playerid,COLOR_GRAD1, "VIP аккаунт уровня: %d", params[2]); }
    case 11: { PI[params[0]][pJob] = params[2]; SendMes(playerid,COLOR_GRAD1, "Работа %d", params[2]); }
    case 13: { PI[params[0]][pBank] = params[2]; SendMes(playerid,COLOR_GRAD1, "Деньги в банке изменены  на %d руб.", params[2]); }
    case 15: { PI[params[0]][pCash] = params[2]; SendMes(playerid,COLOR_GRAD1, "Деньги изменены на %d руб.", params[2]); }
    case 14: { PI[params[0]][pMobile] = params[2]; SendMes(playerid,COLOR_GRAD1, "Мобила %d руб.", params[2]); }
    case 16: { PI[params[0]][pWarns] = params[2]; SendMes(playerid,COLOR_GRAD1, "Количество Варном изменено на %d ", params[2]); }
    case 17: { PI[params[0]][pHeal] = params[2]; SendMes(playerid,COLOR_GRAD1, "Количество аптечек изменено на %d ", params[2]); }
    case 18: { PI[params[0]][pMember] = params[2]; PI[params[0]][pRank] = 1; SendMes(playerid,COLOR_GRAD1, "pMember %d. ", params[2]); }
    case 19: { PI[params[0]][pFightSkill] = params[2]; SendMes(playerid,COLOR_GRAD1, "Box skill %d. ", params[2]); }
    case 20: { PI[params[0]][pFightTime] = params[2]; SendMes(playerid,COLOR_GRAD1, "Box time %d. ", params[2]); }
    case 21: { PI[params[0]][pBoxSkill] = params[2]; SendMes(playerid,COLOR_GRAD1, "Уровень боевых искусств БОКС %d ", params[2]); }
    case 22: { PI[params[0]][pKongfuSkill] = params[2]; SendMes(playerid,COLOR_GRAD1, "Уровень боевых искусств  КОНГ ФУ%d ", params[2]); }
    case 23: { PI[params[0]][pKickboxSkill] = params[2]; SendMes(playerid,COLOR_GRAD1, "Уровень боевых искусств  Кик Бокс %d ", params[2]); }
    case 28: { PI[params[0]][pNarcoLomka] = params[2]; SendMes(playerid,COLOR_GRAD1, "Зависимость %d ", params[2]); }
    case 29: { PI[params[0]][pModel] = params[2]; SendMes(playerid,COLOR_GRAD1, "pModel %d ", params[2]); }
    case 30: { PI[params[0]][pMarried] = 0; SendMes(playerid,COLOR_GRAD1, "pMarriedTo %d ", params[2]); }
    case 31: { PI[params[0]][pProz] = params[2]; SendMes(playerid,COLOR_GRAD1, "процы %d ", params[2]); }
    case 32: { PI[params[0]][pRobTime] = params[2]; SendMes(playerid,COLOR_GRAD1, "Время банка %d ", params[2]); }
    case 33: { PI[params[0]][pcBan] = params[2]; SendMes(playerid,COLOR_GRAD1, "Значение изменено на (%d) ", params[2]); }
    case 34: { PI[params[0]][pcWarn] = params[2]; SendMes(playerid,COLOR_GRAD1, "Значение изменено на (%d) ", params[2]); }
    default: return SendClientMessage(playerid, COLOR_GRAD1, "Ошибка: Неверный номер!");
    }
    OnPlayerUpdateRL(params[0]);
    format(stringer1, sizeof(stringer1), "изменил статистику: [%d] - [%d]", params[1], params[2]);
    Admin_Log(playerid, stringer1, PI[params[0]][pNames], gettime());
    return 1;
}
cmd:setcena(playerid, params[])
{
    if(PI[playerid][pAdmin] != 13) return 1;
    if(sscanf(params, "dd", params[0],params[1])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /setcena [дом] [цена]");
    if(params[0] < 0 || params[0] > 1999) return 1;
    if(params[1] < 0 || params[1] > 100000000) return 1;
    HouseInfo[params[0]][hValue] = params[1];
    SendClientMessage(playerid, COLOR_WHITE, "Цена установлена");
    return 1;
}

cmd:asetpos(playerid, params[])
{
    if(PI[playerid][pAdmin] != 13) return 1;
    if(sscanf(params, "d", params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /asetpos [дом]");
    if(params[0] < 0 || params[0] > 1999) return 1;
    new Float: lwx, Float:lwy, Float:lwz;
    GetPlayerPos(playerid, lwx, lwy, lwz);
    HouseInfo[params[0]][hEntrancex] = lwx;
    HouseInfo[params[0]][hEntrancey]= lwy;
    HouseInfo[params[0]][hEntrancez] = lwz;
    format(mysql_query_, sizeof(mysql_query_), "UPDATE `"TABLE_HOUSE"` SET `hEntrancex` = '%f', `hEntrancey` = '%f', `hEntrancez` = '%f' WHERE `hID` = '%i'",lwx,lwy,lwz,params[0]);
    mysql_function_query(connects, mysql_query_, false, "", "");
    BuyHouse(params[0]);
    SendClientMessage(playerid, COLOR_GREY, "Координаты дома установлена");
    return 1;
}
