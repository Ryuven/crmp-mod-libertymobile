cmd:getgun(playerid, params[])
{
    switch(PI[playerid][pMember])
    {
    case 19:
        {
            if(GetVehicleModel(GetPlayerVehicleID(playerid)) != 470) return SendClientMessage(playerid,COLOR_GREY,"Вы не в Хаммере!");
            if(armmatbi <= 0) return SendClientMessage(playerid,COLOR_GRAD1,"На складе нет оружия!");
            if(PI[playerid][pGun51Time] > gettime()) return     SendClientMessage(playerid,COLOR_GRAD1,"Брать оружие можно раз в 5 минут");
            GiveWeapon(playerid, 30, 100);
            SetPlayerArmour(playerid, 100);
            SetPlayerHealth(playerid,100);
            armmatbi -= 500;
            PI[playerid][pGun51Time] = gettime() + 60;
            SetPlayerChatBubble(playerid,"взял оружие из багажника",COLOR_PURPLE,30.0,10000);
            SendClientMessage(playerid, COLOR_ISPOLZUY,"Вы достали из багажника: AK-47 (100), Бронижелет, Сухой паёк");
            return 1;
        }
    case 3:
        {
            if(GetVehicleModel(GetPlayerVehicleID(playerid)) != 470) return SendClientMessage(playerid,COLOR_GREY,"Вы не в Хаммере!");
            if(armmatsf <= 0) return SendClientMessage(playerid,COLOR_GRAD1,"На складе нет оружия!");
            if(PI[playerid][pGunaviaTime] > gettime()) return SendClientMessage(playerid,COLOR_GRAD1,"Брать оружие можно раз в 5 минут");
            GiveWeapon(playerid, 31, 100);
            SetPlayerArmour(playerid, 100);
            SetPlayerHealth(playerid,100);
            armmatsf -= 500;
            PI[playerid][pGunaviaTime] = gettime() + 60;
            SetPlayerChatBubble(playerid,"взял оружие из багажника",COLOR_PURPLE,30.0,10000);
            SendClientMessage(playerid, COLOR_ISPOLZUY,"Вы достали из багажника: M4 (100), Бронижелет, Сухой паёк");
            return 1;
        }
    }
    return 1;
}
