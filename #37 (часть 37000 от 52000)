cmd:frisk(playerid, params[])
{
    if(!IsACop(playerid)) return SendClientMessage(playerid, COLOR_GREY, "Вы не член Полиции/ФБР/Армии!");
    if(sscanf(params, "d", params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /frisk [id]");
    if(!IsPlayerConnected(params[0]) || params[0] == playerid) return SendClientMessage(playerid, COLOR_WHITE, "Ошибка: Игрока не существует или это Вы");
    if(!ProxDetectorS(3.0, playerid, params[0])) return SendClientMessage(playerid, COLOR_GREY, "Этого игрока рядом с вами нет !");
    new text1[20], text2[20];
    if(PI[params[0]][p_drugs][0] > 0 || PI[params[0]][p_drugs][1] > 0 || PI[params[0]][p_drugs][2] > 0 || PI[params[0]][p_drugs][3] > 0
            || PI[params[0]][p_drugs][4] > 0 || PI[params[0]][p_drugs][5] > 0 || PI[params[0]][p_drugs][6] > 0) text1 = "| Наркотики.";
    else text1 = "| Пустой карман.";
    if(PI[params[0]][pMats] > 0) { text2 = "| Материалы."; } else { text2 = "| Пустой карман."; }
    SendMes(playerid, COLOR_WHITE, ":::: Вещи %s ::::", PI[params[0]][pNames]);
    SendMes(playerid, COLOR_GREY, "%s.", text1);
    SendMes(playerid, COLOR_GREY, "%s.", text2);
    format(stringer1, sizeof(stringer1), "%s обыскал %s", PI[playerid][pNames] ,PI[params[0]][pNames]);
    ProxDetector(playerid, 30.0, COLOR_PURPLE, stringer1);
    return 1;
}
