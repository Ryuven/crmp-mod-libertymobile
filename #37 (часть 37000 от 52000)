    {
        new null;
        for(new b = 1; b <= TOTALBIZZ; b++)
        {
            if(!IsPlayerInRangeOfPoint(playerid, 10.0, BizzInfo[b][bEntrx], BizzInfo[b][bEntry], BizzInfo[b][bEntrz]) || BizzInfo[b][bType] != 2) continue;
            new fuel = BizzInfo[b][bTill] / 3;
            if(BizzInfo[b][bProduct] < 20 || BizzInfo[b][bLock] == 1) return SendClientMessage(playerid,COLOR_GRAD1,"На заправке не хватает топлива!");
            if(PI[playerid][pFuel] > 3) return SendClientMessage(playerid,COLOR_GREY,"У вас уже есть 3 канистры.");
            if(PI[playerid][pCash] < fuel * 20) return SendClientMessage(playerid,COLOR_GRAD1, "Не достаточно денег!");
            PI[playerid][pCash] -= fuel * 20;
            BizzPay[b] += fuel * 20;
            BizzInfo[b][bMoney] += fuel * 20;
            BizzInfo[b][bProduct] -= 20;
            PI[playerid][pFuel]++;
            SendMes(playerid,0x6495EDFF,"Вы купили канистру с 20 литрами бензина за %i руб.",fuel * 20);
            null++;
            break;
        }
        if(null == 0) return SendClientMessage(playerid, COLOR_GREY, "Вы должны находится у заправки!");
    }
    else if(strcmp(x_job, "paintball", true) == 0)
    {
        if(PaintList)
        {
            new winner;
            stringer[0] = 0;
            strcat(stringer,"{0004FF}Команда синих:\n");
            for(new i = 0; i < PaintList; i++) if(PaintInfo[i][Type] == 1) format(stringer1,sizeof(stringer1),"\n%s\t\tУбийств: %i\t\tСмертей: %i",PaintInfo[i][Name],PaintInfo[i][Kills],PaintInfo[i][Deaths]), strcat(stringer,stringer1), winner+=PaintInfo[i][Kills];
            format(stringer1,sizeof(stringer1),"\n___________________________________________\nОбщее количество убийств: %i\n\n",winner), strcat(stringer,stringer1);
            winner = 0;
            strcat(stringer,"{FF4500}Команда красных:\n");
            for(new i = 0; i < PaintList; i++) if(PaintInfo[i][Type] == 2) format(stringer1,sizeof(stringer1),"\n%s\t\tУбийств: %i\t\tСмертей: %i",PaintInfo[i][Name],PaintInfo[i][Kills],PaintInfo[i][Deaths]), strcat(stringer,stringer1), winner+=PaintInfo[i][Kills];
            format(stringer1,sizeof(stringer1),"\n___________________________________________\nОбщее количество убийств: %i",winner), strcat(stringer,stringer1);
            SPD(playerid, 000, DIALOG_STYLE_MSGBOX, "Отчет по матчу", stringer, "Закрыть", "");
        }
        else SPD(playerid, 000, DIALOG_STYLE_MSGBOX, "Отчет по матчу", "Матч не состоялся!", "Закрыть", "");
        return 1;
    }
    return 1;
}
