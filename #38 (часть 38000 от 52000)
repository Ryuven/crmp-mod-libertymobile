
    }
    else return SendClientMessage(playerid, COLOR_GREY, "Этот человек не связан!");
    return 1;
}

CMD:breakcar(playerid)
{
    if(GetPVarInt(playerid,"AntiFloodJack") > gettime()) return SendClientMessage(playerid, COLOR_WHITE, !"{afafaf}Подождите 1 минуту, прежде чем пытаться взломать автомобиль снова.");
    if(GetPVarInt(playerid, "JackCar_use") == 1) return SendClientMessage(playerid, COLOR_WHITE, !"{afafaf}Вы уже взламываете транспорт!");
    if(!PI[playerid][pOtmbI4ka]) return SendClientMessage(playerid, COLOR_GRAD1, "У вас нет отмычек! (Купить их можно в магазине 24/7)");
    if(IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid, COLOR_GREY, "Нельзя использовать в машине!");
    new tacha = GetNearestVehicle(playerid);
    if(tacha == -1) return SendClientMessage(playerid, COLOR_GREY, "Необходимо находится возле машины!");
    new Float:X,Float:Y,Float:Z;
    GetVehiclePos(tacha, X, Y, Z);
    if(!IsPlayerInRangeOfPoint(playerid, 2.0, X, Y, Z)) return SendClientMessage(playerid, COLOR_GREY, "Необходимо неходится возле машины!");
    if(tacha != PI[playerid][pUgonVehicle]) return SendClientMessage(playerid, COLOR_GREY, "Вы не можете взломать чужой транспорт!");
    if(PlayerTied[playerid] || PlayerCuffed[playerid]) return SendClientMessage(playerid, COLOR_GREY, "Вы не можете двигаться!");
    PI[playerid][pOtmbI4ka]--;

    new Random_1 = RandomEx(2, 5),
        Random_2 = RandomEx(6, 10);

    SetPVarInt(playerid, "random_code", Random_1 * Random_2);

    new scm_string[160];
    format(scm_string, sizeof(scm_string), "{ffffff}Защита транспорта требует код-пароль для взлома.\n{32CD32}Напишите, сколько будет: {B22222}%d умножить на %d?\n{cccccc}(У вас 1 попытка).", Random_1, Random_2);
    SPD(playerid, 6975, DIALOG_STYLE_INPUT, !""SERVER"Спутниковая сигнализация", scm_string, !"Далее", !"Отмена");

    /*
    timer_jack[playerid] = SetTimerEx("JackCar", 5000, 0, "dd", playerid, tacha);
    TogglePlayerControllable(playerid, false);
    FIXES_ApplyAnimation(playerid,"ROB_BANK","CAT_Safe_Rob",4.1, 0, 1, 1, 1, 0, 1);
    SetPVarInt(playerid, "JackCar_use", 1);

    stringer1[0] = EOS;
    format(stringer1, 90, "%s взламывает автомобиль", PI[playerid][pNames]);
    ProxDetector(playerid, 30.0, COLOR_PURPLE, stringer1);
    */

    return 1;
}

publics: JackCar(playerid, vehicleid)
{
    if(PI[playerid][pUgonTimer] > 0)
    {
        stringer1[0] = EOS;
        format(stringer1, 90, "%s дверь автомобиля взломана {33aa33}| Удачно",PI[playerid][pNames]);
        ProxDetector(playerid, 30.0, COLOR_PURPLE, stringer1);

        SetVehicleParam(vehicleid, V_LOCK, 0);
        GameTextForPlayer(playerid, "~w~~n~~n~~n~~n~~n~~n~~n~~n~~n~~n~CAR~g~ UNLOCKED", 3000, 3);

        SetPVarInt(playerid, "AntiFloodJack", gettime() + 60);
        ClearAnimations(playerid);
        TogglePlayerControllable(playerid, true);
        KillTimer(timer_jack[playerid]);
        DeletePVar(playerid, "JackCar_use");

        SetPlayerCheckpoint(playerid, 2568.9939,-1892.1960,21.9077,8);

        stringer1[0] = EOS;
        format(stringer1, sizeof(stringer1), "[SMS от Володи]: Всё идёт по плану! Координаты места сбыта отправил на твой радар. Отправитель: Володя.");
        SendClientMessage(playerid, COLOR_YELLOW, stringer1);

        return SendClientMessage(playerid, COLOR_WHITE, !"{33aa33}Вы успешно взломали автомобиль. Угоните его и сдайте в место сбыта {afafaf}[/dropcar]");
    }
    return 1;
}

CMD:dropcar(playerid)
{
    if(PI[playerid][pUgonTimer] > 0)
    {
        SetPlayerCheckpoint(playerid,2568.9939,-1892.1960,21.9077,8);
        SendClientMessage(playerid, COLOR_WHITE, "{33aa33}Место сдачи транспорта отмечено на карте.");
    }
    else return 1;
    return true;
}

cmd:present(playerid,params[])
{
    if(sscanf(params, "d", params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /present [id]");
    if(!IsPlayerConnected(params[0]) || params[0] == playerid) return SendClientMessage(playerid, COLOR_WHITE, "Ошибка: Этого игрока нет, или вы указали свой ид");
    if(!ProxDetectorS(5.0, playerid, params[0])) return SendClientMessage(playerid, COLOR_GREY, "Этого игрока рядом с вами нет!");
    if(GetPlayerWeapon(playerid) != 14) return SendClientMessage(playerid, COLOR_GREY, "У тебя в руках должен быть цветы");
    ApplyAnimation(playerid,"Kissing","gift_give",4.0,0,0,0,0,0,1);
    GiveWeapon(params[0], 14, 1);
    PI[playerid][Weapon][10] = 0;
    PI[playerid][Ammo][10] = 0;
    ResetPlayerWeapons(playerid);
    SetPlayerWeaponss(playerid);
    format(stringer1, sizeof(stringer1), "%s подарил(а) %s цветы",PI[playerid][pNames],PI[params[0]][pNames]);
    ProxDetector(playerid, 30.0, COLOR_PURPLE, stringer1);
    return 1;
}
cmd:spy(playerid)
{
    if(PI[playerid][pOdet] == 0 && PI[playerid][pMember] == 2) return SendClientMessage(playerid,COLOR_WHITE,"Вы не на службе!");
    if((PI[playerid][pMember] != 2 || PI[playerid][pRank] < 3) && PI[playerid][pMember] != 23) return SendClientMessage(playerid, COLOR_GRAD1, "Вам не доступна данная команда!");
    if(GetPVarInt(playerid,"mask"))
    {
        SetPlayerSkinEx(playerid,PI[playerid][pModel]);
        SetPlayerToTeamColor(playerid);
        DeletePVar(playerid,"mask");
        return 1;
    }
    stringer[0] = 0;
    for(new i = 1; i < 29; i ++) format(stringer, sizeof(stringer), "%s%s\n", stringer, GetPlayerFracName[i]);
    SPD(playerid, 4819, DIALOG_STYLE_LIST, "Выберите фракцию", stringer, "Выбрать", "Закрыть");
    return 1;
}
cmd:block(playerid, params[])
{
    if(PI[playerid][pMember] == 1 && PI[playerid][pRank] >= 5 || PI[playerid][pMember] == 10 && PI[playerid][pRank] >= 5 || PI[playerid][pMember] == 21 && PI[playerid][pRank] >= 5
            || PI[playerid][pMember] == 2 && PI[playerid][pRank] >= 3)
    {
        if(PI[playerid][pOdet] == 0) return SendClientMessage(playerid,COLOR_WHITE,"Вы не на службе!");
        if(GetPlayerVirtualWorld(playerid) != 0 || GetPlayerInterior(playerid) != 0) return SendClientMessage(playerid, -1, "Ошибка: Данную команду нельзя использовать в интерьере.");
        if(IsPlayerInAnyVehicle(playerid)) return SendClientMessage(playerid, -1, "Ошибка: нельзя использовать в машине.");
        if(spike_objects[playerid] == 0)
        {
            new Float:x, Float:y, Float:z, Float:a;
            GetPlayerPos(playerid, x, y, z);
            GetPlayerFacingAngle(playerid, a);
            //spike_objects[playerid] = CreateDynamicObject(2899, x, y, z-0.8, 0, 0, a);
            spike_areas[playerid] = CreateDynamicSphere(x, y, z, 3.0, -1, -1, -1);
            FIXES_ApplyAnimation(playerid, "BOMBER", "BOM_Plant", 4.1, 0, 1, 1, 1, 1);
            SetTimerEx("ClearPlayerChatAnim", 300, false, "i", playerid);
            SendClientMessage(playerid, 0x0bda51FF, "Вы установили шипы с дистанционным управлением.");
            return SendClientMessage(playerid, 0x0bda51FF, "Используйте команду {007fff}/block{0bda51}, чтобы дезактивировать их.");
        }
        else
        {
            DestroyDynamicObject(spike_objects[playerid]);
            DestroyDynamicArea(spike_areas[playerid]);
            spike_objects[playerid] = 0;
            spike_areas[playerid] = 0;
            return SendClientMessage(playerid, COLOR_GREY, "Шипы дезактивированы.");
        }
    }
    else return SendClientMessage(playerid, -1, "Ошибка: Вам недоступна данная команда");
}
cmd:robbank(playerid, params[])
{
    if(PI[playerid][pRobTime] == 0)
    {
        if(kazna < 500000) return SendClientMessage(playerid, COLOR_LIGHTRED, "Казна штата слишком мала, чтобы её грабить.");
        if(IsPlayerInRangeOfPoint(playerid, 2.0,2147.2971,1637.2709,993.5759))
        {
            new membersss = PI[playerid][pMember];
            if(membersss==5 || membersss==6 || membersss==12 || membersss==13 || membersss==14 || membersss==15 || membersss==17 || membersss==18)
            {
                if(IsACop(playerid)) return SendClientMessage(playerid, COLOR_LIGHTRED, "Вы коп! Вам нельзя взламывать банковский сейф.");
                if(robbanks == 1) return SendClientMessage(playerid, COLOR_LIGHTRED, "Банковский сейф, уже взламывают.");
                TogglePlayerControllable(playerid, 0);
                FIXES_ApplyAnimation(playerid,"ROB_BANK","CAT_Safe_Rob",4.1,1,1,1,0,0);
                robbanks = 1; robbankstime[playerid] = 10;
                FIXES_ApplyAnimation(playerid,"ROB_BANK","CAT_Safe_Rob",4.1,1,1,1,0,0);
                format(stringer1, sizeof(stringer1), "Незнакомец пытается взломать банковский сейф.");
                ProxDetector(playerid, 30.0, COLOR_PURPLE, stringer1);
            }
            else SendClientMessage(playerid, COLOR_GREY, "Вам не доступна данная функция.");
        }
        else SendClientMessage(playerid, COLOR_LIGHTRED, "Вы не у банковского сейфа.");
    }
    else SendClientMessage(playerid, COLOR_LIGHTRED, "Грабить банк можно раз в 3 часа.");
    return 1;
}
cmd:weather(playerid, params[])
{
    if(FD(playerid)) {
        if(sscanf(params, "d", params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /weather [id погоды]");
        if(params[0] < 0 || params[0] > 45) return SendClientMessage(playerid, COLOR_GREY, "ID Погоды не может быть меньше 0 и больше 45!");
        SetWeather(params[0]);
        format(stringer1, sizeof(stringer1), "[A] %s сменил погоду", PI[playerid][pNames]);
        SendAdminMessage(COLOR_GREY,stringer1);
    }
    return 1;
}
cmd:agm(playerid)
{
    if(PI[playerid][pAdmin] < 1) return 1;
    if(!PI[playerid][pGM]) PI[playerid][pGM] = 1,SetPlayerHealth(playerid, 10000000);
    else PI[playerid][pGM] = 0, SetPlayerHealth(playerid, 100);
    SendMes(playerid, COLOR_WHITE, "Вы %s {FFFFFF}режим бессмертия.", PI[playerid][pGM] ? ("{32CD32}включили") : ("{FF4500}выключили"));
    return 1;
}
cmd:id(playerid, params[])
{
    if(isnull(params)) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /id [id/часть ника]");
    new ids = strval(params);
    SendClientMessage(playerid,COLOR_YELLOW,"Игроки онлайн:");
    new idd=-1;
    if(!IsNumericc(params))
    {
        foreach(new i:Player)
        {
            if(strfind(PI[i][pNames],params,true) != -1)
            {
                if(idd >= 9) {
                    idd++;
                    continue;
                }
                if(PI[i][AFK_Time] > 2) format(stringer1,sizeof(stringer1),"%s {66CC66}id %d {FF4500}AFK",PI[i][pNames],i);
                else format(stringer1,sizeof(stringer1),"%s {66CC66}id %d",PI[i][pNames],i);
                SendClientMessage(playerid,COLOR_WHITE,stringer1);
                idd++;
            }
        }
        if(idd == -1) return SendClientMessage(playerid,COLOR_GREY,"Не найдено");
        else if(idd >= 9) {
            format(stringer1,sizeof(stringer1),"Показано 10 найденных из %d", idd-2);
            SendClientMessage(playerid, COLOR_GREY, stringer1);
        }
    }
    else
    {
        if(!IsPlayerConnected(ids)) return SendClientMessage(playerid,COLOR_GREY,"Не верный ID");
        if(PI[ids][AFK_Time] > 2) format(stringer1,sizeof(stringer1),"%s {66CC66}id %d {FF4500}AFK",PI[ids][pNames],ids);
        else format(stringer1,sizeof(stringer1),"%s {66CC66}id %d",PI[ids][pNames],ids);
        SendClientMessage(playerid,COLOR_WHITE,stringer1);
    }
    return 1;
}
