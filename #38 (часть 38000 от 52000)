cmd:givephone(playerid, params[])
{
    if(!ISAMAFIA(playerid)) return SendClientMessage(playerid, COLOR_GREY,"Вы не мафиози!");
    if(PI[playerid][pRank] < 3) return SendClientMessage(playerid, COLOR_GREY,"Вам недоступна эта функция!");
    if(sscanf(params, "d",params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /givephone [id]");
    if(!IsPlayerConnected(params[0]) || params[0] == playerid) return SendClientMessage(playerid, COLOR_WHITE, "Ошибка: Игрока не существует или это Вы");
    if(!ProxDetectorS(8.0, playerid, params[0])) return SendClientMessage(playerid, COLOR_GREY, "Человек не рядом с вами!");
    if(takephone[params[0]] != 1) return SendClientMessage(playerid, COLOR_GREY, "У него есть телефон");
    format(stringer1, sizeof(stringer1), "%s вернул вам телефон", PI[playerid][pNames]);
    SendClientMessage(params[0], 0x6ab1ffaa, stringer1);
    takephone[params[0]] = false;
    format(stringer1, sizeof(stringer1), "%s вернул телефон %s 'y", PI[playerid][pNames] ,PI[params[0]][pNames]);
    ProxDetector(playerid, 30.0, COLOR_PURPLE, stringer1);
    return 1;
}
cmd:takephone(playerid, params[])
{
    if(!ISAMAFIA(playerid)) return SendClientMessage(playerid, COLOR_GREY,"Вы не мафиози!");
    if(PI[playerid][pRank] < 3) return SendClientMessage(playerid, COLOR_GREY,"Вам недоступна эта функция!");
    if(sscanf(params, "d",params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /takephone [id]");
    if(!IsPlayerConnected(params[0]) || params[0] == playerid) return SendClientMessage(playerid, COLOR_WHITE, "Ошибка: Игрока не существует или это Вы");
    if(!ProxDetectorS(8.0, playerid, params[0])) return SendClientMessage(playerid, COLOR_GREY, "Человек не рядом с вами!");
    if(takephone[params[0]] > 0) return SendClientMessage(playerid, COLOR_GREY, "У него нет телефона");
    format(stringer1, sizeof(stringer1), "%s забрал у Вас мобильный", PI[playerid][pNames]);
    SendClientMessage(params[0], 0x6ab1ffaa, stringer1);
    takephone[params[0]] = 1;
    format(stringer1, sizeof(stringer1), "%s забрал мобильный у %s", PI[playerid][pNames] ,PI[params[0]][pNames]);
    ProxDetector(playerid, 30.0, COLOR_PURPLE, stringer1);
    return 1;
}
cmd:pass(playerid, params[])
{
    if(sscanf(params, "d",params[0])) return SendClientMessage(playerid, -1, "Введите: /pass [ID]");
    if(!IsPlayerConnected(params[0]) || params[0] == playerid) return SendClientMessage(playerid, COLOR_WHITE, "Ошибка: Игрока не существует или это Вы");
    if(!ProxDetectorS(8.0, playerid, params[0])) return SendClientMessage(playerid, COLOR_RED, "Человек слишком далеко!");
    new shltext[16];
	new str[366],sctring[1450];
    if(PI[playerid][pGunLic]) { shltext = "Есть"; } else { shltext = "Нет"; }
    format(str,sizeof(str), "Имя: %s\n",PI[playerid][pNames]);
    strcat(sctring,str);
    format(str,sizeof(str), "Возраст: %d\nТелефон: %d\n",PI[playerid][pLevel],PI[playerid][pPnumber]);
    strcat(sctring,str);
    if(PI[playerid][pFacePass] == 0)//Настояший паспорт
    {
        format(str,sizeof(str), "Фракция: %s\n",GetPlayerFracName[PI[playerid][pMember]]);
        strcat(sctring,str);
        format(str,sizeof(str), "Работа: %s\n",GetPlayerJobName(PI[playerid][pJob]));
        strcat(sctring,str);
        format(str,sizeof(str), "Разрешение на оружие: %s\n",shltext);
        strcat(sctring,str);
        format(str,sizeof(str), "Преступлений: %d\n",PI[playerid][pKills]);
        strcat(sctring,str);
        format(str,sizeof(str), "Законопослушность: %d\n",PI[playerid][pZakonp]);
        strcat(sctring,str);
        format(str,sizeof(str), "Военный билет: %s\n", PI[playerid][pVoenBill] ? ("{33AA33}Служил") : ("{AA3333}Не служил"));
        strcat(sctring,str);
    }
    else
    {
        if(PI[playerid][pFacePass] == 1) format(str,sizeof(str), "Фракция: Гражданин");
        else if(PI[playerid][pFacePass] == 2) format(str,sizeof(str), "Фракция: ФСБ");
        else if(PI[playerid][pFacePass] == 3) format(str,sizeof(str), "Фракция: S.W.A.T");
        else if(PI[playerid][pFacePass] == 4) format(str,sizeof(str), "Фракция: Администрация Президента");
        strcat(sctring,str);
        format(str,sizeof(str), "Работа: Безработный\n");
        strcat(sctring,str);
        format(str,sizeof(str), "Разрешение на оружие: Есть\n");
        strcat(sctring,str);
        format(str,sizeof(str), "Преступлений: %d\n",PI[playerid][pKills]);
        strcat(sctring,str);
        format(str,sizeof(str), "Законопослушность: %d\n",PI[playerid][pZakonp]);
        strcat(sctring,str);
        format(str,sizeof(str), "Военный билет: {33AA33}Служил\n");
        strcat(sctring,str);
    }
    format(stringer1,sizeof(stringer1), "%s показал(а) свой паспорт %s", PI[playerid][pNames], PI[params[0]][pNames]);
    SetPlayerChatBubble(playerid,stringer1,COLOR_PURPLE,30.0,10000);
    format(stringer1, sizeof(stringer1), "%s показал(a) свой паспорт", PI[playerid][pNames]);
    ProxDetector(playerid, 10.0, COLOR_PURPLE, stringer1);
    format(stringer1,sizeof(stringer1),"{ff0000}"SERVER_NAME" {FFFFFF}| Паспорт");
    SPD(playerid, 1932, 0, stringer1, sctring,"Закрыть","");
    return 1;
}
cmd:crashcuff(playerid, params[])
{
    if(sscanf(params, "d",params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /crashcuff [id]");
    if(!IsPlayerConnected(params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Ошибка: Игрока не существует");
    if(PI[playerid][pOtmbI4ka] == 0) return SendClientMessage(playerid, COLOR_GREY, "У вас нет отмычек");
    if(PlayerCuffed[params[0]] == 0) return SendClientMessage(playerid, COLOR_GREY, "Игрок не в наручниках");
    if(crashcuff[playerid] > gettime()) return SendClientMessage(playerid, COLOR_GREY, "Команду можно использовать раз в 30 секунд");
    if (ProxDetectorS(8.0, playerid, params[0]))
    {
        new randomm;
        switch(PI[params[0]][pDonateRank])
        {
        case 1: { randomm = random(12)+1; }
        case 2: { randomm = random(11)+1; }
        case 3: { randomm = random(10)+1; }
        default: { randomm = random(13)+1; }
        }
        switch(randomm)
        {
        case 1..3:
            {
                TogglePlayerControllable(params[0], 1);
                SetPlayerSpecialAction(params[0],SPECIAL_ACTION_NONE);
                RemovePlayerAttachedObject(params[0],0);
                PlayerCuffed[params[0]] = 0;
                crashcuff[playerid] = gettime() + 30;
                PI[playerid][pOtmbI4ka]--;
                format(stringer1, sizeof(stringer1), "Игрок %s скинул с себя наручники.", PI[params[0]][pNames]);
                ProxDetector(playerid, 30.0, COLOR_PURPLE, stringer1);
            }
        default:
            {
                SendClientMessage(playerid, COLOR_GREY, "Вам не удалось скинуть наручники.");
                PI[playerid][pOtmbI4ka]--;
                crashcuff[playerid] = gettime() + 30;
            }
        }
    }
    else return SendClientMessage(playerid, COLOR_GREY, "Игрок не рядом с вами.");
    return 1;
}
cmd:givebilet(playerid, params[])
{
    if(PI[playerid][pLeader] != 3 && PI[playerid][pLeader] != 19 && PI[playerid][pAdmin] < 12) return SendClientMessage(playerid, COLOR_WHITE, "У вас нет прав на выдачу военного билета.");
    if(sscanf(params, "d",params[0])) return SendClientMessage(playerid, -1, "Введите: /givebilet [ID]");
    if(!IsPlayerConnected(params[0]) || params[0] == playerid) return SendClientMessage(playerid, COLOR_WHITE, "Ошибка: Игрока не существует или это Вы");
    if(PI[params[0]][pVoenBill] != 0) return SendClientMessage(playerid, COLOR_WHITE, "У игрока уже имеется военный билет.");
    if(GetDistanceBetweenPlayers(playerid, params[0]) > 2) return SendClientMessage(playerid, COLOR_WHITE, "Игрок должен находится рядом с вами.");
    PI[params[0]][pVoenBill] = 1;
    if(PI[playerid][pAdmin] > 0) SendMes(params[0], -1, "Администратор %s[%d] выдал вам военный билет.",PI[playerid][pNames], playerid);
    else SendMes(params[0], -1, "%s %s[%d] выдал вам военный билет.",fRankName[PI[playerid][pMember]-1][PI[playerid][pRank]-1], PI[playerid][pNames], playerid);
    SendMes(playerid, -1, "Вы выдали военный билет %s[%d]", PI[params[0]][pNames],params[0]);
    format(mysql_query_, sizeof(mysql_query_), "UPDATE `"TABLE_ACCOUNT"` SET `voenbill` = '1' WHERE `pID` = '%d'",PI[params[0]][pID]);
    mysql_function_query(connects, mysql_query_, false, "", "");
    return 1;
}
cmd:givemedcard(playerid, params[])
{
    if(PI[playerid][pMember] == 4 && PI[playerid][pRank] >= 6 || PI[playerid][pAdmin] > 10)
    {
        if(sscanf(params, "d",params[0])) return SendClientMessage(playerid, -1, "Введите: /givemedcard [ID]");
        if(!IsPlayerConnected(params[0]) || params[0] == playerid) return SendClientMessage(playerid, COLOR_WHITE, "Ошибка: Игрока не существует или это Вы");
        if(PI[params[0]][pMedCard] != 0) return SendClientMessage(playerid, COLOR_WHITE, "У игрока уже имеется медицинская карта.");
        if(GetDistanceBetweenPlayers(playerid, params[0]) > 2) return SendClientMessage(playerid, COLOR_WHITE, "Игрок должен находится рядом с вами.");
        PI[params[0]][pMedCard] = 1;
        if(PI[playerid][pAdmin] > 0) SendMes(params[0], -1, "Администратор %s[%d] выдал вам медицинскую карту.",PI[playerid][pNames], playerid);
        else SendMes(params[0], -1, "%s %s[%d] выдал вам медицинскую карту.",fRankName[PI[playerid][pMember]-1][PI[playerid][pRank]-1], PI[playerid][pNames], playerid);
        SendMes(playerid, -1, "Вы выдали медицинскую карту %s[%d]", PI[params[0]][pNames],params[0]);
        format(mysql_query_, sizeof(mysql_query_), "UPDATE `"TABLE_ACCOUNT"` SET `medcard` = '1' WHERE `pID` = '%d'",PI[params[0]][pID]);
        mysql_function_query(connects, mysql_query_, false, "", "");
    }
    else return SendClientMessage(playerid, COLOR_WHITE, "У вас нет прав на выдачу медицинской карты.");
    return 1;
}
