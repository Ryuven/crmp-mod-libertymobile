cmd:agun(playerid, params[])
{
    if(PI[playerid][pAdmin] < 13) return 1;
    if(sscanf(params, "ddd",params[0],params[1],params[2])) return SendClientMessage(playerid, COLOR_WHITE, "• "SERVER"[Подсказка] {ffffff}/agun [ID] [ID Оружия] [Патроны]");
    if(!IsPlayerConnected(params[0])) return 1;
    if(params[2] < 1 || params[2] > 5000) return SendClientMessage(playerid, COLOR_GRAD1, "Нельзя меньше 1 или больше 5000 патронов!");
    if(params[1] < 1 || params[1] > 46) return SendClientMessage(playerid, COLOR_GREY, "ID оружия от 1 до 46!");
    GiveWeapon(params[0],params[1],params[2]);
    SendClientMessage(playerid, COLOR_GRAD1, "Оружие Выдано!");
    SendMes(params[0], -1, "Администратор %s выдал Вам оружие!", PI[playerid][pNames]);
    return 1;
}
cmd:agetipreg(playerid, params[])
{
    if(FD(playerid)) {
        if(sscanf(params, "s[24]",params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /agetipreg [Ник]");
        new nick = GetPlayerID(params[0]);
        if(nick != INVALID_PLAYER_ID) SendMes(playerid,COLOR_GREY,"Ник: %s | IP: %s",params[0],PI[nick][pIpReg]);
        else SendClientMessage(playerid, COLOR_WHITE, "Игрок оффлайн.");
    }
    return 1;
}
cmd:atm(playerid)
{
    if(!IsABankomat(playerid) || GetPlayerState(playerid) != PLAYER_STATE_ONFOOT) return 1;
    FIXES_ApplyAnimation(playerid,"CRIB","CRIB_Use_Switch",4.0,0,0,0,0,0);
    SPD(playerid, 8900, DIALOG_STYLE_LIST, ""SERVER"Банкомат", "1.{ffffff} Снять наличные\n2.{ffffff} Баланс\n3.{ffffff} Домашний счёт\n4.{ffffff} Пополнить счет телефона\n5. {ffffff}Пополнить счёт бизнеса", "Выбрать", "Закрыть");
    return 1;
}
cmd:enter(playerid, params[])
{
    for(new i = 0; i < sizeof(HouseInfo); i++)
    {
        if(IsPlayerInRangeOfPoint(playerid, 2.0, HouseInfo[i][hEntrancex], HouseInfo[i][hEntrancey], HouseInfo[i][hEntrancez]))
        {
            if(PI[playerid][pPhousekey] == i || HouseInfo[i][hLock] == 0)
            {
                SetPlayerInteriorEx(playerid, HouseInfo[i][hInt]);
                SetPlayerVirtualWorldEx(playerid, i+50);
                SetPPos(playerid, HouseInfo[i][hExitx], HouseInfo[i][hExity], HouseInfo[i][hExitz]);
                return 1;
            }
            else return GameTextForPlayer(playerid, "~r~Closed", 5000, 1);
        }
    }
    return 1;
}
