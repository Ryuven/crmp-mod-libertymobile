cmd:carmat(playerid, params[])
{
    switch(PI[playerid][pMember])
    {
    case 3:
        {
            if(GetVehicleModel(GetPlayerVehicleID(playerid)) != 548) return  SendClientMessage(playerid,COLOR_GREY,"Вы не в загрузочном вертолёте!");
            SetPlayerCheckpoint(playerid,-1420.4194,1485.6796,11.8084,14.0);
            SendClientMessage(playerid,COLOR_WHITE,"Отправляйтесь на корабль, для загрузки Оружия");
            VertMats[playerid]=1;
        }
    }
    return 1;
}
cmd:spawn(playerid, params[])
{
    if(PI[playerid][pAdmin] < 2) return 1;
    if(sscanf(params, "d", params[0])) return SendClientMessage(playerid,COLOR_GREY,"Использование: /spawn [id]");
    if(!IsPlayerConnected(params[0])) return SendClientMessage(playerid,COLOR_GREY,"Игрока нет в сети.");
    new string[144];
    SpawnPlayerEx(params[0]);
    format(string, sizeof(string), "Администратор %s[%d] зареспавнил Вас", PI[playerid][pNames], playerid);
    SendClientMessage(params[0], -1, string);
    format(string, sizeof(string), "[A] %s[%d] зареспавнил игрока %s[%d]", PI[playerid][pNames], playerid, PI[params[0]][pNames], params[0]);
    SendAdminMessage(COLOR_GREY, string);
    return 1;
}
cmd:wantedlist(playerid, params[])
{
    if(PI[playerid][pAdmin] < 1) return 1;
    foreach(Player,i)
    {
        if(PI[i][pWanted] > 0 && IsPlayerConnected(i))
        {
            format(stringer1, sizeof(stringer1), "%s [ID: %d] | Звезд: %d", PI[i][pNames], i, PI[i][pWanted]);
            SendClientMessage(playerid, COLOR_LIGHTGREEN, stringer1);
        }
    }
    return 1;
}
cmd:getban(playerid, params[])
{
    if(PI[playerid][pAdmin] < 5) return 1;
    if(isnull(params)) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /getban [ник]");
    mysql_format(connects, mysql_query_, sizeof(mysql_query_), "SELECT * FROM `"TABLE_BANLOG"` WHERE `name` = '%e'", params);
    mysql_function_query(connects, mysql_query_, true, "AdminCheckBan", "dsd", playerid, params, 0);
    return 1;
}
cmd:getbanip(playerid, params[])
{
    if(PI[playerid][pAdmin] < 8) return 1;
    if(isnull(params)) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /getbanip [IP адрес]");
    mysql_format(connects, mysql_query_, sizeof(mysql_query_), "SELECT * FROM `baniplog` WHERE `ip` = '%e'", params);
    mysql_function_query(connects, mysql_query_, true, "AdminCheckBan", "dsd", playerid, params, 1);
    return 1;
}

cmd:lock(playerid, params[])
{
    if(GetPlayerOwnableCar(playerid) == INVALID_VEHICLE_ID)
    {
        if(GetPlayerOwnableCars(playerid) == 0)
        return SendClientMessage(playerid, 0x999999FF, "У Вас нет личного транспорта");
        else
        return SendClientMessage(playerid, 0x999999FF, "Ваш личный транспорт не загружен на сервер");
    }
    new Float: x, Float: y, Float: z;
    GetVehiclePos(GetPlayerOwnableCar(playerid), x, y, z);

    if(IsPlayerInRangeOfPoint(playerid, 10.0, x, y, z))
    {
        ToggleLock(playerid, GetPlayerOwnableCar(playerid));
    }
    else SendClientMessage(playerid, 0x999999FF, "Вы должны стоять рядом с транспортом");
    return 1;
}
alias:lock("lk");

cmd:getvw(playerid)
{
    new string[ 32 ];
    format(string, sizeof(string), "Your virtual world: %i AND Inter %d", GetPlayerVirtualWorld(playerid), GetPlayerInterior(playerid) );
    SendClientMessage(playerid, 0xFFFFFFF, string);
}

cmd:park(playerid, params[])
{
    if(sscanf(params, "d", params[0]) || params[0] <= 0 || params[0] > 3)
    {
        SendClientMessage(playerid, COLOR_WHITE, "Введите: /park [1 / 2]");
        SendClientMessage(playerid, COLOR_WHITE, "Доступные названия:");
        SendClientMessage(playerid, COLOR_WHITE, "  1 - Домашнее авто.");
        return 1;
    }
    if(!IsPlayerInAnyVehicle(playerid))
    return SendClientMessage(playerid, -1, ""error" Вы не в транспорте!");

    if(IsAGreenZone(playerid))
    return SendClientMessage(playerid, -1, ""error" Запрещенно парковать машину в зеленой зоне!");

    switch(params[0])
    {
    case 1:
        {
            new vehicleid = GetPlayerOwnableCar(playerid);
            if(vehicleid == INVALID_VEHICLE_ID)
            {
                if(GetPlayerOwnableCars(playerid) == 0)
                return SendClientMessage(playerid, 0x999999FF, "У Вас нет личного транспорта");
                else
                return SendClientMessage(playerid, 0x999999FF, "Ваш личный транспорт не загружен на сервер");
            }

            if(vehicleid != GetPlayerVehicleID(playerid))
            return SendClientMessage(playerid, 0xCECECEFF, "Вы должны находиться в личном транспорте");

            new index = GetVehicleData(vehicleid, V_ACTION_ID);

            GetVehiclePos(vehicleid, GetOwnableCarData(index, OC_POS_X), GetOwnableCarData(index, OC_POS_Y), GetOwnableCarData(index, OC_POS_Z));
            GetVehicleZAngle(vehicleid, GetOwnableCarData(index, OC_ANGLE));

            SetOwnableCarData(index, OC_World, GetPlayerVirtualWorld(playerid));

            SetOwnableCarData(index, OC_Int, GetPlayerInterior(playerid));

            /*
            n_veh_DestroyVehicle(vehicleid);

            vehicleid = CreateVehicle
            (
                GetOwnableCarData(index, OC_MODEL_ID),
                GetOwnableCarData(index, OC_POS_X),
                GetOwnableCarData(index, OC_POS_Y),
                GetOwnableCarData(index, OC_POS_Z),
                GetOwnableCarData(index, OC_ANGLE),
                GetOwnableCarData(index, OC_COLOR_1),
                GetOwnableCarData(index, OC_COLOR_2),
                -1,
                0,
                VEHICLE_ACTION_TYPE_OWNABLE_CAR,
                index
            );

            PlayerOwnableCarInit(playerid);

            AddTune(index, vehicleid);
            */

            SaveOwnableCar(vehicleid);
            return SendClientMessage(playerid, 0x66CC00FF, "Транспорт припаркован");
        }
    }

    return 1;
}
cmd:alogin(playerid)
{

    if(PI[playerid][pAdmin] && !dostup[playerid]) {
        if(PI[playerid][pDostup] == 0) return SPD(playerid, 2991, DIALOG_STYLE_INPUT, "{FF4500}Админ регистрация", "{FFFFFF}Придумайте пароль из 4-х цифр для авторизации в админ панеле\n{FFFFFF}Введите его в поле ниже:\n", "Далее", "Выход");
        else return SPD(playerid,2934,DIALOG_STYLE_PASSWORD,""SERVER"Админ авторизация","{FFFFFF}Вы вошли как администратор сервера\nПожалуйста, введите Ваш пароль от админки в поле ниже","Вход","Выход");
    }
    return 1;
}

cmd:asellhouse(playerid, params[])
{
    if(PI[playerid][pAdmin] != 13) return 1;
    if(sscanf(params, "d", params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /asellhouse [houseid]");
    if(params[0] < 0 || params[0] > 1999) return 1;
    format(mysql_query_, sizeof(mysql_query_), "UPDATE `"TABLE_ACCOUNT"` SET `pPhousekey` = '255' WHERE `Name` = '%s'",HouseInfo[params[0]][hOwner]);
    mysql_function_query(connects, mysql_query_, false, "", "");
    HouseInfo[params[0]][hHel] = 0;
    HouseInfo[params[0]][hLock] = 1;
    HouseInfo[params[0]][hOwned] = 0;
    SendClientMessage(playerid, COLOR_GREY, "Дом продан");
    format(stringer1, sizeof(stringer1), "продал дом (ID: %d)", params[0]);
    Admin_Log(playerid, stringer1, HouseInfo[params[0]][hOwner], gettime());
    strmid(HouseInfo[params[0]][hOwner], "The State", 0, strlen("The State"), 255);
    BuyHouse(params[0]);
    return 1;
}
cmd:dj(playerid, params[])
{
    if(PI[playerid][pJob] != 15) return SendClientMessage(playerid,COLOR_GREY,"Вы не работаете Диджеем.");
    if(IsPlayerInRangeOfPoint(playerid, 2.0,487.35198974609,-2.1640000343323,1002.3829956055))
    {
        if(AlhambraDJID == INVALID_PLAYER_ID) AlhambraDJ = true, AlhambraDJID = playerid;
        else if(AlhambraDJID != playerid) return SendClientMessage(playerid,COLOR_GREY,"DJ уже работает и он находиться в клубе.");
    }
    else if(IsPlayerInRangeOfPoint(playerid, 1.0,-2648.001953125,1422.7509765625,906.46099853516))
    {
        if(JizzyDJID == INVALID_PLAYER_ID) JizzyDJ = true, JizzyDJID = playerid;
        else if(JizzyDJID != playerid) return SendClientMessage(playerid,COLOR_GREY,"DJ уже работает и он находиться в клубе.");
    }
    else return SendClientMessage(playerid,COLOR_GREY,"Вы не у стойки для DJ.");
    SPD(playerid, 5350, DIALOG_STYLE_LIST, "Панель управления DJ.", "Включить свой трек\nВыключить музыку\nОставить пульт", "Выбрать", "Отмена");
    return 1;
}


cmd:teleport(playerid)
{
    if(PI[playerid][pYtADM] > 0)
    {
        return show_teleport_dialog(playerid);
    }
    if(PI[playerid][pAdmin] > 0)
    {
        return show_teleport_dialog(playerid);
    }
    return 1;
}
alias:teleport("tp");

stock show_teleport_dialog(playerid)
{
    SPD(playerid, 9000, DIALOG_STYLE_LIST, !""SERVER"Меню телепортации", !"{FFFFFF}1. Основные\n{FFFFFF}2. Автосалоны\n{FFFFFF}3. Работы\n{FFFFFF}4. Базы организаций\n{FFFFFF}5. Банки", !"Выбрать", !"Закрыть");
    return 1;
}

cmd:gps(playerid)
{
    if(GetPVarInt(playerid, "work_eda") != -1) return SendClientMessage(playerid, COLOR_WHITE, !""SERVER"~ {FFFFFF}Вы не можете использовать GPS-Навигатор во время работы развозчиком!");
    if(PI[playerid][pRaceMoney]) return SendClientMessage(playerid, COLOR_WHITE, !""SERVER"~ {FFFFFF}Вы не можете использовать GPS-Навигатор во время гонки");
    if(GetPVarInt(playerid, "UgonInvite") == 1 || PI[playerid][pUgonTimer] > 0) return SendClientMessage(playerid, COLOR_WHITE, !""SERVER"~ {FFFFFF}Вы начали угон. Команда /gps Вам недоступна.");
    SPD(playerid, 9624, DIALOG_STYLE_LIST, ""SERVER"GPS - Навигатор", ""SERVER"Найти ближайший 24/7\n"SERVER"Найти ближайшую АЗС\n"SERVER"Найти ближайший банкомат\n{FFFFFF}1. {FFFFFF}Основные\n{FFFFFF}2. {FFFFFF}Автосалоны\n{FFFFFF}3. {FFFFFF}Бизнесы\n{FFFFFF}4. {FFFFFF}Работы\n{FFFFFF}5. {FFFFFF}Базы организаций\n{FFFFFF}6. {FFFFFF}Банки\n{FFFFFF}7. Контейнеры", "Выбрать", "Закрыть");
    DisablePlayerCheckpoint(playerid);
    PicCP[playerid] = 0;
    return 1;
}

CMD:hm(playerid, params[])
{
    if(PI[playerid][pPhousekey] == 255) return SendClientMessage(playerid,COLOR_GREY,"У вас нет дома (Но возможно у вас есть квартира, введите /amenu)");
    if(GetPlayerVirtualWorld(playerid) != PI[playerid][pPhousekey]+50 || !IsPlayerInRangeOfPoint(playerid, 90.0,HouseInfo[PI[playerid][pPhousekey]][hExitx], HouseInfo[PI[playerid][pPhousekey]][hExity], HouseInfo[PI[playerid][pPhousekey]][hExitz])) return SendClientMessage(playerid,COLOR_GREY,"Вы должны быть внутри своего дома");
    new string[600], tstext[10], idhm = PI[playerid][pPhousekey];
    switch(HouseInfo[idhm][hKlass])
    {
    case 1: tstext = "D";
    case 2: tstext = "C";
    case 3: tstext = "B";
    case 4: tstext = "A";
    case 5: tstext = "S";
    default: tstext = "Nope";
    }
    format(string, sizeof(string), "{FFFFFF}Тип / название:\t\t\t\t{dba002}%s\n{FFFFFF}Номер дома:\t\t\t\t%d\n{FFFFFF}Стоимость:\t\t\t\t%d\n{FFFFFF}На счету дома:\t\t\t\t$%d\n\n{FFFFFF}Квартплата:\t\t\t\t%d$\n{FFFFFF}Аптечки: \t\t\t\t%d\n{FFFFFF}Статус: \t\t\t\t\t%s\n\n{35d408}Для открытия панели управления вашим домом\nнажмите 'Изменить'",
    tstext, idhm, HouseInfo[idhm][hValue], HouseInfo[idhm][hTakings], (HouseInfo[idhm][hKlass] < 1) ? 300 : HouseInfo[idhm][hKlass] * 300,
    HouseInfo[idhm][hHel], (HouseInfo[idhm][hLock] > 0) ? "{FF4500}Закрыт" : "{0aed1c}Открыт");
    SPD(playerid, 7425, DIALOG_STYLE_MSGBOX, "{dba002}Информация о доме", string, "Изменить", "Закрыть");
    return 1;
}

cmd:healme(playerid, params[])
{
    if(HealmeObject[playerid] > gettime()) return SendClientMessage(playerid, COLOR_GREY, "Не флуди");
    if(TeamPain[playerid] != 0 && StartPaintBall == 2) return SendClientMessage(playerid,COLOR_GREY,"Использовать аптечки на PaintBall запрещено.");
    if(!PI[playerid][pHeal]) return SendClientMessage(playerid,COLOR_GREY,"У вас нет аптечки");
    if(PI[playerid][pDrugtime] > gettime()) return  SendClientMessage(playerid,COLOR_GRAD1,""error"Нельзя слишком часто использывать аптечки!");
    new Float:health;
    GetPlayerHealth(playerid,health);
    if(health + 60.0 <= 100) SetPlayerHealth(playerid,health+60.0);
    else SetPlayerHealth(playerid,100.0);
    format(stringer1, sizeof(stringer1), "%s перебинтовал(а) рану",PI[playerid][pNames]);
    ProxDetector(playerid, 30.0, COLOR_YELLOW, stringer1);
    SetPlayerChatBubble(playerid,"+60 HP",COLOR_YELLOW,20.0,10000);
    if(PlayerCuffed[playerid] == 0 && !IsPlayerInAnyVehicle(playerid)) {
        ApplyAnimation(playerid,"PED","gum_eat",4.0,0,0,0,0,0,1);
        HealmeObject[playerid] = gettime() + 5;
        AntiSbiv[playerid] = gettime()+10;
    }
    PI[playerid][pHeal] --;
    PI[playerid][pDrugtime] = gettime() + 10;
    return 1;
}
cmd:healhouse(playerid, params[])
{
    for(new i = 0; i < sizeof(HouseInfo); i++)
    {
        if(IsPlayerInRangeOfPoint(playerid, 5.0,HouseInfo[i][hExitx], HouseInfo[i][hExity], HouseInfo[i][hExitz]))
        {
            if(GetPlayerVirtualWorld(playerid) == i+50)
            {
                if(HouseInfo[i][hHel] == 0) return SendClientMessage(playerid, COLOR_GREY, "В этом месте нет аптечек");
                SendClientMessage(playerid, COLOR_WHITE, "Вы были вылечены до 100 процентов");
                format(stringer1, sizeof(stringer1), "Осталось %d аптечек", HouseInfo[i][hHel]);
                SendClientMessage(playerid, 0x6495EDFF, stringer1);
                HouseInfo[i][hHel] -= 1;
                SetPlayerHealth(playerid, 100);
                SetPlayerChatBubble(playerid, "использовал аптечку", COLOR_PURPLE, 30.0, 10000);
                return 1;
            }
        }
    }
    return 1;
}
cmd:pgetip(playerid,params[])
{
    if(FD(playerid)) {
        if(sscanf(params,"s[32]",params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /pgetip [ip]");
      //  if(!IsIpFound(params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введенное вами значение не соотвествует структуре IP-адреса.");
        strmid(check_getip, params[0], 0, strlen(params[0]), 32);
        format(mysql_query_, sizeof(mysql_query_), "SELECT `Name`, `pGetonDate`, `pOnline` FROM `"TABLE_ACCOUNT"` WHERE `pGetonIP` = '%s' ORDER BY `Name` DESC LIMIT 0,15", params[0]);
        mysql_function_query(connects, mysql_query_, true, "Logs","ddd", playerid, 3, 1);
    }
    return 1;
}
cmd:apass(playerid,params[])
{
    if(PI[playerid][pAdmin] < 8) return 1;
    if(sscanf(params,"dd",params[1])) return SendClientMessage(playerid, 0xFFFFFFF, "• "SERVER"[Подсказка] {ffffff}/apass [Новый Пароль] | Если вы забыли пароль >> vk.com/yaganster, vk.com/arab.gangster");
    {
        PI[playerid][pDostup] = params[1];
        SendMes(playerid, 0x00CCFFFF, "Вы сменили свой Админ.Пароль на: "SERVER"%d{00CCFF} Обязательно запишите его !", params[1]);
    }
    format(mysql_query_, sizeof(mysql_query_), "UPDATE `"TABLE_ACCOUNT"` SET `pDostup` = '%d' WHERE `pID` = '%d'", params[1], PI[playerid][pID]);
    mysql_function_query(connects, mysql_query_, false, "", "");
    format(stringer1, sizeof(stringer1), "[A] Администратор %s изменил свой административный пароль | {FF4500}ВНИМАНИЕ !", PI[playerid][pNames]);
    SendAdminMessage(0xAFAFAFFF,stringer1);
    format(stringer1, sizeof(stringer1), "... беспл.смена админ-пароля доступна на 12-ом уровне административных прав ! =)", PI[playerid][pNames]);
    SendAdminMessage(0xAFAFAFFF,stringer1);
    return 1;
}
