cmd:window(playerid)
{
    if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return SendClientMessage(playerid, -1, "Вы должны быть на водительском месте");
    new vod = GetPlayerVehicleID(playerid);
    if(IsAPlane(vod) && IsABoat(vod) && IsANoSpeed(vod)) return SendClientMessage(playerid, -1, "На этом транспортном средстве нельзя открывать окна");
    new bool:driver, bool:passenger, bool:backleft, bool:backright;
    GetVehicleParamsCarWindows(vod, driver, passenger, backleft, backright);
    format(stringer, sizeof(stringer), "\
    {F3BB33}1.{FFFFFF} Водительское окно: %s\n\
    {F3BB33}2.{FFFFFF} Пассажирское переднее окно: %s\n\
    {F3BB33}3.{FFFFFF} Пассажирское заднее левое окно: %s\n\
    {F3BB33}4.{FFFFFF} Пассажирское заднее правое окно: %s\n",
    !driver ? ("{63BD4E}Открыто") : ("{F04245}Закрыто"),
    !passenger ? ("{63BD4E}Открыто") : ("{F04245}Закрыто"),
    !backleft ? ("{63BD4E}Открыто") : ("{F04245}Закрыто"),
    !backright ? ("{63BD4E}Открыто") : ("{F04245}Закрыто"));
    SPD(playerid, 4322, DIALOG_STYLE_LIST, "{63BD4E}Управление окнами транспорта", stringer, "Выбрать", "Отмена");
    return 1;
}
cmd:cuff(playerid, params[])
{
    if(!IsACop(playerid)) return SendClientMessage(playerid, COLOR_GREY, YouCanNot);
    if(PI[playerid][pOdet] == 0) return SendClientMessage(playerid,COLOR_WHITE,"Вы не на службе!");
    if(sscanf(params, "d",params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /cuff [id]");
    if(!IsPlayerConnected(params[0]) || params[0] == playerid) return SendClientMessage(playerid, COLOR_WHITE, "Ошибка: Игрока не существует или это Вы");
    if(PlayerTied[playerid] > 0)  return SendClientMessage(playerid, COLOR_GREY, "Вы связаны!");
    if(IsACop(params[0])) return    SendClientMessage(playerid, COLOR_GREY, "Вы не можете надеть наручники на законника");
    if(PlayerCuffed[params[0]] == 2) return    SendClientMessage(playerid, COLOR_GREY, "На этого человека уже одеты наручники");
    if(PI[params[0]][pWanted] == 0) return    SendClientMessage(playerid, COLOR_GREY, "Вы не можете надеть наручники на этого человека (он не в розыске)");
    if (!ProxDetectorS(2.0, playerid, params[0])) return SendClientMessage(playerid, COLOR_GREY, "Рядом с вами никого нет !");
    if(PlayerCuffed[params[0]] != 1) return SendClientMessage(playerid, COLOR_GREY, "Игрок не оглушен, используйте тазер.");
    format(stringer1, sizeof(stringer1), "На Вас надел наручники %s.", PI[playerid][pNames]);
    SendClientMessage(params[0], 0x6ab1ffaa, stringer1);
    format(stringer1, sizeof(stringer1), "Вы надели наручники на %s.", PI[params[0]][pNames]);
    SendClientMessage(playerid, 0x6ab1ffaa, stringer1);
    format(stringer1, sizeof(stringer1), "%s надел(а) на %s наручники.", PI[playerid][pNames] ,PI[params[0]][pNames]);
  //  SetPlayerAttachedObject(params[0], 0, 19418, 6, -0.011000, 0.028000, -0.022000, -15.600012, -33.699977, -81.700035, 0.891999, 1.000000, 1.168000);
    GameTextForPlayer(params[0], "~r~Cuffed", 2500, 3);
    ProxDetector(playerid, 30.0, COLOR_PURPLE, stringer1);
    SetPlayerSpecialAction(params[0],SPECIAL_ACTION_CUFFED);
    TogglePlayerControllable(params[0], false);
    PlayerCuffed[params[0]] = 2;
    PlayerCuffedTime[params[0]] = 3600;

    SetTimerEx("UnFreez", 3600000, 0, "d", params[0]);
    return 1;
}
cmd:uncuff(playerid, params[])
{
    if(!IsACop(playerid)) return SendClientMessage(playerid, COLOR_GREY, YouCanNot);
    if(PI[playerid][pOdet] == 0) return SendClientMessage(playerid,COLOR_WHITE,"Вы не на службе!");
    if(sscanf(params, "d",params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /uncuff [id]");
    if(!IsPlayerConnected(params[0]) || params[0] == playerid) return SendClientMessage(playerid, COLOR_WHITE, "Ошибка: Игрока не существует или это Вы");
    if(PlayerTied[playerid] > 0)  return SendClientMessage(playerid, COLOR_GREY, "Вы связаны!");
    if (!ProxDetectorS(5.0, playerid, params[0])) return SendClientMessage(playerid, COLOR_GREY, "Рядом с вами никого нет !");
    if(PlayerCuffed[params[0]] == 2)
    {
        format(stringer1, sizeof(stringer1), "С Вас снял наручники %s.", PI[playerid][pNames]);