    if(PI[playerid][pAdmin] >= 12) SendClientMessage(playerid, 0xB8860BFF, "{FF4500}ВНИМАНИЕ!{FFFFFF} Купив, и пользуя административные права вы...");
    if(PI[playerid][pAdmin] >= 12) SendClientMessage(playerid, 0xB8860BFF, "{FF4500}соглашаетесь{FFFFFF} соблюдать правила, и подтверждаете то, что прочитали..");
    if(PI[playerid][pAdmin] >= 12) SendClientMessage(playerid, 0xB8860BFF, "{FF4500}лицензионное{FFFFFF} соглашение, и правила проекта и наказывать нарушителей только по нормам..");
    if(PI[playerid][pAdmin] >= 12) SendClientMessage(playerid, 0xB8860BFF, "{FF4500}КОТОРЫЕ{FFFFFF} выдвинуты проектом. В противном случае вы будете получать выговоры, и будете сняты.");
    if(PI[playerid][pAdmin] >= 12) SendClientMessage(playerid, 0xB8860BFF, "{FF4500}Команды просмотра{FFFFFF}: /licsog /aprav /anak | /amusic - К музыке доступ выдется по просьбе.");
    return 1;
}

cmd:jetpack(playerid,  params[])
