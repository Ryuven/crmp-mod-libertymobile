            format(stringer1, sizeof stringer1, "Передать %s", Drugs[itemID+1][d_name]);
            if(!strlen(inputtext)) {
                SPD(playerid, 592, DIALOG_STYLE_INPUT, stringer1, "\nВведите ID игрока\n", "Выбрать", "Выйти");
                return 1;
            }
            user = strval(inputtext);
            if(!IsPlayerConnected(user) || user == INVALID_PLAYER_ID || user == playerid) return SendClientMessage(playerid, COLOR_WHITE, "Ошибка: Игрока не существует или это Вы");
            if(!ProxDetectorS(3.0, playerid, user)) return SendClientMessage(playerid, COLOR_GREY, "Этого игрока рядом с вами нет !");
            if(PI[user][p_drugs][itemID] >= 200) return SendClientMessage(playerid, COLOR_WHITE, "* У игрока нет места, максимальное количество 200 грамм!");
            TransferDrugsPlayer(playerid, user, itemID, GetPVarInt(playerid, "TransferDrugsQuantity"));
        }
