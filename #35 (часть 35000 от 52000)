        SendClientMessage(playerid, -1, reward);
    }

    ShowPlayerDialog(playerid, D_BLACKJACK, DIALOG_STYLE_MSGBOX, "Подтвердите выход", reward, "Да", "Нет");
    SendClientMessage(playerid, -1, reward);

}
*/

cmd:pc(playerid, params[])
{
    if(!PI[playerid][pAdmin]) return 1;
    extract params -> new check_id; else return SendClientMessage(playerid, 0xCECECEFF, "[WIPE PROJECT Mobile] Используйте: /pc [id]");
    new fmt_str[256],name[32];
    GetPlayerName(check_id, name, 32);
    format
    (
    fmt_str, 256,
    "{ffffff}Игрок: {ffff00}%s{ffffff}\n\
        Пинг: {ffff00}%d{ffffff}\n\
        Устройство: {ffff00}%s",
    name, GetPlayerPing(check_id),
    player_device[PI[check_id][P_PLAYER_DEVICE]]
    );
    ShowPlayerDialog
    (
    playerid, 0, DIALOG_STYLE_MSGBOX,
    "{ffffff}WIPE PROJECT MOBILE",
    fmt_str,
    "Далее", "Отмена"
    );

    return 1;
}
cmd:nextskin(playerid)
{
    if(GetPVarInt(playerid,"PlayerInClothes"))
    {
        SetPVarInt(playerid,"PlayerSkinSlot", GetPVarInt(playerid,"PlayerSkinSlot")+1);
        if(GetPVarInt(playerid,"PlayerSkinSlot") >= 32) SetPVarInt(playerid,"PlayerSkinSlot", 0);

        new slot = GetPVarInt(playerid,"PlayerSkinSlot");
        {
            SetPlayerSkinEx(playerid,SkinsMale[slot][skinID]);
            SendClientMessage(playerid, COLOR_WHITE, !"Используйте команду /nextskin - чтобы посмотреть следующий скин!");
            SendClientMessage(playerid, COLOR_WHITE, !"Используйте команду /backskin - чтобы посмотреть предыдущий скин!");
            SendClientMessage(playerid, COLOR_WHITE, !"Используйте команду /buyskin - чтобы купить скин!");
            SendClientMessage(playerid, COLOR_WHITE, !"Используйте команду /exitskin - чтобы покинуть магазин одежды!");

            new scm_string[64];
            format(scm_string, sizeof(scm_string), "%d rub", SkinsMale[slot][skinPrice]);
            GameTextForPlayer(playerid,scm_string,5000,3);
        }
    }
    return 1;
}
cmd:backskin(playerid)
{
    if(!GetPVarInt(playerid,"PlayerInClothes")) return 1;

    SetPVarInt(playerid,"PlayerSkinSlot", GetPVarInt(playerid,"PlayerSkinSlot")-1);
    if(GetPVarInt(playerid,"PlayerSkinSlot") <= 0) SetPVarInt(playerid,"PlayerSkinSlot", 0);

    new slot = GetPVarInt(playerid,"PlayerSkinSlot");
    {
        SetPlayerSkinEx(playerid,SkinsMale[slot][skinID]);
        SendClientMessage(playerid, COLOR_WHITE, !"Используйте команду /nextskin - чтобы посмотреть следующий скин!");
        SendClientMessage(playerid, COLOR_WHITE, !"Используйте команду /backskin - чтобы посмотреть предыдущий скин!");
        SendClientMessage(playerid, COLOR_WHITE, !"Используйте команду /buyskin - чтобы купить скин!");
        SendClientMessage(playerid, COLOR_WHITE, !"Используйте команду /exitskin - чтобы покинуть магазин одежды!");

        new scm_string[64];
        format(scm_string, sizeof(scm_string), "%d rub", SkinsMale[slot][skinPrice]);
        GameTextForPlayer(playerid,scm_string,5000,3);
    }
    return 1;
}
cmd:buyskin(playerid)
{
    if(!GetPVarInt(playerid,"PlayerInClothes")) return 1;
    new check;

    check = SkinsMale[GetPVarInt(playerid,"PlayerSkinSlot")][skinPrice];

    if(PI[playerid][pCash] < check) {
        new bizid = GetPVarInt(playerid, "Bizzid");

        DeletePVar(playerid,"PlayerSkinSlot");
        DeletePVar(playerid,"PlayerInClothes");
        SetCameraBehindPlayer(playerid);
        SetPlayerVirtualWorldEx(playerid, 0);
        SetPPos(playerid,BizzInfo[bizid][bEntrx],BizzInfo[bizid][bEntry],BizzInfo[bizid][bEntrz]);
        SetPlayerInteriorEx(playerid, 0);

        UpdatePlayerData(playerid, "pChar", PI[playerid][pChar]);
        DeletePVar(playerid,"OldSkin");
        DeletePVar(playerid,"Bizzid");

        TogglePlayerControllable(playerid, true);
        SendClientMessage(playerid, -1,"У вас недостаточно денег для покупки этого скина");
        return 1;
    }
    new slot = GetPVarInt(playerid,"PlayerSkinSlot");
    new bizid = GetPVarInt(playerid, "Bizzid");

    PI[playerid][pChar] = SkinsMale[slot][skinID];
    SetPlayerSkinEx(playerid,SkinsMale[slot][skinID], true);
    PI[playerid][pCash] -= SkinsMale[slot][skinPrice];
    BizzInfo[bizid][bMoney] += SkinsMale[slot][skinPrice];
    BizzPay[bizid] += SkinsMale[slot][skinPrice];

    DeletePVar(playerid,"PlayerSkinSlot");
    DeletePVar(playerid,"PlayerInClothes");
    SetCameraBehindPlayer(playerid);
    SetPlayerVirtualWorldEx(playerid, 0);
    SetPPos(playerid,BizzInfo[bizid][bEntrx],BizzInfo[bizid][bEntry],BizzInfo[bizid][bEntrz]);
    SetPlayerInteriorEx(playerid, 0);

    BizzInfo[bizid][bProduct] -= 50;
    UpdatePlayerData(playerid, "pChar", PI[playerid][pChar]);

    SaveBizz(bizid);
    DeletePVar(playerid,"OldSkin");
    DeletePVar(playerid,"Bizzid");

    TogglePlayerControllable(playerid, true);
    //}
    return 1;
}

cmd:exitskin(playerid)
{
    DeletePVar(playerid,"PlayerInClothes");
    SetPlayerSkinEx(playerid, GetPVarInt(playerid,"OldSkin"));
    DeletePVar(playerid,"PlayerSkinSlot");
    new bizid = GetPVarInt(playerid, "Bizzid");
    SetPlayerVirtualWorldEx(playerid, 0);
    SetPPos(playerid,BizzInfo[bizid][bEntrx],BizzInfo[bizid][bEntry],BizzInfo[bizid][bEntrz]);
    SetPlayerInteriorEx(playerid, 0);
    DeletePVar(playerid,"Bizzid");
    DeletePVar(playerid,"OldSkin");
    SetCameraBehindPlayer(playerid);
    TogglePlayerControllable(playerid, true);
    return 1;
}
cmd:exitgarage(playerid)
{
    if(IsPlayerInRangeOfPoint(playerid,  10.0, 995.9982,999.8981,1501.0314) && GetPlayerVirtualWorld(playerid) == PI[playerid][pPhousekey]+50 && GetPlayerState(playerid) == 2)
    {
        SetVehiclePos(GetPlayerOwnableCar(playerid),HouseInfo[PI[playerid][pPhousekey]][hCarx], HouseInfo[PI[playerid][pPhousekey]][hCary], HouseInfo[PI[playerid][pPhousekey]][hCarz]);
        SetVehicleZAngle(GetPlayerOwnableCar(playerid), HouseInfo[PI[playerid][pPhousekey]][hCarc]);
        SetCameraBehindPlayer(playerid);
        LinkVehicleToInterior(GetPlayerOwnableCar(playerid),0);
        SetPlayerInteriorEx(playerid,0);
        SetPlayerVirtualWorldEx(playerid,0);
        SetVehicleVirtualWorld(GetPlayerOwnableCar(playerid), 0);
    }
    return 1;
}
cmd:gofbi(playerid)
{
    if(IsPlayerInRangeOfPoint(playerid, 5.0, -2528.4910, 507.5064, -11.1088) && PI[playerid][pMember] == 2 && GetPlayerState(playerid) == 2)
    {
        SetVehiclePos(GetPlayerVehicleID(playerid), -2443.2319, 522.4879, 29.6709);
        SetVehicleZAngle(GetPlayerVehicleID(playerid), 179.7420);
        SetCameraBehindPlayer(playerid);
        return 1;
    }
    else if(IsPlayerInRangeOfPoint(playerid, 5.0, -2438.5713, 521.9441, 29.6710) && PI[playerid][pMember] == 2 && GetPlayerState(playerid) == 2)
    {
        SetVehiclePos(GetPlayerVehicleID(playerid), -2529.2104, 501.5463, -11.1091);
        SetVehicleZAngle(GetPlayerVehicleID(playerid), 270.1569);
        SetCameraBehindPlayer(playerid);
        return 1;
    }
    return 1;
}
cmd:exit(playerid)
{
    if ( GetPVarInt ( playerid, "apartment_id" ) > 0 && GetPlayerVirtualWorld ( playerid ) == GetPVarInt ( playerid, "apartment_id" ) )
    {
        new z = GetPVarInt ( playerid, "p_podezd_id" ) - 1 ;
        new t = GetPVarInt ( playerid, "apartment_id" ) - 1 ;
        DeletePVar ( playerid, "apartment_id" ) ;

        SetPPos(playerid, ap_info [ t ] [ ap_pos ] [ 0 ], ap_info [ t ] [ ap_pos ] [ 1 ], ap_info [ t ] [ ap_pos ] [ 2 ]);
        SetPlayerInterior(playerid, 0);

        if(ap_info [ t ] [ ap_floor ] != 1) SetPlayerVirtualWorld(playerid, pod_info [ z ] [ pod_world ] + (ap_info [ t ] [ ap_floor ] - 1));
        else SetPlayerVirtualWorld(playerid, pod_info [ z ] [ pod_world ]);

        SetCameraBehindPlayer(playerid);

        if ( ap_info [ t ] [ ap_radio_status ] == 1 )StopAudioStreamForPlayer ( playerid ) ;
        return 1;
    }
    if(GetPlayerVirtualWorld(playerid) > 40)
    {
        for(new h = 0; h < sizeof(HouseInfo); h++)
        {
            if(IsPlayerInRangeOfPoint(playerid,  3.0, -1026.7469,-2611.7305,18.6245) && GetPlayerVirtualWorld(playerid) == h+50)
            {
                SetPlayerInteriorEx(playerid,0);
                SetPlayerVirtualWorldEx(playerid,0);
                SetPPos(playerid,HouseInfo[h][hEntrancex], HouseInfo[h][hEntrancey], HouseInfo[h][hEntrancez]);
                SetPVarInt(playerid, "ExitHouse",1);
                return 1;
            }
            else if(IsPlayerInRangeOfPoint(playerid, 2.0, HouseInfo[h][hExitx], HouseInfo[h][hExity], HouseInfo[h][hExitz]) && GetPlayerVirtualWorld(playerid) == h+50)
            {
                switch(HouseInfo[h][hKlass])
                {
                case 3: format(stringer1, sizeof(stringer1), ""SERVER"[1]{FFFFFF} Улица\n"SERVER"[2]{FFFFFF} Гараж\n"SERVER"[3]{FFFFFF} Подвал");
                case 4..5: format(stringer1, sizeof(stringer1), ""SERVER"[1]{FFFFFF} Улица\n"SERVER"[2]{FFFFFF} Гараж\n"SERVER"[3]{FFFFFF} Подвал");
                default: format(stringer1, sizeof(stringer1), ""SERVER"[1]{FFFFFF} Улица\n"SERVER"[2]{FFFFFF} Гараж");
                }
                return SPD(playerid,70,DIALOG_STYLE_LIST, "{FFD300}Выберите место, куда вы хотите выйти:",stringer1, "Далее", "Отмена");
            }
            else if(PI[playerid][pLocal] != -1 && (IsPlayerInRangeOfPoint(playerid, 3.0, -1050.9579,453.4256,2.0479) || IsPlayerInRangeOfPoint(playerid, 3.0, -1579.0845,467.1120,43.3883)) && GetPlayerVirtualWorld(playerid) == h+50)
            {
                SetPlayerInterior(playerid,HouseInfo[PI[playerid][pLocal]][hInt]);
                SetPlayerVirtualWorld(playerid,PI[playerid][pLocal]+50);
                SetPPos(playerid,HouseInfo[h][hExitx], HouseInfo[h][hExity], HouseInfo[h][hExitz]);
            }
        }
    }
    return 1;
}

