cmd:showudost(playerid, params[])
{
    if(!IsACop(playerid) || !IsAArm(playerid)) return SendClientMessage(playerid, COLOR_GREY, "У вас нет удостоверения.");
    if(sscanf(params, "d",params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /showudost [id]");
    if(!IsPlayerConnected(params[0])) return 1;
    if(!ProxDetectorS(8.0, playerid, params[0])) return 1;
    new ttext[32];
    if(PI[playerid][pMember] == 0) { ttext = "Гражданин"; }
    else if(PI[playerid][pMember] == 1) { ttext = "МВД Г. Арзамас"; }
    else if(PI[playerid][pMember] == 2) { ttext = "ФСБ"; }
    else if(PI[playerid][pMember] == 10) { ttext = "SFPD"; }
    else if(PI[playerid][pMember] == 21) { ttext = "LVPD"; }
    else if(PI[playerid][pMember] == 26) { ttext = "Администрация Президента"; }
    else if(PI[playerid][pMember] == 25) { ttext = "SAPD SWAT"; }
    else if(PI[playerid][pMember] == 25) { ttext = "Мэрия"; }
    else if(PI[playerid][pMember] == 3) { ttext = "SFA"; }
    else if(PI[playerid][pMember] == 19) { ttext = "LVA"; }
    new msg[] = "Имя: %s\nТелефон: %i\nВозраст: %i\nПодразделение: %s\nРанг: %i";
    format(stringer1, sizeof(stringer1), msg,PI[playerid][pNames],PI[playerid][pPnumber],PI[playerid][pLevel],ttext,PI[playerid][pRank]);
    SPD(params[0], 10036, DIALOG_STYLE_MSGBOX, "Удостоверение",stringer1, "Готово", "");
    format(stringer1, sizeof(stringer1), "%s показал своё удостоверение %s'у",PI[playerid][pNames],PI[params[0]][pNames]);
    ProxDetector(playerid, 30.0, COLOR_PURPLE, stringer1);
    format(stringer1,sizeof(stringer1), "показал своё удостоверение %s'у",PI[params[0]][pNames]);
    SetPlayerChatBubble(playerid,stringer1,COLOR_PURPLE,30.0,10000);
    return 1;
}
cmd:amask(playerid, params[])
{
    if(PI[playerid][pAdmin] < 12) return SendClientMessage(playerid, COLOR_GRAD1, "Вы администратор ниже 12-ого уровня !");
    SetPlayerColor(playerid, 0x7a766700);
    PI[playerid][pMask] = 6000000;
    PI[playerid][Texts][2] --;
    GameTextForPlayer(playerid, "~b~ INVISABLE ON", 800, 4);
    return 1;
}
cmd:offamask(playerid, params[])
{
    if(PI[playerid][pAdmin] < 12) return SendClientMessage(playerid, COLOR_GRAD1, "Вы администратор ниже 12-ого уровня !");
    SetPlayerToTeamColor(playerid);
    PI[playerid][pMask] = 1;
    PI[playerid][Texts][2] --;
    GameTextForPlayer(playerid, "~b~ INVISABLE OFF", 800, 4);
    return 1;
}
cmd:mask(playerid)
{
    if(PI[playerid][pMask] == 0)
    {
        if(!PI[playerid][Texts][2]) return SendClientMessage(playerid, COLOR_GRAD1, "У вас нету маски!");
        SetPlayerColor(playerid, 0x7a766700);
        PI[playerid][pMask] = 600;
        PI[playerid][Texts][2] --;
        FIXES_ApplyAnimation(playerid, "SHOP", "ROB_Shifty", 4.1, 0, 0, 0, 0, 0);
        SendClientMessage(playerid, -1, "{3399FF}Ваше место положение на GPS скрыто на 10 минут!");
        SetPlayerChatBubble(playerid, "{FF9900}Одевает маску", -1, 20.0, 500);
        GameTextForPlayer(playerid, "~b~ INVISABLE ON", 800, 4);
        PlayerPlaySound(playerid, 21000, 0.0, 0.0, 0.0);
        format(stringer1, sizeof(stringer1), "%s достал маску", PI[playerid][pNames]);
        ProxDetector(playerid, 20.0, COLOR_PURPLE, stringer1);

        PI[playerid][MASK_TIMER] = SetTimerEx("offmask", 600000, 0, "d", playerid);

    }
    else
    {
        SetPlayerToTeamColor(playerid);
        PI[playerid][pMask] = 0;
        format(stringer1, sizeof(stringer1), "%s снял маску",  PI[playerid][pNames]);
        ProxDetector(playerid, 20.0, COLOR_PURPLE, stringer1);
    }
    return 1;
}

forward offmask(playerid);
public offmask(playerid)
{
    PI[playerid][pMask] = 0;
    SetPlayerToTeamColor(playerid);
    SetPlayerChatBubble(playerid, "{FF9900}Снимает маску", -1, 20.0, 500);
    GameTextForPlayer(playerid, "~y~ INVISABLE OFF", 800, 4);
    PlayerPlaySound(playerid, 21001, 0.0, 0.0, 0.0);
    RemovePlayerAttachedObject(playerid, 0);
    return KillTimer(PI[playerid][MASK_TIMER]);
}

cmd:putkazna(playerid, params[])
{
    if(sscanf(params, "d",params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /putkazna [кол-во]");
    if(params[0] > 20000000 || params[0] < 1) return SendClientMessage(playerid,COLOR_WHITE,"Ошибка: Нельзя меньше 1 и больше 20.000.000 руб.");
    if(PI[playerid][pCash] < params[0]) return SendClientMessage(playerid,COLOR_GREY,"У вас недостаточно денег");
    if(!IsPlayerInRangeOfPoint(playerid, 15.0,686.8506,-13.5878,1201.858)) return SendClientMessage(playerid, COLOR_WHITE, "Вы далеко от места пожертвования!");
    PI[playerid][pCash] -= params[0];
    kazna += params[0];
    SendMes(playerid, 0x6495EDFF, "Вы повысили казну штата на %s, теперь в казне %s руб..", formatInt(params[0]), formatInt(kazna));
    format(stringer1, sizeof(stringer1), "Положил в казну штата %s руб..", formatInt(params[0]));
    SetPlayerChatBubble(playerid,stringer1,COLOR_PURPLE,30.0,10000);
    format(stringer1,sizeof(stringer1),"{C2A2DA}Казна штата: "SERVER"%s\n{C2A2DA}Налог государства: "SERVER"%s\n{C2A2DA}Пожертвовать денег введите '/putkazna'", formatInt(kazna), formatInt(nalog));
    Update3DTextLabelText(kaznameria, 0xC2A2DAFF, stringer1);
    SaveMeria();
    return 1;
}
cmd:takekazna(playerid, params[])
{
    if(PI[playerid][pLeader] == 7 || PI[playerid][pLeader] == 26)
    {
        if(sscanf(params, "d",params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /takekazna [кол-во]");
        if(params[0] > 5000000 || params[0] < 1) return SendClientMessage(playerid,COLOR_WHITE,"Ошибка: Нельзя меньше 1 и больше 5.000.000 руб.");
        if(kazna < params[0]) return SendClientMessage(playerid,COLOR_GREY,"В казне нет столько денег!");
        if(!IsPlayerInRangeOfPoint(playerid, 15.0, 688.8973,-27.8779,1201.8580)) return SendClientMessage(playerid, COLOR_WHITE, "Вы должны находится в Мэрии.");
        GiveMoney(playerid, params[0]);
        kazna -= params[0];
        SendMes(playerid, 0x6495EDFF, "Вы сняли с казны штата %s руб., теперь в казне %s руб..", formatInt(params[0]), formatInt(kazna));
        format(stringer1,sizeof(stringer1),"{C2A2DA}Казна штата: "SERVER"%s\n{C2A2DA}Налог государства: "SERVER"%s\n{C2A2DA}Пожертвовать денег введите '/putkazna'", formatInt(kazna), formatInt(nalog));
        Update3DTextLabelText(kaznameria, 0xC2A2DAFF, stringer1);
        SaveMeria();
    }
    return 1;
}
cmd:setleader(playerid, params[])
{
    if(PI[playerid][pAdmin] < 5) return 1;
    if(sscanf(params, "dd", params[0], params[1])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /setleader [id] [номер фракции (1 - 28)]");
    if(params[1] != 1 && params[1] != 2 && params[1] != 5 && params[1] != 6 && params[1] != 14 && params[1] != 16 && params[1] != 19 && params[1] != 7 && params[1] != 23 && params[1] != 24) return SendClientMessage(playerid, COLOR_GREY, "Такой лидерки не существует!");
    if(!IsPlayerConnected(params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Ошибка: Игрока не существует");
    if(PI[params[0]][pMember] > 0 && !PI[params[0]][pAdmin]) return SendClientMessage(playerid, COLOR_GRAD1,"Игрок во фракции.");
    if(PI[params[0]][pWarns] >= 1) return SendClientMessage(playerid, COLOR_GRAD1,"У игрока предупреждение.");
    new query[400];
    format(query, sizeof(query), "SELECT * FROM `accounts` WHERE `pLeader` = '%d' LIMIT 1", params[1]);
    mysql_function_query(connects, query, true, "SetLeader", "ddd", playerid, params[0], params[1]);
    return 1;
}

publics: SetAuctionLeader(issuerid, fraction)
{

    SendMes(issuerid, COLOR_BLUE, "Вас назначили контролировать организацию %s", GetPlayerFracName[fraction]);
    switch(fraction)
    {
    case 1,10,21: PI[issuerid][pRank] = 15, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (54) : (187);
    case 2: PI[issuerid][pRank] = 10, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (84) : (163);
    case 3,19: PI[issuerid][pRank] = 15, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (61) : (73);
    case 4: PI[issuerid][pRank] = 10, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (30) : (31);
    case 5: PI[issuerid][pRank] = 10, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (98) : (109);
    case 6: PI[issuerid][pRank] = 10, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (186) : (169);
    case 7: PI[issuerid][pRank] = 10, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (17) : (59);
    case 11: PI[issuerid][pRank] = 10, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (59) : (172);
    case 12: PI[issuerid][pRank] = 10, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (104) : (195);
    case 13: PI[issuerid][pRank] = 10, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (110) : (190);
    case 14: PI[issuerid][pRank] = 10, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (272) : (214);
    case 15: PI[issuerid][pRank] = 10, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (46) : (111);
    case 16: PI[issuerid][pRank] = 13, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (116) : (164);
    case 17: PI[issuerid][pRank] = 10, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (115) : (41);
    case 18: PI[issuerid][pRank] = 10, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (173) : (226);
    case 23: PI[issuerid][pRank] = 12, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (164) : (172);
    case 24: PI[issuerid][pRank] = 10, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (67) : (55);
    case 25: PI[issuerid][pRank] = 10, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (285) : (76);
    case 26: PI[issuerid][pRank] = 10, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (295) : (12);
    case 27,28: PI[issuerid][pRank] = 5, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (189) : (172);
    }
    SetPlayerSkin(issuerid, PI[issuerid][pModel]);
    SetPlayerArmour(issuerid,0);
    SetPlayerHealth(issuerid,100);
    PI[issuerid][pLeader] = fraction;
    PI[issuerid][pMember] = fraction;
    PI[issuerid][pJob] = 0;
    if(!PI[issuerid][pAdmin]) PI[issuerid][pVigs] = 0;
    SetPlayerToTeamColor(issuerid);

    switch(PI[issuerid][pMember])
    {
    case 1..4, 7, 10, 11, 19, 21, 25, 26: PI[issuerid][pOdet] = 1, SendClientMessage(issuerid,COLOR_BLUE,"Ваш рабочий день успешно начат!");
    }
    new query[400];
    format(query, sizeof(query), "UPDATE `accounts` SET `pMember` = '%d', `pLeader` = '%d', `pRank` = '%d', `pModel` = '%d', `pJob` = '0', `odet` = '%d' , `vigs` = '%d' WHERE `pID` = '%d'",fraction,fraction,PI[issuerid][pRank],PI[issuerid][pModel],PI[issuerid][pOdet],PI[issuerid][pVigs],PI[issuerid][pID]);
    mysql_function_query(connects, query, false, "", "");
    return 1;
}

publics: SetLeader(playerid, issuerid, fraction) {
    new rows, fields;
    cache_get_data(rows, fields);
    if(!rows) {
        SendMes(issuerid, COLOR_BLUE, "Администратор %s назначил Вас контролировать организацию %s", PI[playerid][pNames], GetPlayerFracName[fraction]);
        format(stringer1, sizeof(stringer1), "[A] %s назначил %s контролировать фракцию %s (%d).",PI[playerid][pNames], PI[issuerid][pNames],GetPlayerFracName[fraction],fraction);
        SendAdminMessage(COLOR_GREY,stringer1);
        switch(fraction)
        {
        case 1,10,21: PI[issuerid][pRank] = 15, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (54) : (187);
        case 2: PI[issuerid][pRank] = 10, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (84) : (163);
        case 3,19: PI[issuerid][pRank] = 15, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (61) : (73);
        case 4: PI[issuerid][pRank] = 10, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (30) : (31);
        case 5: PI[issuerid][pRank] = 10, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (98) : (109);
        case 6: PI[issuerid][pRank] = 10, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (186) : (169);
        case 7: PI[issuerid][pRank] = 10, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (17) : (59);
        case 11: PI[issuerid][pRank] = 10, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (59) : (172);
        case 12: PI[issuerid][pRank] = 10, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (104) : (195);
        case 13: PI[issuerid][pRank] = 10, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (110) : (190);
        case 14: PI[issuerid][pRank] = 10, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (272) : (214);
        case 15: PI[issuerid][pRank] = 10, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (46) : (111);
        case 16: PI[issuerid][pRank] = 13, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (116) : (164);
        case 17: PI[issuerid][pRank] = 10, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (115) : (41);
        case 18: PI[issuerid][pRank] = 10, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (173) : (226);
        case 23: PI[issuerid][pRank] = 12, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (164) : (172);
        case 24: PI[issuerid][pRank] = 10, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (67) : (55);
        case 25: PI[issuerid][pRank] = 10, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (285) : (76);
        case 26: PI[issuerid][pRank] = 10, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (295) : (12);
        case 27,28: PI[issuerid][pRank] = 5, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (189) : (172);
        }
        SetPlayerSkin(issuerid, PI[issuerid][pModel]);
        SetPlayerArmour(issuerid,0);
        SetPlayerHealth(issuerid,100);
        PI[issuerid][pLeader] = fraction;
        PI[issuerid][pMember] = fraction;
        PI[issuerid][pJob] = 0;
        if(!PI[issuerid][pAdmin]) PI[issuerid][pVigs] = 0;
        SetPlayerToTeamColor(issuerid);
        format(stringer1, sizeof(stringer1), "выдал лидерство организации %s",GetPlayerFracName[fraction]);
        Admin_Log(playerid, stringer1, PI[issuerid][pNames], gettime());
        switch(PI[issuerid][pMember])
        {
        case 1..4, 7, 10, 11, 19, 21, 25, 26: PI[issuerid][pOdet] = 1, SendClientMessage(issuerid,COLOR_BLUE,"Ваш рабочий день успешно начат!");
        }
        new query[400];
        format(query, sizeof(query), "UPDATE `accounts` SET `pMember` = '%d', `pLeader` = '%d', `pRank` = '%d', `pModel` = '%d', `pJob` = '0', `odet` = '%d' , `vigs` = '%d' WHERE `pID` = '%d'",fraction,fraction,PI[issuerid][pRank],PI[issuerid][pModel],PI[issuerid][pOdet],PI[issuerid][pVigs],PI[issuerid][pID]);
        mysql_function_query(connects, query, false, "", "");
    } else {
        new string[144], name[24];
        cache_get_field_content(0, "Name", name, connects, 24);
        format(string, sizeof(string), "У этой фракции уже есть лидер(%s)! Сначала снимите его!", name);
        SendClientMessage(playerid, -1, string);
    }
    return 1;
}
cmd:templeader(playerid, params[])
{
    if(PI[playerid][pAdmin] < 3) return 1;
    {
        if(sscanf(params, "d",params[0])) return SendClientMessage(playerid, 0xCECECEFF, "Используйте: /templeader [ID фракции]");
        if(params[0] != 1 && params[0] != 2 && params[0] != 5 && params[0] != 6 && params[0] != 14 && params[0] != 16 && params[0] != 19 && params[0] != 7 && params[0] != 23 && params[0] != 24) return SendClientMessage(playerid, COLOR_GREY, "Такой лидерки не существует!");
        new issuerid = playerid;
        switch(params[0])
        {
        case 1,10,21: PI[issuerid][pRank] = 15, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (54) : (187);
        case 2: PI[issuerid][pRank] = 10, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (84) : (163);
        case 3,19: PI[issuerid][pRank] = 15, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (61) : (73);
        case 4: PI[issuerid][pRank] = 10, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (30) : (31);
        case 5: PI[issuerid][pRank] = 10, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (98) : (109);
        case 6: PI[issuerid][pRank] = 10, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (186) : (169);
        case 7: PI[issuerid][pRank] = 10, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (17) : (59);
        case 11: PI[issuerid][pRank] = 10, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (59) : (172);
        case 12: PI[issuerid][pRank] = 10, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (104) : (195);
        case 13: PI[issuerid][pRank] = 10, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (110) : (190);
        case 14: PI[issuerid][pRank] = 10, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (272) : (214);
        case 15: PI[issuerid][pRank] = 10, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (46) : (111);
        case 16: PI[issuerid][pRank] = 13, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (116) : (164);
        case 17: PI[issuerid][pRank] = 10, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (115) : (41);
        case 18: PI[issuerid][pRank] = 10, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (173) : (226);
        case 23: PI[issuerid][pRank] = 12, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (164) : (172);
        case 24: PI[issuerid][pRank] = 10, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (67) : (55);
        case 25: PI[issuerid][pRank] = 10, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (285) : (76);
        case 26: PI[issuerid][pRank] = 10, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (295) : (12);
        case 27,28: PI[issuerid][pRank] = 5, PI[issuerid][pModel] = (PI[issuerid][pSex] == 1) ? (189) : (172);
        }
        new string[144];
        SetPlayerSkinEx(playerid, PI[playerid][pModel]);
        SetPlayerArmour(playerid,0);
        SetPlayerHealth(playerid,100);
        SetPVarInt(playerid, "TempLeader", 1);
        PI[playerid][pLeader] = params[0];
        PI[playerid][pMember] = params[0];
        PI[playerid][pJob] = 0;
        if(!PI[playerid][pAdmin]) PI[playerid][pVigs] = 0;
        SetPlayerToTeamColor(playerid);
        format(stringer1, sizeof(stringer1), "выдал временное лидерство организации {00ff00}%s",GetPlayerFracName[PI[playerid][pMember]]);
        format(string, sizeof(string), "[A] %s[%d] выдал себе временное лидерство организации {00ff00}%s", PI[playerid][pNames], playerid, GetPlayerFracName[PI[playerid][pMember]]);
        SendAdminMessage(COLOR_GREY, string);
        Admin_Log(playerid, string, PI[playerid][pNames], gettime());
        switch(PI[playerid][pMember])
        {
        case 1..4, 7, 10, 11, 19, 21, 25: PI[playerid][pOdet] = 1, SendClientMessage(playerid,COLOR_BLUE,"Ваш рабочий день успешно начат!");
        }
    }
    return 1;
}
cmd:nalog(playerid, params[])
{
    if(PI[playerid][pLeader] == 7 || PI[playerid][pLeader] == 26)
    {
        if(sscanf(params, "d",params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /nalog [кол-во]");
        if(params[0] > 15000 || params[0] < 1000) return SendClientMessage(playerid,COLOR_WHITE,"Налог не можеть быть меньше 1.000 и больше 15.000!");
        if(IsPlayerInRangeOfPoint(playerid, 50.0, 688.8973,-27.8779,1201.8580) || IsPlayerInRangeOfPoint(playerid, 50.0, 688.8973,-27.8779,1201.8580))
        {
            nalog = params[0];
            SendClientMessageToAll(COLOR_WHITE, " ===== [Срочные новости] ===== ");
            format(stringer1, sizeof(stringer1), "Призидент штата поменял налог государства. Теперь всемирный налог равен {FFFFFF}%s {6495ED}руб..",formatInt(params[0]));
            SendClientMessageToAll(0x6495EDFF, stringer1);
            format(stringer1,sizeof(stringer1),"{C2A2DA}Казна штата: "SERVER"%s\n{C2A2DA}Налог государства: "SERVER"%s\n{C2A2DA}Пожертвовать денег введите '/putkazna'", formatInt(kazna), formatInt(nalog));
            Update3DTextLabelText(kaznameria, 0xC2A2DAFF, stringer1);
            SaveMeria();
        }
        else return SendClientMessage(playerid,COLOR_WHITE,"Необходимо находиться в здании мэрии, или правительства!");
    }
    return 1;
}
cmd:election(playerid, params[])
{
    if(PI[playerid][pAdmin] < 13) return 1;
    ShowPlayerDialog(playerid, 8922, 2, "Выборы", "Начать выборы\nЗакончить выборы\nИзменить имена кандидатов", "Принять", "Отмена");
    return 1;
}
cmd:getpick(playerid, params[])
{
    if(PI[playerid][pAdmin] < 7) return 1;
    if(Pick == 0) return SendClientMessage(playerid, COLOR_GREY, "В штате нет выборов!");
    GetPick(playerid, 1);
    return 1;
}
cmd:free(playerid, params[])
{
    if(PI[playerid][pMember] == 7 && PI[playerid][pRank] == 6)
    {
        if(sscanf(params, "dd", params[0],params[1])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /free [id] [цена]");
        if(PI[params[0]][pJailed] != 1) return SendClientMessage(playerid, COLOR_GRAD1, "Человек не в тюрьме!");
        if(params[1] < 100 || params[1] > 50000) { SendClientMessage(playerid, COLOR_GREY, "Цена не может быть меньше 100 и больше 50000!"); return 1; }
        if(params[1] == playerid)  return  SendClientMessage(playerid,COLOR_GRAD1, "[Ошибка] Вы указали свой ID");
        if(!ProxDetectorS(15.0, playerid, params[0])) return SendClientMessage(playerid, COLOR_GRAD1, "Этот человек далеко от вас!");
        format(stringer1, sizeof(stringer1), "[Адвокат] Вы предложили %s выпустить его за %s руб.",PI[params[0]][pNames],formatInt(params[1]));
        SendClientMessage(playerid, 0x6ab1ffaa, stringer1);
        format(stringer1, sizeof(stringer1), "[Адвокат] Адвокат %s предлагает Вам свободу за %s руб.",PI[playerid][pNames],formatInt(params[1]));
        SendClientMessage(params[0], 0x6ab1ffaa, stringer1);
        SPD(params[0],6622,DIALOG_STYLE_MSGBOX,"Адвокатские услуги", "Выберите способ оплаты:", "Наличные", "Банк");
        FreeOffer[params[0]] = playerid;
        FreePrice[params[0]] = params[1];
        SposobOffer[params[0]] = 0;
    }
    else SendClientMessage(playerid, COLOR_GREY, "Вы не адвокат!");
    return 1;
}

cmd:agivevip(playerid, params[])
{
    if(PI[playerid][pAdmin] != 13) return 1;
    if(sscanf(params, "dd", params[0],params[1])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /agivevip [id] [уровень VIP]");
    if(!IsPlayerConnected(params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Ошибка: Игрока не существует");
    switch(params[1])
    {
    case 1: SendClientMessage(params[0], COLOR_WHITE, "Поздравляем, вы получили от администрации {FFA500}Bronze VIP");
    case 2: SendClientMessage(params[0], COLOR_WHITE, "Поздравляем, вы получили от администрации {FFA500}Silver VIP");
    case 3: SendClientMessage(params[0], COLOR_WHITE, "Поздравляем, вы получили от администрации {FFA500}Gold VIP");
    default: SendClientMessage(params[0], COLOR_WHITE, "Администратор забрал у вас VIP Аккаунт!");
    }
    if(params[2] == 0)
    {
        PI[params[0]][pDonateRank] = 0;
        SendClientMessage(playerid, COLOR_WHITE, "- Вы забрали у игрока VIP Статус.");
    }
    else
    {
        PI[params[0]][pDonateRank] = params[1];
        SendClientMessage(playerid, COLOR_WHITE, "- Вы выдали игроку VIP Статус.");
    }
    return 1;
}

cmd:menu(playerid, params[])
{
    if(PI[playerid][pLeader] == 23 || PI[playerid][pMember] == 23) SPD(playerid,7763,DIALOG_STYLE_LIST,"Портативный компьютер","{FF4500}1. Доступные контракты\n2. Проверить контракт\n3. Отменить заказ\n4. Члены агенства {32CD32}Online\n5. Поддельный паспорт\n6. Надеть маску","Далее","Закрыть");
    else return SendClientMessage(playerid, COLOR_WHITE, "Вам не доступна данная функция");
    return 1;
}


