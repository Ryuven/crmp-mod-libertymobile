cmd:changeskin(playerid, params[])
{
    if(!PI[playerid][pMember]) return SendClientMessage(playerid,COLOR_WHITE,"Вы не состоите во организации");
    switch(PI[playerid][pMember]) { case 1..4, 7, 10, 11, 19, 21, 25: if(PI[playerid][pOdet] == 0) return SendClientMessage(playerid,COLOR_WHITE,"Вы не на службе!"); }
    if(PI[playerid][pRank] < gFractionRank[PI[playerid][pMember]-1][2]) return SendMes(playerid, -1, "Доступно с %i ранга", gFractionRank[PI[playerid][pMember]-1][2]);
    if(sscanf(params, "d", params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Используйте: /changeskin [id игрока]");
    if(!IsPlayerConnected(params[0])) return 1;
    if(!ProxDetectorS(8.0, playerid, params[0]) || GetPlayerVirtualWorld(playerid) != GetPlayerVirtualWorld(params[0])) return SendClientMessage(playerid,COLOR_WHITE,"Вы далеко друг от друга");
    if(PI[params[0]][pMember] != PI[playerid][pMember]) return SendClientMessage(playerid,COLOR_WHITE,"Игрок не состоит в вашей организации");
    if(PI[params[0]][pRank] >= PI[playerid][pRank] && params[0] != playerid) return SendClientMessage(playerid,COLOR_WHITE,"Ваш ранг недостаточен");
    new fractionid = PI[playerid][pMember] - 1;
    SetPVarInt(playerid,"actplayerid",params[0]);
    for(new i;i<12;i++)
    {
        new skinid = gFractionSkin[fractionid][i];
        if(!skinid) break;
        if(skinid == PI[params[0]][pModel])
        {
            if(!i) format(stringer,sizeof(stringer),"{33AA33}Скин %d [%d]",i+1,skinid);
            else format(stringer,sizeof(stringer),"%s\n{33AA33}Скин %d [%d]",stringer,i+1,skinid);
        }
        else
        {
            if(!i) format(stringer,sizeof(stringer),"Скин %d [%d]",i+1,skinid);
            else format(stringer,sizeof(stringer),"%s\nСкин %d [%d]",stringer,i+1,skinid);
        }
    }
    SPD(playerid,2213,2,PI[params[0]][pNames],stringer,"OK","Отмена");
    return 1;
}
cmd:bizlock(playerid, params[])
{
    if(PI[playerid][pMember] != 2 || PI[playerid][pRank] < 5) return 1;
    new null, null_;
    for(new b = 1; b <= TOTALBIZZ; b++)
    {
        if(!IsPlayerInRangeOfPoint(playerid, 3.0,BizzInfo[b][bEntrx], BizzInfo[b][bEntry], BizzInfo[b][bEntrz]) && !IsPlayerInRangeOfPoint(playerid, 3.0,BizzInfo[b][bExitx], BizzInfo[b][bExity], BizzInfo[b][bExitz]) || BizzInfo[b][bLock]) continue;
        foreach(Player,i) if(GetDistanceBetweenPlayers(playerid,i) < 15 && PI[i][pMember] == 2 && PI[i][pRank] > 2) null++;
        null_++;
        if(null < 2) return SendClientMessage(playerid, COLOR_GREY,"С вами должны находиться как минимум 2 понятых!");
        BizzInfo[b][bLock] = 1;
        SendClientMessage(playerid, COLOR_GRAD1, "Вы закрыли бизнес");
        PlayerPlaySound(playerid, 1145, 0.0, 0.0, 0.0);
        SaveBizz(b);
        return 1;
    }
    if(null_ == 0) return SendClientMessage(playerid, COLOR_GREY,"Вы должны находиться у бизнеса!");
    return 1;
}
cmd:lomka(playerid, params[])
{
    if(PI[playerid][pMember] != 4) return 1;
    if(sscanf(params, "d",params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /lomka [id]");
    if(!IsPlayerConnected(params[0]) || params[0] == playerid) return 1;
    SetPlayerWeather(params[0], 14);
    SendMes(playerid,COLOR_GREEN,"Вы спасли %s от ломки",PI[params[0]][pNames]);
    SendMes(params[0],COLOR_GREEN,"Медик %s спас вас от ломки",PI[playerid][pNames]);
    FIXES_ApplyAnimation(params[0],"CARRY","crry_prtial",4.0,0,0,0,0,0,1);
    if(GetPVarInt(params[0], "CurrentPlayerDrug")) {
        DeletePVar(playerid, "CurrentPlayerDrug");
        ClearPlayerDrugEffects(playerid);
    }
    return 1;
}
cmd:heal(playerid, params[])
{
    if(PI[playerid][pMember] != 4 || (TeamPain[playerid] != 0 && StartPaintBall == 2)) return 1;
    if(sscanf(params, "d",params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /heal [id]");
    if(!IsPlayerConnected(params[0]) || params[0] == playerid) return SendClientMessage(playerid, COLOR_GRAD1, "Такого игрока нет, или это вы!");
    if(!ProxDetectorS(8.0, playerid, params[0])) return SendClientMessage(playerid, COLOR_GRAD1, "Этот человек далеко от вас!");
    format(stringer1, sizeof(stringer1), "Вы предложили вылечить %s за %d руб..",PI[params[0]][pNames],healpric);
    SendClientMessage(playerid, 0x6ab1ffaa, stringer1);
    format(stringer1,sizeof(stringer1), "Желаете вылечиться за %d руб.?",healpric);
    SPD(params[0],5522,DIALOG_STYLE_MSGBOX,"Лечение", stringer1, "Да", "Нет");
    HealOffer[params[0]] = playerid;
    HealPrice[params[0]] = healpric;
    return 1;
}
cmd:back(playerid) {
    if(!GetPVarFloat(playerid, "BackX")) return 1;
    SetPPos(playerid, GetPVarFloat(playerid, "BackX"), GetPVarFloat(playerid, "BackY"), GetPVarFloat(playerid, "BackZ"));
    SetPlayerInteriorEx(playerid, GetPVarInt(playerid, "BackIn"));
    SetPlayerVirtualWorldEx(playerid, GetPVarInt(playerid, "BackVw"));
    DeletePVar(playerid, "BackX"), DeletePVar(playerid, "BackY"), DeletePVar(playerid, "BackZ"), DeletePVar(playerid, "BackIn"), DeletePVar(playerid, "BackVw");
    SendClientMessage(playerid, COLOR_WHITE, "Вы были успешно телепортированы!");
    return 1;
}

cmd:bank(playerid)
{
    if(!IsPlayerInRangeOfPoint(playerid, 5.0, -99.3043,711.8790,1600.2032) && !IsPlayerInRangeOfPoint(playerid, 5.0, -101.3860,711.8343,1600.2032)
    && !IsPlayerInRangeOfPoint(playerid, 5.0, -103.4569,712.0463,1600.2032) && !IsPlayerInRangeOfPoint(playerid, 5.0, -105.4870,711.9747,1600.1958)
    && !IsPlayerInRangeOfPoint(playerid, 5.0, 2904.1794,2511.7959,1051.0000)) return SendClientMessage(playerid, COLOR_WHITE, !"Вы далеко от банковского окна!");

    return SPD(playerid, 8810, DIALOG_STYLE_LIST, "Банковские услуги", "{ffffff}1. Управление счетами\n2. Перевести на счёт бизнеса\n3. Домашний счёт\n4. Забрать деньги за дом\n5. Заказать продукты для бизнеса", "Выбрать", "Закрыть");
}

alias:an("ans", "reps", "ot", "pm");
CMD:an(playerid, params[])
{
    if(GetPlayerAdmin(playerid))
    {
        new targetid, text[81];

        if (sscanf(params, "is[81]", targetid, text) || targetid < 0 ) {
            if (!report_Show(playerid) && strlen(params) > 2) {
                SendClientMessage(playerid, COLOR_WHITE, !"{afafaf}Используйте: /an [playerid / reportid] [текст / ответ]");
            }
            return true;
        }
        if (targetid >= MAX_PLAYERS) {

            new report_id = targetid, status = REPORT_STATUS_ANSWERED, Cache:result;

            mysql_query_[0] = EOS;

            mysql_format(connects, mysql_query_, sizeof (mysql_query_), "SELECT * FROM `"TABLE_REPORTS"` WHERE `id` = '%d' AND `status` = 0", report_id);
            result = mysql_query(connects, mysql_query_, true);

            targetid = cache_get_field_content_int(0, "player_id");
            cache_get_field_content(0, "text", TempReportInfo[playerid][reportText], connects, 144);
            cache_get_field_content(0, "date", TempReportInfo[playerid][reportDate], connects, 20);

            mysql_query_[0] = EOS;
            new rows = cache_num_rows();

            if(!rows) {
                cache_delete(result);
                SendClientMessage(playerid, COLOR_WHITE, !"{afafaf}На данный запрос уже был дан ответ!");
                return 1;
            }
            mysql_format(connects, mysql_query_, sizeof (mysql_query_), "UPDATE `"TABLE_REPORTS" `SET `status` = '%d', `answer` = '%s' WHERE `id` = '%d' AND `status` = 0", status, text, report_id);
            mysql_tquery(connects, mysql_query_);

            format(stringer1, sizeof (stringer1), "\
                {FFFFFF}Дата запроса: {afafaf}%s\n \n\
                {FFFFFF}Ваш запрос №%d:\n\
                {afafaf}| {afafaf}%s\n \n\
                {FFFFFF}Ответ от администрации:\n\
                {afafaf}| "SERVER"%s",
                TempReportInfo[playerid][reportDate], report_id, TempReportInfo[playerid][reportText], text
            );
            SPD(targetid, 0000, DIALOG_STYLE_MSGBOX, ""SERVER"Ответ администрации", stringer1, !"Спасибо", !"");

            format(stringer1, sizeof (stringer1), ""SERVER"Ваш запрос №%d: {FFFFFF}%s", report_id, TempReportInfo[playerid][reportText]);
            SendClientMessage(targetid, COLOR_YELLOW, stringer1);
            format(stringer1, sizeof (stringer1), ""SERVER"Ответ от администратора %s[%d]: {FFFFFF}%s", PI[playerid][pNames], playerid, text);
            SendClientMessage(targetid, COLOR_YELLOW, stringer1), stringer1[0] = EOS;

            format(stringer1, sizeof(stringer1), "{afafaf}[A] %s[%d] ответил игроку %s[%i]: %s",
                PI[playerid][pNames], playerid, PI[targetid][pNames], targetid, text
            );
            SendAdminMessage(0xFfa500AA, stringer1);

            if(PI[playerid][pAdmin]) PI[playerid][pRepAdm]++;
            else PI[playerid][pHotvet] ++;

            cache_delete(result);

        } else {
            if (!IsPlayerConnected(targetid))
                return SendClientMessage(playerid, COLOR_WHITE, !"{afafaf}Игрок не найден");

            if (strlen(text) > 80)
                return SendClientMessage(playerid, COLOR_WHITE, !"{afafaf}Нельзя писать больше 80 символов!");

            if (targetid == playerid)
                return SendClientMessage(playerid, COLOR_WHITE, !"{afafaf}Нельзя ответить самому себе");

            format(stringer1, sizeof(stringer1), ""SERVER"Ответ от администратора %s[%d]: {FFFFFF}%s",
                PI[playerid][pNames], playerid, text
            );
            SendClientMessage(targetid, 0xffa141FF, stringer1);

            format(stringer1, sizeof(stringer1), "{afafaf}[A] %s[%d] ответил игроку %s[%d]: %s",
                PI[playerid][pNames], playerid, PI[targetid][pNames], targetid, text
            );
            SendAdminMessage(0xFfa500AA, stringer1);

            if(PI[playerid][pAdmin]) PI[playerid][pRepAdm]++;
            else PI[playerid][pHotvet] ++;

            stringer1[0] = EOS;
        }
    }
    return true;
}

