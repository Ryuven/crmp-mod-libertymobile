        for(new i = 0; i < sizeof HouseInfo; i++)
        {
            if(count < 30)
            {
                if(HouseInfo[i][hID])
                {
                    if(HouseInfo[i][hOwned])
                    {
                        switch(HouseInfo[i][hKlass])
                        {
                        case 0: takings = 300;
                        case 1: takings = 600;
                        case 2: takings = 900;
                        case 3: takings = 1200;
                        case 4: takings = 1500;
                        }
                        if(HouseInfo[i][hTakings] < takings * 2)
                        {
                            format(stringer1, 64, "№ дома %d\tСлетит через %d Деней\t Гос.стоимость %d\n", i, HouseInfo[i][hTakings]/takings, HouseInfo[i][hValue]);
                            strcat(fstring, stringer1);
                            //if(count % 10 == 0 && count != 0) format(fstring, sizeof fstring, "%s\t%i\n", fstring, i);
                            //else format(fstring, sizeof fstring, "%s\t%i", fstring, i);
                            count++;
                        }
                    }
                }
                //continue;
            }
        }
        if(count > 1) SPD(playerid, 0, DIALOG_STYLE_LIST, "{00FF00}Номера должников", fstring, "Закрыть", "");
        else SendClientMessage(playerid, -1, "В штате нет должников!");
        return 1;
    }
    else if(TOTALREALTORS > 0)
    {
        if(pickupid >= RI[0][bPickup] && pickupid <= RI[TOTALREALTORS-1][bPickup])
        {
            for(new i = 0; i < TOTALREALTORS; i++)
            {
                if(pickupid == RI[i][bPickup])
                {
