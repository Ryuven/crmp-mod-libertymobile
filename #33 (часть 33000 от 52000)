}

cmd:offjail(playerid, params[])
{
    if(PI[playerid][pAdmin] < 8) return 1;
    if(GetPVarInt(playerid,"MuteTime") > gettime())return SendClientMessage(playerid,COLOR_GREY,"Посадить/Заткнуть игрока можно раз в 10 секунд");


    new NameA[24], time,description[128];
    if(sscanf(params,"s[24]ds[128]",NameA,time,description)) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /offjail [Имя] [minute] [причина]");

    if(time > 60 || time < 0) return SendClientMessage(playerid,COLOR_WHITE,"Ошибка: Нельзя меньше 0 и больше 60");

    if((FD(params[0]) && !FD(playerid)) || IsIpFound(description)) return 1;
    //RemoveBadWords(params[2]);

    new query[144];
    format(query, sizeof(query), "SELECT * FROM `accounts` WHERE `Name` = '%s' AND `pAdmin` = 0 LIMIT 1", NameA);
    mysql_tquery(connects, query);
    if(cache_num_rows())
    {
        new mystring[250];
        mysql_format(connects, mystring, sizeof mystring, "UPDATE `accounts` SET `pJailTime` = '%d', `pJailed` = 4, `pMestoJail` = 2 WHERE `Name` = '%s'", time*60, NameA);
        mysql_tquery(connects, mystring);

        PI[playerid][pMuteAdm]++;
        format(stringer1, sizeof(stringer1), "[A-OFF] %s посадил игрока %s в деморган на %i минут(ы). Причина: %s",PI[playerid][pNames],NameA, time, description);
        SendAdminMessageToAll(COLOR_LIGHTRED ,stringer1,params[0]);
        Admin_Log(playerid, stringer1, NameA, gettime());
        SetPVarInt(playerid,"MuteTime",gettime()+10);
    }
    else
    SendClientMessage(playerid, -1, "Ник нейм не найден в базе данных или игрок администратор!");
    return 1;
}

alias:unjail("unprison");
cmd:unjail(playerid, params[]) {
    if(PI[playerid][pAdmin] < 4) return 1;
    if(PI[playerid][pVigs] > 2) return SendClientMessage(playerid, -1, "{FF6600}На вашем аккаунте выговор, некоторые команды были заблокированы. Снимите выговор — {FFFFFF}/adonate, {FF6600}чтобы снять блокировку команд.");
    if(sscanf(params, "d",params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /unjail [id]");
    if(!IsPlayerConnected(params[0])) return 1;
    format(stringer1, sizeof(stringer1), "Вы были освобождены администратором %s",PI[playerid][pNames]);
    SendClientMessage(params[0], COLOR_LIGHTRED, stringer1);
    format(stringer1, sizeof(stringer1), "[A] %s освободил игрока %s из деморгана", PI[playerid][pNames],PI[params[0]][pNames], params[1]);
    SendAdminMessage(COLOR_GREY,stringer1);
    if(PI[params[0]][pJailed] != 4) PI[params[0]][pUsedJail] = 0;
    PI[params[0]][pJailed] = 0;
    PI[params[0]][pJailTime] = 0;
    SpawnPlayerEx(params[0]);
    return 1;
}
cmd:rang(playerid, params[])
{
    if(!PI[playerid][pMember]) return 1;
    if(PI[playerid][pRank] < gFractionRank[PI[playerid][pMember]-1][2]) return SendMes(playerid, -1, "Доступно с %i ранга", gFractionRank[PI[playerid][pMember]-1][2]);
    if(sscanf(params, "dd",params[0],params[1])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /rang [id] [ранг]");
    switch(PI[playerid][pMember])
    {
    case 1,10,3,19,21: if(params[1] > 14 || params[1] < 1) return SendClientMessage(playerid, COLOR_GREY, "Нельзя меньше 1 и больше 14!");
    case 2,26,4,5,6,11,12,13,14,15,17,18,22,24,25,7: if(params[1] > 9 || params[1] < 1) return SendClientMessage(playerid, COLOR_GREY, "Нельзя меньше 1 и больше 9!");
    case 27,28: if(params[1] > 4 || params[1] < 1) return SendClientMessage(playerid, COLOR_GREY, "Нельзя меньше 1 и больше 4!");
    case 16: if(params[1] > 12 || params[1] < 1) return SendClientMessage(playerid, COLOR_GREY, "Нельзя меньше 1 и больше 12!");
    case 23: if(params[1] > 11 || params[1] < 1) return SendClientMessage(playerid, COLOR_GREY, "Нельзя меньше 1 и больше 11!");
    }
    if(!IsPlayerConnected(params[0]) || params[0] == playerid) return 1;
    switch(PI[playerid][pMember])
    {
    case 1..4, 7, 10, 11, 19, 21, 25:
        {
            if(PI[playerid][pOdet] == 0) return SendClientMessage(playerid,COLOR_WHITE,"Вы не на службе!");
            if(PI[params[0]][pRank] <= 5 && params[1] > 5)
            {
            //    if(PI[params[0]][pMedCard] == 0) return SendClientMessage(playerid,COLOR_WHITE,"Для получения данного ранга, у игрока должна быть медицинская карта.");
                if((PI[params[0]][pMember] == 1 || PI[params[0]][pMember] == 2 || PI[params[0]][pMember] == 10 || PI[params[0]][pMember] == 21 || PI[params[0]][pMember] == 25) && PI[params[0]][pVoenBill] == 0)
                {
                    return SendClientMessage(playerid,COLOR_WHITE,"Для получения данного ранга, у игрока должен быть военный билет. Купит его он может в /donaterub");
                }
            }
        }
    }
    if((PI[params[0]][pMember] == 3 || PI[params[0]][pMember] == 19) && params[1] >= 3 && PI[params[0]][pVoenBill] == 0)
    {
        PI[params[0]][pVoenBill] = 1;
        SendClientMessage(params[0], COLOR_GRAD1, "Поздравляем! Вы получили военный билет!");
        format(mysql_query_, sizeof(mysql_query_), "UPDATE `"TABLE_ACCOUNT"` SET `voenbill` = '1' WHERE `pID` = '%d'",PI[params[0]][pID]);
        mysql_function_query(connects, mysql_query_, false, "", "");
    }
    if (PI[params[0]][pLeader] >= 1) return SendClientMessage(playerid, COLOR_GREY, "[Ошибка] Вы указали ID лидера");
    if(PI[playerid][pMember] != PI[params[0]][pMember]) return SendClientMessage(playerid, COLOR_GREY, "Данный игрок не в вашей фракции!");
    if(PI[playerid][pMember] > 0 && PI[playerid][pRank] <= params[1]) return SendClientMessage(playerid,COLOR_GREY,"Ошибка");
    if(PI[playerid][pLeader] <= 0 && PI[playerid][pRank] <=  PI[params[0]][pRank]) return SendClientMessage(playerid, COLOR_GREY, "Ошибка");
    format(stringer1, sizeof(stringer1), "Вас повысил/понизил до %d ранга %s",params[1], PI[playerid][pNames]);
    SendClientMessage(params[0], 0x6BB3FFAA, stringer1);
    format(stringer1, sizeof(stringer1), "Вы повысили %s до %d ранга.", PI[params[0]][pNames],params[1]);
    SendClientMessage(playerid, 0x6BB3FFAA, stringer1);
    PI[params[0]][pRank] = params[1];
    return 1;
}

cmd:uninvite(playerid, params[])
{

    if(!PI[playerid][pMember]) return 1;
    switch(PI[playerid][pMember]) { case 1..4, 7, 10, 11, 19, 21, 25: if(PI[playerid][pOdet] == 0) return SendClientMessage(playerid,COLOR_WHITE,"Вы не на службе!"); }
    if(PI[playerid][pRank] < gFractionRank[PI[playerid][pMember]-1][1]) return SendMes(playerid, -1, "Доступно с %i ранга", gFractionRank[PI[playerid][pMember]-1][1]);
    if(sscanf(params, "ds[128]",params[0],params[1])) return SendClientMessage(playerid, COLOR_GRAD2, "Используйте: /uninvite [id] [причина]");
    if(!IsPlayerConnected(params[0]) || params[0] == playerid) return 1;
    if(PI[playerid][pRank] == PI[params[0]][pRank]) return SendClientMessage(playerid, COLOR_GREY, "[Ошибка] Вы не можете уволить этого игрока");
    if(PI[playerid][pLeader] <= 0 && PI[playerid][pRank] <= PI[params[0]][pRank]) return SendClientMessage(playerid, COLOR_GREY, "[Ошибка] Вы не можете уволить этого игрока");
    if(PI[params[0]][pMember] == PI[playerid][pLeader] || PI[params[0]][pMember] == PI[playerid][pMember])
    {
        if(PI[params[0]][pMember] <= 0 || PI[params[0]][pLeader] >= 1) return 1;
        if(IsPlayerStartCroupier(params[0]))
        {
            if(CroupierTable[params[0]] != -1)
            {
                switch(CroupierGame[params[0]])
                {
                case 1: OnPlayerExitDiceTable(params[0], CroupierTable[params[0]]);
                case 2: OnPlayerExitRouleTable(params[0], CroupierTable[params[0]]);
                case 3: OnPlayerExitBlackJackTable(params[0], CroupierTable[params[0]]);
                }
            }
            StartDuty[params[0]] = 0;
        }
        format(stringer1, sizeof(stringer1), "%s выгнал вас из организации. Причина: %s",PI[playerid][pNames],params[1]);
        SendClientMessage(params[0], 0x6495EDFF, stringer1);
        SetPlayerSkinEx(params[0], PI[params[0]][pChar]);
        format(stringer1, sizeof(stringer1), "Вы выгнали %s из организации с причиной: %s",PI[params[0]][pNames],params[1]);
        SendClientMessage(playerid, 0x6495EDFF, stringer1);
        SendClientMessage(params[0], COLOR_WHITE, "Теперь вы обычный гражданин...");
        SetPlayerArmour(params[0],0);
        ResetWeapons(params[0]);
        PI[params[0]][pMember] = 0;
        PI[params[0]][pLeader] = 0;
        PI[params[0]][pRank] = 0;
        SetPlayerColor(params[0], 0xFFFFFF80);
        SetPlayerSkinEx(params[0], PI[params[0]][pChar]);
        format(stringer1, sizeof(stringer1), "[A] Игрок %s[%d] уволил из организации %s[%d]. Причина: %s", PI[playerid][pNames], playerid,PI[params[0]][pNames], params[0], params[1]);
        SendAdminMessage(COLOR_GREY,stringer1);
        PI[params[0]][pOdet] = 0;
        format(stringer, sizeof(stringer), "UPDATE `"TABLE_ACCOUNT"` SET `pMember` = '0', `pRank` = '0', `pLeader` = '0', `odet` = '0' WHERE `pID` = '%d'",PI[params[0]][pID]);
        mysql_function_query(connects, stringer, false, "", "");
    }
    return 1;
}
cmd:mp(playerid, params[])
{
    if(PI[playerid][pAdmin] < 6) return SendClientMessage(playerid,COLOR_GREY,"У вас нет прав!");
    SPD(playerid, 5343, DIALOG_STYLE_LIST,""SERVER"Создание МП", "1. Включить/Выключить телепорт\n2. Раздать оружие(В радиусе 50)\n3. Раздать жизни(В радиусе 50)\n4. Обезоружить всех(В радиусе 50)\n5. Выдать временный скин(В радиусе 50)\n6. Выдать временный клист(В радиусе 50)", "Выбрать", "Отмена");
    return 1;
}
cmd:bugs(playerid, params[])
{
    if(GetPVarInt(playerid, "bug_time") > gettime()) return SendClientMessage(playerid, -1, "Отправлять информацию о баге можно только раз в 30 секунд.");
    ShowPlayerDialog(playerid, 22831, DIALOG_STYLE_INPUT, ""SERVER"Сообщение об ошибке игрового мода", "{FFFFFF}| Напишите в окно баг который Вы нашли. Мы обязательно исправим |","Отправить","Отмена");
    return 1;
}
cmd:sms(playerid, params[])
{
    if(PI[playerid][pPayDayHad] < 2) return SendClientMessage(playerid, COLOR_GREY, "Вы должны пробыть в игре 2 часа (/mm - Статистика персонажа).");
    if(sscanf(params, "ds[128]",params[0],params[1])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /sms [id] [текст]");
    if(gag[playerid] == 1) return SendClientMessage(playerid, COLOR_WHITE, "Ошибка: У вас стоит кляп");
    if(PI[playerid][pMuteTime] > gettime()) return SendClientMessage(playerid, COLOR_LIGHTRED, "У вас бан чата!");
    if(takephone[playerid] == 1) return SendClientMessage(playerid, COLOR_LIGHTRED, "У Вас нет телефона");
    if(strlen(params[1]) > 95) return SendClientMessage(playerid,COLOR_GREY,"Сообщение слишком длинное!");
    if(IsAIP(params[1])) {
        SendClientMessage(playerid, -1, "Подозрения в рекламе!");
        return 0;
    }
    switch (params[0])
    {
    case 11888:
        {
            if(smsls == 0) return SendClientMessage(playerid, COLOR_GREY, "Приём SMS отключен");
            if(PI[playerid][pBank] <= 0)
            {
                format(stringer1, sizeof(stringer1), "[Исходящее SMS]: %s | Получатель: Россия 24",params[1]);
                SendClientMessage(playerid,  COLOR_YELLOW, stringer1);
                SendClientMessage(playerid,  COLOR_GRAD1, "Сообщение не доставлено, не достаточно средств");
                format(stringer1, sizeof(stringer1), "%s достаёт мобильник", PI[playerid][pNames]);
                SetPlayerChatBubble(playerid,stringer1,COLOR_PURPLE,5.0,10000);
                return 1;
            }
            format(stringer1, sizeof(stringer1), "[Эфир] %s. Отправитель: %s[%i]",params[1],PI[playerid][pNames], playerid);
            SendFamilyMessage(16, COLOR_YELLOW, stringer1);
            format(stringer1, sizeof(stringer1), "[Исходящее SMS]: %s | Получатель: Россия 24",params[1]);
            SendClientMessage(playerid,  COLOR_YELLOW, stringer1);
            PI[playerid][pMobile] -= smspricels;
            FracBank[0][fLsnews] += smspricels;
        }
    }
    if (IsPlayerConnected(params[0]))
    {
        if(params[0] != INVALID_PLAYER_ID)
        {
            if(PhoneOnline[params[0]] > 0)
            {
                SendClientMessage(playerid, COLOR_GREY, "Телефон абонента выключен...");
                format(stringer1, sizeof(stringer1), "%s достаёт мобильник", PI[playerid][pNames]);
                SetPlayerChatBubble(playerid,stringer1,COLOR_PURPLE,5.0,10000);
                return 1;
            }
            if(PI[params[0]][AFK_Time] > 1) return SendClientMessage(playerid, COLOR_GREY, "Данный игрок AFK");
            PI[playerid][pBank] -=20;
            format(stringer1, sizeof(stringer1), "[Входящее SMS]: %s | Отправитель: %s[%d]",params[1],PI[playerid][pNames], playerid);
            SendClientMessage(params[0], COLOR_YELLOW, stringer1);
            SetPlayerChatBubble(params[0],"SMS <<<",COLOR_YELLOW,5.0,10000);
            foreach(Player,i)
            {
                if(BigEarSms[i] != 0)
                {
                    format(stringer1, sizeof(stringer1), "» Входящее к %s[%d]: %s. Отправитель: %s[%d].",PI[params[0]][pNames],params[0],params[1],PI[playerid][pNames],playerid);
                    SendClientMessage(i, COLOR_YELLOW, stringer1);
                }
            }
            PlayerPlaySound(params[0], 1052, 0.0, 0.0, 0.0);
            PlayerPlaySound(playerid, 1052, 0.0, 0.0, 0.0);
            format(stringer1, sizeof(stringer1), "[Исходящее SMS]: %s | Получатель: %s[%d]",params[1], PI[params[0]][pNames], params[0]);
            SendClientMessage(playerid,  COLOR_YELLOW, stringer1);
            SetPlayerChatBubble(playerid,"SMS >>>",COLOR_YELLOW,5.0,10000);
            return 1;
        }
    }
    return 1;
}
cmd:tazer(playerid, params[])
{
    if(PI[playerid][pMember] == 2 || PI[playerid][pLeader] == 2 || IsACop(playerid)) {
        if(sscanf(params, "d",params[0])) return SendClientMessage(playerid,COLOR_WHITE,"Введите: /tazer [1/2/3]");
        switch(params[0])
        {
        case 1:
            {
                format(stringer1, sizeof(stringer1), "Агент ФСБ %s оглушил всех",PI[playerid][pNames]);
                ProxDetector(playerid, 20.0, COLOR_PURPLE, stringer1);
                foreach(Player,i)
                {
                    new Float:X,Float:Y,Float:Z;
                    GetPlayerPos(playerid,X,Y,Z);
                    if(IsPlayerInRangeOfPoint(i,20,X,Y,Z) && i != playerid)
                    {
                        SendClientMessage(i,-1,"Вы заморожены на 15 секунд");
                        FIXES_ApplyAnimation(i,"PED","handsup",4.1,0,1,1,1,0,0);
                        PlayerCuffed[i] = 1;
                        PlayerCuffedTime[i] = 15;
                        SetTimerEx("UnFreez", 15000, 0, "d", i);
                    }
                }
            }
        case 2:
            {
                format(stringer1, sizeof(stringer1), "Агент ФСБ %s оглушил всех рядомстоящих законников",PI[playerid][pNames]);
                ProxDetector(playerid, 20.0, COLOR_PURPLE, stringer1);
                foreach(Player,i)
                {
                    new Float:X,Float:Y,Float:Z;
                    GetPlayerPos(playerid,X,Y,Z);
                    if(IsPlayerInRangeOfPoint(i,20,X,Y,Z) && i != playerid)
                    {
                        if(IsAArm(i) || IsACop(i) || IsAMedic(i))
                        {
                            SendClientMessage(i,-1,"Вы заморожены на 15 секунд");
                            FIXES_ApplyAnimation(i,"PED","handsup",4.1,0,1,1,1,0,0);
                            PlayerCuffed[i] = 1;
                            PlayerCuffedTime[i] = 15;
                            SetTimerEx("UnFreez", 15000, 0, "d", i);
                        }
                    }
                }
            }
        case 3:
            {
                format(stringer1, sizeof(stringer1), "Агент ФСБ %s оглушил всех рядомстоящих жителей", PI[playerid][pNames]);
                ProxDetector(playerid, 20.0, COLOR_PURPLE, stringer1);
                foreach(Player,i)
                {
                    new Float:X,Float:Y,Float:Z;
                    GetPlayerPos(playerid,X,Y,Z);
                    if(IsPlayerInRangeOfPoint(i,20,X,Y,Z) && i != playerid)
                    {
                        if(PI[i][pMember] == 0 || ISAMAFIA(i))
                        {
                            SendClientMessage(i,-1,"Вы заморожены на 15 секунд");
                            FIXES_ApplyAnimation(i,"PED","handsup",4.1,0,1,1,1,0,0);
                            PlayerCuffed[i] = 1;
                            PlayerCuffedTime[i] = 15;
                            SetTimerEx("UnFreez", 15000, 0, "d", i);
                        }
                    }
                }
            }
        }
    }
    return 1;
}

cmd:veha(playerid, params[])
{
    if(!FD(playerid)) return true;
    if(sscanf(params, "ddd",params[0],params[1],params[2])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /veh2 [id авто] [Цвет №1] [Цвет №2]");
    new intt = GetPlayerInterior(playerid);
    if(params[0] < 400 || params[0] > 611) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /veh2 [id авто] [Цвет №1] [Цвет №2]");
    if(params[1] < 0 || params[1] > 254) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /veh2 [id авто] [Цвет №1] [Цвет №2]");
    if(params[2] < 0 || params[2] > 254) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /veh2 [id авто] [Цвет №1] [Цвет №2]");
    new Float:X,Float:Y,Float:Z, string[144];
    GetPlayerPos(playerid, X,Y,Z);
    createdvehs ++ ;
    createdvehicles[createdvehs] = CreateVehicle(params[0], X,Y,Z, 0.0, params[1], params[2], 60000, 0, VEHICLE_ACTION_TYPE_ADMIN);
    vehcreat[createdvehicles[createdvehs]] = 1;
    PutPlayerInVeh(playerid, createdvehicles[createdvehs], 0);
    zavodis[playerid] = 1;
    LinkVehicleToInterior(createdvehicles[createdvehs], intt);
    format(string, sizeof(string), "[A] %s[%d] создал автомобиль '%s' (id %d)(color %d, %d)", PI[playerid][pNames], playerid, g_vehicle_info[params[0] - 400][VI_NAME], params[0], params[1], params[2]);
    SendAdminMessage(COLOR_GREY, string);
    format(stringer1, sizeof(stringer1), "Автомобиль ID: %d создан, для удаления /delveh", params[0]);
    SendClientMessage(playerid, 0x6ab1ffaa, stringer1);
    return 1;
}

cmd:veh(playerid)
{
    if(PI[playerid][pAdmin] < 5) return 1;
    {
        if(IsAGreenZone(playerid)) return SendClientMessage(playerid, COLOR_WHITE, !"Запрещено создавать машину в зеленой зоне!");

        new dialog_string[128];
        stringer[0] = EOS;

        for(new i = 0; i < 40; i ++)
        {
            format(dialog_string, sizeof(dialog_string), "{ffffff}%s\t{afafaf}[ID: %d]\n", g_vehicle_info[car_autosalon[i][0] - 400][VI_NAME], car_autosalon[i][0]);
            strcat(stringer, dialog_string);
        }

        return SPD(playerid, 2362, DIALOG_STYLE_TABLIST, !""SERVER_NAME" | Создание транспорта", stringer, !"Создать", !"Закрыть");
    }
}

cmd:acar(playerid, params[])
{
    if(PI[playerid][pYtADM] > 0)
    {
        if(IsAGreenZone(playerid)) return SendClientMessage(playerid, -1, "Запрещено создавать машину в зеленой зоне!");
        if(sscanf(params, "dd",params[0],params[1])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /acar [Цвет №1] [Цвет №2]");
        new intt = GetPlayerInterior(playerid);
        if(params[0] < 0 || params[0] > 254) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /acar [Цвет №1] [Цвет №2]");
        if(params[1] < 0 || params[1] > 254) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /acar [Цвет №1] [Цвет №2]");

        if(GetPVarInt(playerid, "Acar")) return 1;

        new Float:X,Float:Y,Float:Z;
        GetPlayerPos(playerid, X,Y,Z);
        createdvehs ++ ;
        createdvehicles[createdvehs] = CreateVehicle(415, X,Y,Z, 0.0, params[0], params[1], 60000, 0, VEHICLE_ACTION_TYPE_ADMIN);
        vehcreat[createdvehicles[createdvehs]] = 1;

        PutPlayerInVeh(playerid, createdvehicles[createdvehs], 0);
        SetPVarInt(playerid, "IdCar", createdvehs);
        SetPVarInt(playerid, "IdVeh", GetPlayerVehicleID(playerid));
        zavodis[playerid] = 1;
        SetVehicleData(createdvehicles[createdvehs], V_FUEL, 100);
        LinkVehicleToInterior(createdvehicles[createdvehs], intt);
        format(stringer1, sizeof(stringer1), "[A] %s[%d] создал админский Ferrari (Цвет 1: %d, Цвет 2: %d)", PI[playerid][pNames], playerid, params[0], params[1]);
        SendAdminMessage(COLOR_GREY, stringer1);
        SendClientMessage(playerid, 0x6ab1ffaa, !"Автомобиль создан. Для удаления: /delveh.");
    }
    else
    {
    	if(PI[playerid][pAdmin] < 2) return 1;
        if(IsAGreenZone(playerid)) return SendClientMessage(playerid, -1, "Запрещено создавать машину в зеленой зоне!");
        if(sscanf(params, "dd",params[0],params[1])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /acar [Цвет №1] [Цвет №2]");
        new intt = GetPlayerInterior(playerid);
        if(params[0] < 0 || params[0] > 254) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /acar [Цвет №1] [Цвет №2]");
        if(params[1] < 0 || params[1] > 254) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /acar [Цвет №1] [Цвет №2]");

        if(GetPVarInt(playerid, "Acar")) return 1;

        new Float:X,Float:Y,Float:Z;
        GetPlayerPos(playerid, X,Y,Z);
        createdvehs ++ ;
        createdvehicles[createdvehs] = CreateVehicle(415, X,Y,Z, 0.0, params[0], params[1], 60000, 0, VEHICLE_ACTION_TYPE_ADMIN);
        vehcreat[createdvehicles[createdvehs]] = 1;

        PutPlayerInVeh(playerid, createdvehicles[createdvehs], 0);
        SetPVarInt(playerid, "IdCar", createdvehs);
        SetPVarInt(playerid, "IdVeh", GetPlayerVehicleID(playerid));
        zavodis[playerid] = 1;
        SetVehicleData(createdvehicles[createdvehs], V_FUEL, 100);
        LinkVehicleToInterior(createdvehicles[createdvehs], intt);
        format(stringer1, sizeof(stringer1), "[A] %s[%d] создал админский Ferrari (Цвет 1: %d, Цвет 2: %d)", PI[playerid][pNames], playerid, params[0], params[1]);
        SendAdminMessage(COLOR_GREY, stringer1);
        SendClientMessage(playerid, 0x6ab1ffaa, !"Автомобиль создан. Для удаления: /delveh.");
    }
    return 1;
}
cmd:delveh(playerid, params[])
{
    if(PI[playerid][pYtADM])
    {
        if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return 1;
        if(GetVehicleData(GetPlayerVehicleID(playerid), V_ACTION_TYPE) == VEHICLE_ACTION_TYPE_ADMIN)
        {
            RemoveFromVehicle(playerid);
            n_veh_DestroyVehicle(GetPlayerVehicleID(playerid));
        }
        return 1;
    }
    else if(PI[playerid][pAdmin] >= 8)
    {
        if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return 1;
        if(GetVehicleData(GetPlayerVehicleID(playerid), V_ACTION_TYPE) == VEHICLE_ACTION_TYPE_ADMIN)
        {
            RemoveFromVehicle(playerid);
            n_veh_DestroyVehicle(GetPlayerVehicleID(playerid));
        }
        return 1;
    }
    return 1;
}
cmd:dvall(playerid)
{
    if(PI[playerid][pAdmin] < 8) return 1;
    new total_car = 0, string[144];
    for(new i = 0; i < MAX_VEHICLES + 1; i++)
    {
        if(GetVehicleData(i, V_ACTION_TYPE) == VEHICLE_ACTION_TYPE_ADMIN)
        {
            n_veh_DestroyVehicle(i);
            total_car ++;
        }

    }
    createdvehs = 0;
    format(string, sizeof(string), "{FF4500}[SYSTEM] {FFFFFF}Вы удалили {00ff00}%i {FFFFFF}автомобилей созданных на сервере!", total_car);
    SendClientMessage(playerid, -1, string);
    format(string, sizeof(string), "[A] %s[%d] удалил весь созданный транспорт на сервере. (%i шт)",PI[playerid][pNames], playerid, total_car);
    SendAdminMessage(COLOR_GREY, string);
    return 1;
}
cmd:obj(playerid)
{
    if(FD(playerid)) return SPD(playerid, 6565, DIALOG_STYLE_LIST, "Редактор","[1] Создать объект\n[2] Редактировать объекты","Выбрать","Отмена");
    else return 1;
}
cmd:duty(playerid, params[])
{
    if(PI[playerid][pMember] != 11) return SendClientMessage(playerid, COLOR_GRAD1, "Вы не состоите в автошколе!");
    if(!IsPlayerInRangeOfPoint(playerid,50.0,-781.4494,1546.2416,1139.2369)) return SendClientMessage(playerid, COLOR_GRAD1, "Вы не в автошколе!");
    if(GetPVarInt(playerid,"DutyAsh") == 0) format(stringer1, sizeof(stringer1), "Инструктор %s приступил(а) к работе",PI[playerid][pNames]), SetPVarInt(playerid,"DutyAsh",1);
    else format(stringer1, sizeof(stringer1), "Инструктор %s завершил(а) рабочий день",PI[playerid][pNames]), SetPVarInt(playerid,"DutyAsh",0);
    ProxDetector(playerid, 25.0, COLOR_PURPLE, stringer1);
    return 1;
}
cmd:mutelist(playerid,params[])
{
    if(PI[playerid][pAdmin] == 0) return 1;
    new countmute = 0;
    foreach(Player,i)
    {
        if(PI[i][pMuteTime] > gettime() && IsPlayerConnected(i))
        {
            format(stringer1, sizeof(stringer1), "%s [ID: %d] | %d секунд", PI[i][pNames], i,PI[i][pMuteTime]-gettime());
            countmute++;
            SendClientMessage(playerid, COLOR_LIGHTGREEN, stringer1);
        }
    }
    if(countmute == 0) SendClientMessage(playerid, COLOR_GRAD1, "Нет игроков с баном чата");
    else
    {
        format(stringer1, sizeof(stringer1), "Всего: %d человек!", countmute);
        SendClientMessage(playerid, COLOR_ISPOLZUY, stringer1);
    }
    return 1;
}
cmd:warnmans(playerid,params[])
{
    if(PI[playerid][pAdmin] < 5) return 1;
    new countmute = 0;
    foreach(Player,i)
    {
        if(PI[i][pWarns] == 0) continue;
        format(stringer1,sizeof(stringer1),"{FFA500}%s[%i]. {FFFFFF}Warns: {FFA500}%i.{FFFFFF} До снятия:{FFA500} %i часов",PI[i][pNames],i, PI[i][pWarns],10 - PI[i][punWarnstime]);
        SendClientMessage(playerid, COLOR_LIGHTGREEN, stringer1);
        countmute++;
    }
    if(countmute == 0) SendClientMessage(playerid, COLOR_GRAD1, "Нет игроков с варном!");
    else
    {
        format(stringer1, sizeof(stringer1), "Всего: %d человек!", countmute);
        SendClientMessage(playerid, COLOR_ISPOLZUY, stringer1);
    }
    return 1;
}
cmd:jaillist(playerid, params[])
{
    if(!PI[playerid][pAdmin]) return 1;
    new countmute;
    foreach(Player,i) {
        if(PI[i][pJailTime] < gettime()) continue;
        format(stringer1,sizeof(stringer1),"{FFA500}%s[%i]. {FFFFFF}Время заключение: {FFA500}%i.",PI[i][pNames], i, PI[i][pJailTime]-gettime());
        SendClientMessage(playerid, COLOR_LIGHTGREEN, stringer1);
        countmute++;
    }
    if(countmute == 0) SendClientMessage(playerid, COLOR_GRAD1, "Заключенных не найдено!");
    else SendMes(playerid, COLOR_ISPOLZUY, "Всего: %d человек!",countmute);
    return 1;
}
cmd:knocklist(playerid,params[])
{
    if(PI[playerid][pAdmin] <= 1) return 1;
    foreach(Player,i)
    {
        if(PlayerTied[playerid] > 0 && IsPlayerConnected(i))
        {
            format(stringer1, sizeof(stringer1), "%s [ID: %d]", PI[i][pNames], i);
            SendClientMessage(playerid, COLOR_LIGHTGREEN, stringer1);
        }
    }
    return 1;
}
cmd:kiss(playerid,params[])
{
    params[0] = GetClosestPlayer(playerid);
    if(GetPlayerInterior(playerid) != 0) return 1;
    if(PI[playerid][pJailTime] > 0 || PI[params[0]][pJailTime] > 0) return SendClientMessage(playerid, COLOR_GREY, "Нельзя использовать эту команду в тюрьме.");
    if(GetPlayerState(playerid) != PLAYER_STATE_ONFOOT || GetPlayerState(params[0]) != PLAYER_STATE_ONFOOT) return SendClientMessage(playerid, COLOR_GREY, "Невозможно совершить действие!");
    if(PI[params[0]][pSex] == PI[playerid][pSex]) return SendClientMessage(playerid, COLOR_GREY, "Вы не можете его поцеловать.");
    if(!ProxDetectorS(4.0, playerid, params[0])) return SendClientMessage(playerid, COLOR_GREY, "Рядом никого нет!");
    FIXES_ApplyAnimation(params[0],"BD_FIRE","GRLFRD_KISS_03",4.0,0,0,0,0,0,1);
    FIXES_ApplyAnimation(playerid,"BD_FIRE","PLAYA_KISS_03",4.0,0,0,0,0,0,1);
    return 1;
}
cmd:minet(playerid, params[])
{
    if(sscanf(params, "d",params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /minet [id]");
    if(!IsPlayerConnected(params[0]) || playerid == params[0]) return 1;
    if(PI[playerid][pJailed] == 1) return SendClientMessage(playerid, COLOR_WHITE, "Нельзя использовать в тюрьме!");
    if(!ProxDetectorS(4.0, playerid, params[0])) return 1;
    if(GetPVarInt(playerid,"MinetTime") > gettime()) return SendClientMessage(playerid, -1, "Можно использовать раз в 30 секунд.");
    SetPVarInt(playerid,"MinetTime",gettime()+30);
    FIXES_ApplyAnimation(params[0],"BLOWJOBZ","BJ_COUCH_END_P",4.1,0,1,1,1,5,1);
    FIXES_ApplyAnimation(playerid,"BLOWJOBZ","BJ_COUCH_END_W",4.1,0,1,1,1,5,1);
    SendMes(playerid,COLOR_GREEN,"Вы сделал минет %s :)",PI[params[0]][pNames]);
    SendMes(params[0],COLOR_INDIGO,"Вам сделал минет %s :)",PI[playerid][pNames]);
    return 1;
}
cmd:iznas(playerid, params[])
{
    if(sscanf(params, "d",params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /iznas [ID]");
    if(!IsPlayerConnected(params[0]) || playerid == params[0]) return 1;
    if(PI[playerid][pJailed] > 0) return SendClientMessage(playerid, COLOR_WHITE, "Нельзя использовать в тюрьме!");
    if(GetDistanceBetweenPlayers(playerid, params[0]) < 2)
    {
        if(GetPVarInt(playerid,"IznasTime") > gettime()) return SendClientMessage(playerid, -1, "Можно использовать раз в 30 секунд.");
        SetPVarInt(playerid,"IznasTime",gettime()+30);
        SendMes(playerid,0xFF0000AA,"Вы изнасиловали игрока %s",PI[params[0]][pNames]);
        SendMes(params[0],0x00FF00AA,"Вас изнасиловали %s",PI[playerid][pNames]);
        FIXES_ApplyAnimation(params[0],"SNM","SPANKINGW",4.1,0,1,1,1,1);
        FIXES_ApplyAnimation(playerid,"SNM","SPANKEDW",4.1,0,1,1,1,1);
    }
    else SendClientMessage(playerid,COLOR_WHITE,"Вы слишком далеко!");
    return 1;
}
cmd:myskill(playerid,params[])
{
    format(stringer, sizeof(stringer), "{FFFFFF}SDPistol:{32CD32}.................{FFFFFF}%d процентов.\nDesertEagle:{32CD32}..............{FFFFFF}%d процентов.\nShotGun:{32CD32}..................{FFFFFF}%d процентов.\nMP5:{32CD32}......................{FFFFFF}%d процентов.\nAK47:{32CD32}.....................{FFFFFF}%d процентов.\nM4:{32CD32}.......................{FFFFFF}%d процентов.",
    PI[playerid][pSDPistol],PI[playerid][pDesertEagle],PI[playerid][pShotGun],PI[playerid][pMP5],PI[playerid][pAK47],PI[playerid][pM4]);
    if(sscanf(params, "d",params[0]))
    {
        SPD(playerid,000,DIALOG_STYLE_MSGBOX,"Skill",stringer,"Выход","");
        format(stringer1, 90, "%s просматривает свои навыки владения оружием",PI[playerid][pNames]);
        ProxDetector(playerid, 25.0, COLOR_PURPLE, stringer1);
        return 1;
    }
    if(!ProxDetectorS(8.0, playerid, params[0])) return SendClientMessage(playerid,COLOR_GREY,"Игрок не рядом с вами");
    format(stringer1, 90, "Skills %s",PI[playerid][pNames]);
    SPD(params[0],000,DIALOG_STYLE_MSGBOX,stringer1,stringer,"Выход","");
    format(stringer1, 90, "%s показывает свои навыки владения оружием %s",PI[playerid][pNames],PI[params[0]][pNames]);
    ProxDetector(playerid, 25.0, COLOR_PURPLE, stringer1);
    return 1;
}
