cmd:uval(playerid, params[])
{
    if(PI[playerid][pAdmin] < 6) return 1;
    if(sscanf(params, "ds[128]",params[0],params[1])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /uval [id игрока] [причина]");
    if(!IsPlayerConnected(params[0])) return 1;
    if(PI[params[0]][pMember] == 0) return 1;
    if(IsPlayerStartCroupier(params[0]))
    {
        if(CroupierTable[params[0]] != -1)
        {
            switch(CroupierGame[params[0]])
            {
            case 1: OnPlayerExitDiceTable(params[0], CroupierTable[params[0]]);
            case 2: OnPlayerExitRouleTable(params[0], CroupierTable[params[0]]);
            case 3: OnPlayerExitBlackJackTable(params[0], CroupierTable[params[0]]);
            }
        }
        StartDuty[params[0]] = 0;
    }
    format(stringer1, sizeof(stringer1), "уволил из организации %s, с причиной: %s",GetPlayerFracName[PI[params[0]][pMember]],params[1]);
    Admin_Log(playerid, stringer1, PI[params[0]][pNames], gettime());
    SendMes(params[0], 0xff0000ff, "Администратор %s уволил Вас из организации. Причина: %s", PI[playerid][pNames], params[1]);
    SendMes(playerid, 0x6ab1ffaa, "Вы уволили %s из организации.", PI[params[0]][pNames]);
    SendClientMessage(params[0], COLOR_WHITE, "Теперь вы обычный гражданин...");
    SetPlayerArmour(params[0],0);
    ResetWeapons(params[0]);
    PI[params[0]][pMember] = 0;
    PI[params[0]][pLeader] = 0;
    PI[params[0]][pRank] = 0;
    SetPlayerColor(params[0], 0xFFFFFF80);
    SetPlayerSkinEx(params[0], PI[params[0]][pChar]);
    format(stringer1, sizeof(stringer1), "[A] Администратор %s[%d] уволил из организации %s[%d]. Причина: %s", PI[playerid][pNames], playerid,PI[params[0]][pNames], params[0], params[1]);
    SendAdminMessage(COLOR_GREY,stringer1);
    PI[params[0]][pOdet] = 0;
    if(!PI[params[0]][pAdmin]) PI[params[0]][pVigs] = 0;
    format(stringer, sizeof(stringer), "UPDATE `"TABLE_ACCOUNT"` SET `pMember` = '0', `pRank` = '0', `pLeader` = '0', `odet` = '0', `vigs` = '%d' WHERE `pID` = '%d'",PI[params[0]][pVigs],PI[params[0]][pID]);
    mysql_function_query(connects, stringer, false, "", "");
    return 1;
}
cmd:trailer(playerid, params[])
{
    if(PI[playerid][pMember] != 1 && PI[playerid][pJob] != 2) return SendClientMessage(playerid, COLOR_GREY, "Вам недоступна данная команда!");
    new car = GetPlayerVehicleID(playerid);
    if(GetVehicleModel(car) != 525 || GetPlayerState(playerid) != 2) return SendClientMessage(playerid, COLOR_GREY, "Вам нужно быть в эвакуаторе!");
    new Float:pX,Float:pY,Float:pZ;
    new Float:vX,Float:vY,Float:vZ, veh;
    GetPlayerPos(playerid,pX,pY,pZ);
    for(new i = 1; i < MAX_VEHICLES; i++)
    {
        GetVehiclePos(i,vX,vY,vZ);
        if ((floatabs(pX-vX)<7.0)&&(floatabs(pY-vY)<7.0)&&(floatabs(pZ-vZ)<7.0)&&(i != car)) {
            veh ++;
            if(IsTrailerAttachedToVehicle(car)) DetachTrailerFromVehicle(car);
            else AttachTrailerToVehicle(i, car);
            break;
        }
    }
    if(!veh) SendClientMessage(playerid,0xFFFF00AA,"Рядом нет автомобиля");
    return 1;
}
cmd:aprf(playerid, params[])
{
    new str[256],sctring[156];
    if(PI[playerid][pAdmin] >= 1) format(str,sizeof(str),"{ffffff}PREFIX-INFORM:\n"); strcat(sctring,str);
    if(PI[playerid][pAdmin] >= 1) format(str,sizeof(str),"1. Использование команды /SETPREFIX [ID] {FF6600}(Сразу после ID ставьте цифру '1'){ffffff} \n"); strcat(sctring,str);
    if(PI[playerid][pAdmin] >= 1) format(str,sizeof(str),"2. Чтобы сделать префикс цветным смотрите цвета в формате {FFFFFF} {FF6600}(colorscheme.ru){ffffff} \n"); strcat(sctring,str);
    if(PI[playerid][pAdmin] >= 1) format(str,sizeof(str),"3. Цветной префикс разрешено устанавливать только за деньги / старшим админам  \n"); strcat(sctring,str);
    SPD(playerid,0000,DIALOG_STYLE_MSGBOX,"{65c8ff}PRFX-INFO:",sctring,"Принять","");
    return 1;
}

cmd:prefix(playerid, params[])
{
    if(PI[playerid][pAdmin] < 5) return 1;
    if(sscanf(params, "ds[32]", params[0], params[1])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /prefix [ID] [Префикс]");
    if(!IsPlayerConnected(params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Человек не найден!");
    if(PI[params[0]][pAdmin] < 1 && dostup[playerid] < 1) return SendClientMessage(playerid, COLOR_WHITE, "Игрок не администратор или он не залогинен!");
    new string[144], query[144];
    sscanf(params[1], "s[32]", PI[params[0]][pPrefix]);
    format(query, sizeof(query), "UPDATE `accounts` SET `pPrefix` = '%s' WHERE `pID` = '%d'", PI[params[0]][pPrefix], PI[params[0]][pID]);