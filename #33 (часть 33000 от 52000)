cmd:licenses(playerid,params[])
{
    new text1[20],text4[20],text5[20],text6[20],text7[20];
    if(PI[playerid][pVodPrava]) { text1 = "Есть"; } else { text1 = "Отсутствует"; }
    if(PI[playerid][pFlyLic]) { text4 = "Есть"; } else { text4 = "Отсутствует"; }
    if(PI[playerid][pBoatLic]) { text5 = "Есть"; } else { text5 = "Отсутствует"; }
    if(PI[playerid][pFishLic]) { text6 = "Есть"; } else { text6 = "Отсутствует"; }
    if(PI[playerid][pGunLic]) { text7 = "Есть"; } else { text7 = "Отсутствует"; }
    if(sscanf(params, "d",params[0]))
    {
        SendClientMessage(playerid,0x0AA8DAFF,"-------======[ LICENSES ]======-------");
        SendMes(playerid, 0xC0E1EEFF, "Водительские права: %s",text1);
        SendMes(playerid, 0xC0E1EEFF, "Лицензия на воздушный транспорт: %s",text4);
        SendMes(playerid, 0xC0E1EEFF, "Лицензия на водный транспорт: %s",text5);
        SendMes(playerid, 0xC0E1EEFF, "Лицензия рыболова: %s",text6);
        SendMes(playerid, 0xC0E1EEFF, "Лицензия на оружие: %s",text7);
        SendClientMessage(playerid,0x0AA8DAFF,"===============================");
        format(stringer1,sizeof(stringer1), "%s смотрит свои лицензии",PI[playerid][pNames]);
        ProxDetector(playerid, 30.0, COLOR_PURPLE, stringer1);
        return 1;
    }
    if(!ProxDetectorS(8.0, playerid, params[0])) return SendClientMessage(playerid,COLOR_GREY,"Игрок не рядом с вами");
    SendMes(params[0], 0xC0E1EEFF, "-------======[ LICENSES %s ]======-------",PI[playerid][pNames]);
    SendMes(params[0], 0xC0E1EEFF, "Водительские права: %s",text1);
    SendMes(params[0], 0xC0E1EEFF, "Лицензия на воздушный транспорт: %s",text4);
    SendMes(params[0], 0xC0E1EEFF, "Лицензия на водный транспорт: %s",text5);
    SendMes(params[0], 0xC0E1EEFF, "Лицензия рыболова: %s",text6);
    SendMes(params[0], 0xC0E1EEFF, "Лицензия на оружие: %s",text7);
    SendClientMessage(params[0],0x0AA8DAFF,"===============================");
    format(stringer1,sizeof(stringer1), "%s достал свои лицензии и показал %s'у",PI[playerid][pNames],PI[params[0]][pNames]);
    ProxDetector(playerid, 30.0, COLOR_PURPLE, stringer1);
    format(stringer1,sizeof(stringer1), "показал свои лицензии %s'у",PI[params[0]][pNames]);
    SetPlayerChatBubble(playerid,stringer1,COLOR_PURPLE,30.0,10000);
    ShowPlayerDialog(playerid, 10201, DIALOG_STYLE_LIST, "{FF0000}"SERVER_NAME" {FFFFFF}| Лицензии", "Закрыть", "");
    return 1;
}

cmd:business(playerid)
{
    if(GetPlayerBizz(playerid) == 0) return SendClientMessage(playerid, COLOR_GREY,"Вы не владеете бизнесом!");
    new biz = GetPVarInt(playerid, "PlayerBizz"), string[600], mafiatext[20];
    if(BizzInfo[biz][bMafia] == 6) { mafiatext = "Тамбовская ОПГ"; }
    else if(BizzInfo[biz][bMafia] == 14) { mafiatext = "Ореховская ОПГ"; }
    else if(BizzInfo[biz][bMafia] == 5) { mafiatext = "Курганская ОПГ"; }
    else if(BizzInfo[biz][bMafia] == 0) { mafiatext = "Нет"; }

    new days = floatround((BizzInfo[biz][bMoney]/100)/24);
    if(days > 30) days = 30;

    if(BizzInfo[biz][bType] == 2) {
        format(string, sizeof(string), "{FFFFFF}Название:\t\t\t\t{dbba25}Автозаправочная станция\n{FFFFFF}Номер бизнеса:\t\t\t%d\nВладелец:\t\t\t\t%s\n\nБаланс предприятия:\t\t\t%d$\nБизнес арендован на: \t\t\t%d дней\nСтоимость:\t\t\t\t%d$\nКонтроль:\t\t\t\t%s\nПлата за вход:\t\t\t\t%d\nСтатус:\t\t\t\t\t%s\n\n{35d408}Для открытия панели управления вашим бизнесом\n{35d408}нажмите 'Изменить'",
        biz, BizzInfo[biz][bOwner], BizzInfo[biz][bMoney], days, BizzInfo[biz][bPrice], mafiatext, BizzInfo[biz][bEnter], (!BizzInfo[biz][bLock])?("{239304}Открыт") : ("{df3f12}Закрыт"));
    } else {
        format(string, sizeof(string), "{FFFFFF}Название:\t\t\t\t{dbba25}%s\n{FFFFFF}Номер бизнеса:\t\t\t%d\nВладелец:\t\t\t\t%s\n\nБаланс предприятия:\t\t\t%d$\nБизнес арендован на: \t\t\t%d дней\nСтоимость:\t\t\t\t%d$\nКонтроль:\t\t\t\t%s\nПлата за вход:\t\t\t\t%d\nСтатус:\t\t\t\t\t%s\n\n{35d408}Для открытия панели управления вашим бизнесом\n{35d408}нажмите 'Изменить'",
        BizzInfo[biz][bName], biz, BizzInfo[biz][bOwner], BizzInfo[biz][bMoney], floatround((BizzInfo[biz][bMoney]/100)/24), BizzInfo[biz][bPrice], mafiatext, BizzInfo[biz][bEnter], (!BizzInfo[biz][bLock])?("{239304}Открыт") : ("{df3f12}Закрыт"));
    }
    ShowPlayerDialog(playerid, 9199, DIALOG_STYLE_MSGBOX, "{dba002}Информация о бизнесе", string, "Изменить", "Закрыть");
    return 1;
}

cmd:buybiz(playerid, params[])
{
    if(GetPlayerBizz(playerid) > 0) return SendClientMessage(playerid, COLOR_GREY,"Вы уже владеете бизнесом!");
   // if(PI[playerid][pAdmin] > 0) return SendClientMessage(playerid, COLOR_GREY, "Администратор не может иметь бизнес!");
    new full = 0, full_ = 0, str[128];
    for(new idx = 1; idx <= TOTALBIZZ; idx++)
    {
        if(IsPlayerInRangeOfPoint(playerid, 5.0, BizzInfo[idx][bEntrx],BizzInfo[idx][bEntry],BizzInfo[idx][bEntrz]))
        {
            full++;
            if(!strcmp(BizzInfo[idx][bOwner],"None",true))
            {
                full_++;
                if(PI[playerid][pBank] < BizzInfo[idx][bPrice]) return SendClientMessage(playerid, COLOR_GREY,"У вас недостаточно средств на банковском счету!");
                else
                {
                    SetPVarInt(playerid,"PlayerBizz",idx);
                    ClearString();
                    format(str,128,"{FFFFFF}Название:\t\t"SERVER"%s\n",BizzInfo[idx][bName]);
                    strcat(stringer,str);
                    format(str,128,"{FFFFFF}Стоимость:\t\t{3399FF}%s\n\n",formatInt(BizzInfo[idx][bPrice]));
                    strcat(stringer,str);
                    strcat(stringer,"{00cb00}Вы уверены что хотите купить этот бизнес?");
                    ShowPlayerDialog(playerid, 9152, 0, "{63CB00}Покупка нового бизнеса", stringer, "Да", "Нет");
                    break;
                }
            }
        }
    }
    if(full == 0) return SendClientMessage(playerid, COLOR_GREY,"Вы должны находится рядом с бизнесом!");
    else if(full_ == 0) return SendClientMessage(playerid, COLOR_GREY,"Бизнес уже имеет владельца!");
    return 1;
}
cmd:leave(playerid, params[])
{
    if(PI[playerid][pDonateRank] != 3) return SendClientMessage(playerid, -1, "Вы не имеете Gold VIP на своём аккаунте!");
    if(PI[playerid][pCash] < 200000) return SendClientMessage(playerid, 0x6495EDFF, "Вам необходимо 200.000 руб. для увольнения");
    if(PI[playerid][pMember]== 0) return SendClientMessage(playerid,COLOR_WHITE,"Вы не состоите в организации!");
    if(PI[playerid][pLeader] > 0) return SendClientMessage(playerid, COLOR_BLUE, "Вы руководитель фракции,поэтому уволится не получится!");
    SPD(playerid,1119, DIALOG_STYLE_MSGBOX, "Увольнение","Вы действительно хотите покинуть данную организацию за 1.000.000$?", "Да", "Нет");
    return 1;
}

cmd:fuelcars(playerid, params[])
{
    if(PI[playerid][pAdmin] < 7) return 1;
    for(new i = 1; i < MAX_VEHICLES; i++)
    {
        SetVehicleData(i, V_FUEL, 100);
    }
    format(stringer1,sizeof(stringer1),"Администратор %s заправил весь транспорт!",PI[playerid][pNames]);
    SendClientMessageToAll(COLOR_LIGHTRED,stringer1);
    return 1;
}
cmd:spawncars(playerid, params[])
{
    if(PI[playerid][pAdmin] < 8) return 1;

    new vehicles_count;

    for(new i = 1; i < MAX_VEHICLES; i++)
    {
        if(!IsValidVehicle(i)) continue;
        else if(IsVehicleOccupied(i)) continue;
        else if(IsTrailerAttachedToAnyVehicle(i)) continue;

        SetVehicleToRespawn(i);
        vehicles_count++;
    }

    new fmt_text[90];

    format(fmt_text, sizeof fmt_text, "Вы зареспавнили весь незанятый транспорт. Всего зареспавнено: %d шт.", vehicles_count);
    SendClientMessage(playerid, 0x1E90FFFF, fmt_text);

    format(fmt_text,sizeof(fmt_text),"Администратор %s зареспавнил незанятый людьми транспорт!",PI[playerid][pNames]);
    SendAdminMessage(0xCECECEFF, fmt_text);
    SendClientMessageToAll(COLOR_LIGHTRED,fmt_text);


    return 1;
}
cmd:spveh(playerid, params[])
{
    if(PI[playerid][pAdmin] < 8) return 1;
    if(sscanf(params, "d",params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /spveh [радиус]");
    new Float:pos = params[0],Float:car_x,Float:car_y,Float:car_z;
    for(new i = 1; i < MAX_VEHICLES; i++)
    {
        GetVehiclePos(i,car_x,car_y,car_z);
        if(IsPlayerInRangeOfPoint(playerid,pos,car_x,car_y,car_z)) SetVehicleToRespawn(i);
    }
    format(stringer1, sizeof(stringer1), "[A] %s[%d] зареспавнил автомобили в радиусе %d метров",PI[playerid][pNames], playerid, params[0]);
    SendAdminMessage(COLOR_GREY,stringer1);
    return 1;
}
