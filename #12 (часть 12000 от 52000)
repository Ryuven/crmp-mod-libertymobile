                format(stringer,144,"Вы купили дом у %s'a. Цена: %s", PI[playerd][pNames], formatInt(price)), SendClientMessage(playerid,0x6495EDFF, stringer);
                format(stringer, 144, "%s достал документы на дом и передал в руки %s",PI[playerd][pNames], PI[playerid][pNames]);
                ProxDetector(playerid, 30.0, COLOR_PURPLE, stringer);

                UpdatePlayerData(playerid, "pPhousekey", PI[playerid][pPhousekey]);
                UpdatePlayerData(playerd, "pPhousekey", PI[playerd][pPhousekey]);
            }
            else format(stringer,128,"%s отклонил предложение купить у вас дом", PI[playerid][pNames]), SendClientMessage(playerd,0x6495EDFF, stringer);
            SELL[1][playerd] = INVALID_PLAYER_ID;
            DeletePVar(playerid, "HHouse");
            DeletePVar(playerid, "HHouseMoney");
            return 1;
