                format(mysql_query_, sizeof(mysql_query_), "UPDATE `"TABLE_HOUSE"` SET `hOwner` = '%s' WHERE `hID` = '%i'",PI[playerid][pNames],PI[playerid][pPhousekey]);
                mysql_function_query(connects, mysql_query_, false, "", "");

