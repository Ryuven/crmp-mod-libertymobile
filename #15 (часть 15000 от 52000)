                {
                case 0: day = 2, max_day = 4;
                case 1:
                    {
                        if(PI[playerid][total_hour] < 10) return SCMF(playerid, -1, ""error" Вам необходимо отыграть 10 часов что бы пополнить счёт дома на 7 дней! (%d/10)",PI[playerid][total_hour]);
                        day = 7, max_day = 14;
                    }
                case 2:
                    {
                        if(PI[playerid][total_hour] < 30) return SCMF(playerid, -1, ""error" Вам необходимо отыграть 30 часов что бы пополнить счёт дома на 30 дней! (%d/30)",PI[playerid][total_hour]);
                        day = 30, max_day = 30;
                    }
                }
                new takking = day*plata;
                if (PI[playerid][pBank] < takking) return SendMes(playerid, COLOR_GREY, "Для пополнения счёта необходимо %d",takking), SPD(playerid, 10010, DIALOG_STYLE_MSGBOX, ""SERVER"Банкомат", "На вашем счету недостаточно денег!", "Назад", "");
                else
                {
                    if(HouseInfo[bouse][hTakings] >= max_day*plata) return SCMF(playerid, -1, ""error" Вам нельзя больше %d дней!",max_day);
                    PI[playerid][pBank] -= takking,HouseInfo[bouse][hTakings] += takking;
                    SendMes(playerid,0x6495EDFF, "Вы положили на счёт Вашей квартиры %d руб.. Квартирный счёт: %d руб.", takking, HouseInfo[bouse][hTakings]);
                    SaveHouse(bouse);
                    return 1;
                }
            }
            else return 1;
        }
