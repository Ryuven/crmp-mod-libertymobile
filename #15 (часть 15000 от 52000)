    case 5033:
        {
            new house = PI[playerid][pPhousekey];
            if(!response || house == 255) return 1;
            HouseInfo[house][hHel] = 0;
            HouseInfo[house][hLock] = 1;
            HouseInfo[house][hOwned] = 0;
            strmid(HouseInfo[house][hOwner], "The State", 0, strlen("The State"), 255);
            GiveMoney(playerid, HouseInfo[house][hValue]);
            format(stringer, sizeof(stringer), "Дом продан за %d руб.", HouseInfo[house][hValue]);
            SendClientMessage(playerid, COLOR_WHITE, stringer);
            foreach(Player,i)
            {
                if(PI[i][pHomez] == PI[playerid][pPhousekey])
                {
                    PI[i][pHomez] = 255;
                    SendClientMessage(i, COLOR_LIGHTRED, "Дом в котором вы были прописаны продан его владельцем.");
                    SendClientMessage(i, COLOR_LIGHTRED, "Вам была выдана компенсация от государства за потерю жилья. В размере - 1.000.000 руб.");
                    PI[playerid][pCash] += 1000000;
                }
            }
            mysql_format(connects, mysql_query_, sizeof(mysql_query_), "UPDATE `"TABLE_ACCOUNT"` SET `pHomez` = '255' WHERE `pHomez` = '%d'",house);
            mysql_function_query(connects, mysql_query_, false, "", "");
            BuyHouse(house);
            SaveHouse(house);
            HouseInfo[house][hRobhouse] = false;
            PI[playerid][pPhousekey] = 255;
            format(mysql_query_, sizeof(mysql_query_), "UPDATE `"TABLE_ACCOUNT"` SET `pPhousekey` = '255' WHERE `pID` = '%d'",PI[playerid][pID]);
            mysql_function_query(connects, mysql_query_, false, "", "");
            return 1;
        }
