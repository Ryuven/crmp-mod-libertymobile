stock SPD(playerid, dialogid, style, caption[], info[], button1[], button2[])
{
    ShowPlayerDialog(playerid, dialogid, style, caption, info, button1, button2);
    SetPVarInt(playerid, "USEDIALOGID", dialogid);
    return 1;
}

