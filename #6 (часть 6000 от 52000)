public OnPlayerConnect(playerid)
{

	ShoskiPon[playerid][pVodPravapon] = 0;

	Checkpoint[playerid]=0;
	
	pablochek[playerid]=0;

    moped_online[playerid] = INVALID_VEHICLE_ID;

    online_server++;
    if(online_server > ronline_server) ronline_server = online_server;

    SetPVarInt(playerid, "work_eda_house", 0);
    SetPVarInt(playerid, "work_eda", -1);
    SetPVarInt(playerid,"AutoShop",-1);

    PI[playerid] = NULL_PI;

    new buffer[40+1];
    gpci(playerid, buffer, sizeof(buffer));
    // printf("name %s gpci %s", PI[playerid][pNames], buffer);

    GetPlayerName(playerid, PI[playerid][pNames], 24);
    player_family [ playerid ] = 0;

    PI[playerid][pFamilyText] = Text3D:INVALID_3DTEXT_ID;

    if(!strcmp(PI[playerid][pNames], "Flame_Player"))
    {
        KickEx(playerid);
        return SendClientMessage(playerid, COLOR_WHITE, !"{afafaf}Смените никнейм в настройках лаунчера. (Пример: Ivan_Ivanov)");
    }

    PI[playerid][P_PLAYER_DEVICE] =
    PlayerReportFix[playerid] = 0;

    switch_fp[playerid] = false;


    SetPlayerData(playerid, P_OWNABLE_CAR, INVALID_VEHICLE_ID);

    gIsRequestClassCalled{playerid} = false;
    if(!strcmp(buffer, "44355A5834564C393054584655524D4F52584B41"))
    {
        SendClientMessage(0xffcc00ff, -1, "Пошел на хуй, читор ебанный");
        KickEx(playerid);
        return 1;
