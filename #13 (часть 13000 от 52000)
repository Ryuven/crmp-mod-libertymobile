    case 70:
        {
            if(!response) return 1;
            switch(listitem)
            {
            case 0:
                {
                    new i = PI[playerid][pLocal];
                    if(IsPlayerInRangeOfPoint(playerid, 3.0,HouseInfo[i][hExitx], HouseInfo[i][hExity], HouseInfo[i][hExitz]))
                    SetPlayerInterior(playerid,0);
                    SetPlayerVirtualWorld(playerid,0);
                    SetPPos(playerid,HouseInfo[i][hEntrancex],HouseInfo[i][hEntrancey],HouseInfo[i][hEntrancez]);
                    PI[playerid][pLocal] = 255;
                    SetPVarInt(playerid, "ExitHouse",1);
                }
            case 1:
                {
                    new house = PI[playerid][pPhousekey];
                    if(!IsPlayerInRangeOfPoint(playerid, 20.0,HouseInfo[house][hExitx],HouseInfo[house][hExity],HouseInfo[house][hExitz])) return SendClientMessage(playerid, COLOR_GRAD1,"Войти в гараж можно только в своем доме.");
                    SetPlayerInteriorEx(playerid, 0);
