            if(!ProxDetectorS(2.0, playerid, idd)) return   SendClientMessage(playerid, COLOR_GREY, "[Ошибка] Не рядом с вами!");
            SendMes(idd,0x6ab1ffaa,"%s забрал у вас лицензию на оружие", PI[playerid][pNames]);
            PI[idd][pGunLic] = 0;
            format(stringer, sizeof(stringer), "%s забрал(а) у %s лицензию на оружие", PI[playerid][pNames], PI[idd][pNames]);
            ProxDetector(playerid, 10.0, COLOR_PURPLE, stringer);
            SendMes(playerid,0x6ab1ffaa,"Вы забрали лицензию на оружие у %s.",PI[idd][pNames],idd);
            SPD(playerid, 7779, DIALOG_STYLE_LIST, "Отобрать лицензии:", "- Отобрать водительские права\n- Отобрать лицензию на полёты \n- Отобрать лицензию на рыболовлю\n- Отобрать лицензию на вождение водного транспорта\n- Отобрать лицензия на оружие\n- Наркотики\n- Оружие\n- Материалы", "Ок", "Отмена");
            return 1;
        }
