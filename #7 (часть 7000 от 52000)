        case dFamilyCreate:
        {
            if(!response) return 1;

            if(PI[playerid][pCash] < 2000000)
            {
                SendClientMessage(playerid, COLOR_WHITE, !"У вас недостаточно денег. Нужно 2.000.000 рублей.");
                return ShowPlayerDialog(playerid,dFamilyCreate,DIALOG_STYLE_INPUT,""SERVER"Создание семьи","{FFFFFF}Для того чтобы создать семью, Вам нужно оплатить {FFFFFF}2.000.000 рублей\n{FFFFFF}Если вы согласны с условиями, то\nВведите название своей семьи в поле ниже:\n",!"Создать",!"Отмена");
            }
            if ( strlen ( inputtext ) < 2 || strlen ( inputtext ) > 28 )
            {
                SendClientMessage(playerid,-1,"Название семьи не может быть больше 28 или меньше 2 сиволов!");
                return ShowPlayerDialog(playerid,dFamilyCreate,DIALOG_STYLE_INPUT,""SERVER"Создание семьи","{FFFFFF}Для того чтобы создать семью, Вам нужно оплатить {33aa33}2.000.000 рублей\n{FFFFFF}Если вы согласны с условиями, то\nВведите название своей семьи в поле ниже:\n",!"Создать",!"Отмена");
            }
            new temp [ 32 ],query_string [ 256 ];
            mysql_real_escape_string ( inputtext, temp ) ;
            format ( query_string, sizeof query_string, "INSERT INTO `family_system` ( `fam_name`, `fam_creator`, `fam_members_amount`, `fam_data`, `fam_house` ) VALUES ( '%s', '%s', '1', NOW( ), '-1')", temp, PI[ playerid ][pNames]) ;
            mysql_function_query ( connects, query_string, false, "", "" ) ;

            player_family [ playerid ] = family_count + 1;

            format(family_info [ family_count ] [ f_name ], 32 , temp);

            format(family_info [ family_count ] [ f_creator ], MAX_PLAYER_NAME, "%s", PI[playerid][pNames]);
            family_info [ family_count ] [ f_chat_color ] = 0;
            family_info [ family_count ] [ f_members_amount ] = 1;
            family_info [ family_count ] [ f_online ] = 1;
            family_info [ family_count ] [ f_house ] = -1;
