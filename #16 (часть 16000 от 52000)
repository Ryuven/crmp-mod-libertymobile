                for(new i = 0; i < sizeof(HouseInfo); i++)
                {
                    if(IsPlayerInRangeOfPoint(playerid, 3.0,HouseInfo[i][hEntrancex], HouseInfo[i][hEntrancey], HouseInfo[i][hEntrancez]))
                    {
                        if(strcmp(PI[playerid][pNames], HouseInfo[i][hOwner], true) == 0 && PI[playerid][pPhousekey] == 255) PI[playerid][pPhousekey] = i;
                        if(PI[playerid][pPhousekey] == i || HouseInfo[i][hLock] == 0 || PI[playerid][pHomez] == i || IsACop(playerid) || IsAArm(playerid) || PI[playerid][pAdmin] >= 3)
