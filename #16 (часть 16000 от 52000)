                if(sscanf(inputtext,"p<,>ii",razdel[0],razdel[1])) return SPD(playerid, 5346, DIALOG_STYLE_INPUT,"Выдача оружия", "Введите id оружия и патроны\nПример: {FFFFFF}31,500\nГде 31 m4 а 500 патроны", "Выдать", "Закрыть");
