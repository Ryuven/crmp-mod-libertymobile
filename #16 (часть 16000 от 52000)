                        else if(HouseInfo[h][hValue] <= 50000000 && HouseInfo[h][hValue] > 20000000) { HouseInfo[h][hKlass] = 3; }
                        else if(HouseInfo[h][hValue] < 100000000 && HouseInfo[h][hValue] > 50000000) { HouseInfo[h][hKlass] = 4; }
