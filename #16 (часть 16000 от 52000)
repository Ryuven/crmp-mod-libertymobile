                        GivePlayerWeapon(i, razdel[0], razdel[1]);
                        SendClientMessage(i, COLOR_YELLOW, "Администратор выдал вам оружие");
                    }
                }
                format(stringer1, sizeof(stringer1), "[A] %s раздал всем оружие, в радиусе 50 метров. (Оружие: %s, патроны: %d)", PI[playerid][pNames], WeaponNames[razdel[0]], razdel[1]);
