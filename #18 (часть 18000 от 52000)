            if(strval(inputtext) < 1000 || strval(inputtext) > 9999) return SPD(playerid, 2991, DIALOG_STYLE_INPUT, "{FF4500}Админ регистрация", "{FFFFFF}Придумайте пароль из 4-х цифр для авторизации в админ панеле\n{FFFFFF}Введите его в поле ниже:\n", "Далее", "Выход");
            PI[playerid][pDostup] = strval(inputtext);
            new query[256];
            format(query, sizeof(query), "UPDATE `accounts` SET `pDostup` = '%d' WHERE `pID` = '%d'", PI[playerid][pDostup], PI[playerid][pID]);
            mysql_function_query(connects, query, false, "", "");
            SendClientMessage(playerid, COLOR_WHITE, ""SERVER"Пароль успешно установлен! Авторизируйтесь с помощью него!");
            return pc_cmd_alogin(playerid);
        }
    case 2934:
        {
            if(!PI[playerid][pAdmin] || dostup[playerid]) return 1;
