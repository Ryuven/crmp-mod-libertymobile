                format(stringer, sizeof(stringer), "{CCFF00}[A] %s[%d] авторизовался (%d lvl) [United Arab Emirates / Dubai]",PI[playerid][pNames],playerid,PI[playerid][pAdmin]);
            }
            else  {
                format(stringer, sizeof(stringer), "{CCFF00}[A] %s[%d] авторизовался (%d lvl)",PI[playerid][pNames],playerid,PI[playerid][pAdmin]);
            }
            SendAdminMessage(0xCCFF00ff,stringer);
            if(PI[playerid][pVigs] > 0) SendMes(playerid, 0xff0000ff, "{FF6600}У Вас есть %i выговор(а), некоторые команды были заблокированы. Снимите выговор — {FFFFFF}/adonate",PI[playerid][pVigs]); //{FF6600}На вашем аккаунте выговор, некоторые команды были заблокированы.
            return 1;
        }
