cmd:skin(playerid, params[])
{
    if(PI[playerid][pAdmin] < 7)
    {
        if(!YouTube(playerid))
        return 1;
    }
    if(sscanf(params, "d", params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /skin [id скина]");
    if(params[0] > 289 || params[0] < 1) { SendClientMessage(playerid, COLOR_GREY, "Неправильный ID скина!"); return 1; }
    PI[playerid][pChar] = params[0];
    SetPlayerSkinEx(playerid, PI[playerid][pChar], true);
    return 1;
}

cmd:buyadm(playerid)
{
    new fstring[129],big_fmt[599];
