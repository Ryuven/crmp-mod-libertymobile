cmd:setskin(playerid, params[])
{
    if(PI[playerid][pAdmin] < 7) return 1;
    if(sscanf(params, "dd", params[0],params[1])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /setskin [id игрока/ник] [id скина]");
    if(params[1] > 289 || params[1] < 1) { SendClientMessage(playerid, COLOR_GREY, "Неправильный ID скина!"); return 1; }
    if(!IsPlayerConnected(params[0])) return 1;
    PI[params[0]][pChar] = params[1];
    format(stringer1, sizeof(stringer1), "Ваш скин был изменен администратором %s", PI[playerid][pNames]);
    SendClientMessage(params[0], 0x6495EDFF, stringer1);
    format(stringer1, sizeof(stringer1), "Вы изменили скин %s на номер: %i",PI[params[0]][pNames],params[1]);
    SendClientMessage(playerid, 0x6495EDFF, stringer1);
    SetPlayerSkinEx(params[0], PI[params[0]][pChar], true);
    return 1;
}
cmd:pay(playerid, params[])
{
    if(sscanf(params, "dd", params[0],params[1])) return    SendClientMessage(playerid, COLOR_WHITE, "Введите: /pay [id] [сумма]");
    if(params[1] < 1 || params[1] > 100000) return SendClientMessage(playerid, COLOR_GRAD1, "Нельзя передать меньше 1 руб.а и больше 99999 руб..");
    if(PI[playerid][pPayDayHad] < 3) return SendClientMessage(playerid, COLOR_GREY, "Вы должны пробыть в игре ещё 2 часа (/mm - Статистика персонажа).");
    if(!IsPlayerConnected(params[0])) return 1;
    if(!ProxDetectorS(5.0, playerid, params[0])) return SendClientMessage(playerid, COLOR_GRAD1, "Вы слишком далеко.");
    if(PI[playerid][pCash] < params[1]) return 1;
    GiveMoney(params[0], params[1]);
    PI[playerid][pCash] -= params[1];
    format(stringer1, sizeof(stringer1), "Вы передали %s[%i], %s руб..",PI[params[0]][pNames],params[0],formatInt(params[1]));
    PlayerPlaySound(playerid, 1052, 0.0, 0.0, 0.0);
    SendClientMessage(playerid, COLOR_GRAD1, stringer1);
    format(stringer1, sizeof(stringer1), "Вы получили %s руб. от %s[%i].",formatInt(params[1]),PI[playerid][pNames], playerid);
    SendClientMessage(params[0], COLOR_GRAD1, stringer1);
    PlayerPlaySound(params[0], 1052, 0.0, 0.0, 0.0);
    format(stringer1,sizeof(stringer1), "достал бумажник и передал деньги %s",PI[params[0]][pNames]);
    SetPlayerChatBubble(playerid,stringer1,COLOR_PURPLE,30.0,10000);
    return 1;
}
cmd:mxpell(playerid, params[])
{
    if(PI[playerid][pMember] != 7 && PI[playerid][pMember] != 16) return SendClientMessage(playerid,COLOR_WHITE,"Вы не сотрудник мэрии / новостей!");
    if(sscanf(params, "ds[128]",params[0],params[1])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /mxpell [id] [причина]");
    if(!ProxDetectorS(15.0, playerid, params[0])) return SendClientMessage(playerid, COLOR_GREY, "Игрок слишком далеко!");
    if(IsACop(params[0]) || IsAArm(params[0]) || PI[params[0]][pMember] == 4 || PI[params[0]][pMember] == 7) return SendClientMessage(playerid, COLOR_RED, "Вы не можете выгнать Госсударственного работника!");
    if(!IsPlayerConnected(params[0]) || params[0] == playerid) return 1;
    if(PI[playerid][pMember] == 7)
    {
        if(!IsPlayerInRangeOfPoint(playerid, 60.0, 688.8973,-27.8779,1201.8580)) return SendClientMessage(playerid, COLOR_GREY, "Необходимо находится в мэрии!");
        SetPlayerInteriorEx(params[0],0);
        SetPPos(params[0],1480.8832,-1769.0471,18.7958);
        SetPlayerFacingAngle(params[0], 0.3133);
        DisablePlayerCheckpoint(params[0]);
        CP[params[0]] = 0;
        SetPVarInt(params[0],"MerEnter",gettime()+300);
        format(stringer1,100,"Сотрудник: %s выставил вас за дверь мэрии. Причина: %s",PI[playerid][pNames],params[1]);
        SendClientMessage(params[0],COLOR_WHITE,stringer1);
        format(stringer1,100,"Вы выставили %s за дверь.",PI[params[0]][pNames]);
        SendClientMessage(playerid,COLOR_WHITE,stringer1);
    }
    else
    {
        if(!IsPlayerInRangeOfPoint(playerid, 60.0,1745.7302,-1915.4186,1961.1769)) return SendClientMessage(playerid, COLOR_GREY, "Необходимо находится в студии!");
        SetPlayerInteriorEx(params[0],0);
        SetPPos(params[0],1658.2383,-1693.4553,15.6094);
        SetPlayerFacingAngle(params[0], 175.8234);
        SetPlayerVirtualWorldEx(params[0], 0);
        DisablePlayerCheckpoint(params[0]);
        CP[params[0]] = 0;
        SetPVarInt(params[0],"NewsEnter",gettime()+300);
        format(stringer1,100,"Сотрудник: %s выставил вас за дверь студии. Причина: %s",PI[playerid][pNames],params[1]);
        SendClientMessage(params[0],COLOR_WHITE,stringer1);
        format(stringer1,100,"Вы выставили %s за дверь.",PI[params[0]][pNames]);
        SendClientMessage(playerid,COLOR_WHITE,stringer1);
    }
    return 1;
}
cmd:axpell(playerid, params[])
{
    if(PI[playerid][pMember] != 11) return SendClientMessage(playerid,COLOR_WHITE,"Вы не сотрудник автошколы!");
    if(sscanf(params, "ds[128]",params[0],params[1])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /axpell [id] [причина]");
    if(!ProxDetectorS(15.0, playerid, params[0])) return SendClientMessage(playerid, COLOR_GREY, "Игрок слишком далеко!");
    if(IsACop(params[0]) || IsAArm(params[0]) || PI[params[0]][pMember] == 4 || PI[params[0]][pMember] == 7) return SendClientMessage(playerid, COLOR_RED, "Вы не можете выгнать Госсударственного работника!");
    if(!IsPlayerConnected(params[0]) || params[0] == playerid) return 1;
    if(IsPlayerInRangeOfPoint(playerid, 30.0,-790.2884,1546.5732,1139.2369))
    {
        SetPlayerInteriorEx(params[0],0);
        SetPlayerVirtualWorldEx(params[0], 0);
        SetPPos(params[0],-2026.9436,-99.6266,35.1641);
        SetPlayerFacingAngle(params[0], 355.8038);
        SetPVarInt(params[0],"AvtoEnter",gettime()+300);
        format(stringer1,100,"Сотрудник: %s выставил вас за дверь автошколы. Причина: %s",PI[playerid][pNames],params[1]);
        SendClientMessage(params[0],COLOR_WHITE,stringer1);
        format(stringer1,100,"Вы выставили %s за дверь.",PI[params[0]][pNames]);
        SendClientMessage(playerid,COLOR_WHITE,stringer1);
    }
    return 1;
}
cmd:mdpell(playerid, params[])
{
    if(PI[playerid][pMember] != 4) return SendClientMessage(playerid,COLOR_WHITE,"Вы не врач!");
    if(sscanf(params, "ds[128]",params[0],params[1])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /mdpell [id] [причина]");
    if(!ProxDetectorS(15.0, playerid, params[0])) return SendClientMessage(playerid, COLOR_GREY, "Игрок слишком далеко!");
    if(IsACop(params[0]) || IsAArm(params[0]) || PI[params[0]][pMember] == 4 || PI[params[0]][pMember] == 7) return SendClientMessage(playerid, COLOR_RED, "Вы не можете выгнать Госсударственного работника!");
    if(!IsPlayerConnected(params[0]) || params[0] == playerid) return 1;
    if(!IsPlayerInRangeOfPoint(playerid, 50.0,347.8218,400.8753,1022.8019)) return SendClientMessage(playerid, COLOR_GREY, "Вы находитесь не на территории больницы.");
    PI[params[0]][pHospital] = 0;
    UpdatePlayerData(params[0], "pHospital", PI[params[0]][pHospital]);
    SetPPos(params[0],1177.1721,-1323.3220,14.0576);
    SetPlayerInteriorEx(params[0],0);
    SetPlayerFacingAngle(params[0], 270);
    SetPlayerVirtualWorldEx(params[0], 0);
    SetPVarInt(params[0],"MdEnter",gettime()+300);
    format(stringer1,100,"Сотрудник: %s выставил вас за дверь больницы. Причина: %s",PI[playerid][pNames],params[1]);
    SendClientMessage(params[0],COLOR_WHITE,stringer1);
    format(stringer1,100,"Вы выставили %s за дверь.",PI[params[0]][pNames]);
    SendClientMessage(playerid,COLOR_WHITE,stringer1);
    return 1;
}
cmd:money(playerid, params[])
{
    if(FD(playerid))
    {
        if(sscanf(params, "dd",params[0],params[1])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /money [ид] [сумма]");
        if(!IsPlayerConnected(params[0])) return 1;
        format(stringer1, sizeof(stringer1), "%s руб. переведены на счёт игроку %s",formatInt(params[1]),PI[params[0]][pNames]);
        SendClientMessage(playerid, COLOR_WHITE, stringer1);
        GiveBank(params[0], params[1]);
        format(stringer1, sizeof(stringer1), "Пополнение счёта на сумму: %s руб.", formatInt(params[1]));
        SendClientMessage(params[0], COLOR_WHITE, stringer1);
        format(stringer1, sizeof(stringer1), "Новый баланс в банке: %s руб.", formatInt(PI[params[0]][pBank]));
        SendClientMessage(params[0], COLOR_WHITE, stringer1);
        return 1;
    }
    SendClientMessage(playerid, -1, "Команда изъята из доступа");
    return 1;
}

cmd:avig(playerid, params[])
{
    if(PI[playerid][pAdmin] < 7) return 1;
    new nicks[24];
    if(sscanf(params, "s[24]s[100]",nicks, params[1])) return SendClientMessage(playerid, COLOR_WHITE, "Используйте: /avig [ник] [причина]");

    new ids = GetPlayerID(nicks);
    if(IsPlayerConnected(ids))
    {
        if(!PI[ids][pLeader] && !PI[ids][pAdmin]) return SendClientMessage(playerid, COLOR_WHITE, "Игрок не является администратором / лидером");
        if(playerid == ids) return SendClientMessage(playerid, COLOR_WHITE, "Невозможно выдать выговор самому себе.");
        PI[ids][pVigs] ++;
        if(PI[ids][pVigs] >= 3)
        {
            if(PI[ids][pLeader] && !PI[ids][pAdmin])
            {
                format(stringer1, sizeof(stringer1), "Администратор %s снял лидера фракции %s: %s (3/3). Причина: %s", PI[playerid][pNames], GetPlayerFracName[PI[ids][pMember]], PI[ids][pNames], params[1]);
                SendClientMessageToAll(COLOR_LIGHTRED, stringer1);
                format(stringer1, sizeof(stringer1), "снял лидеры фракции %s по причине (3/3): %s", GetPlayerFracName[PI[ids][pMember]], params[1]);
                Admin_Log(playerid, stringer1, nicks, gettime());
                SetPlayerSkinEx(ids, PI[ids][pChar], true);
                SetPlayerArmour(ids,0);
                SetPlayerHealth(ids,100);
                ResetWeapons(ids);
                PI[ids][pLeader] = 0;
                PI[ids][pMember] = 0;
                PI[ids][pRank] = 0;
                PI[ids][pVigs] = 0;
                PI[ids][pOdet] = 0;
                SetPlayerToTeamColor(ids);
                format(mysql_query_, sizeof(mysql_query_), "UPDATE `"TABLE_ACCOUNT"` SET `pMember` = '0', `pLeader` = '0', `pRank` = '0', `odet` = '0' , `vigs` = '0' WHERE `pID` = '%d'",PI[ids][pID]);
                mysql_function_query(connects, mysql_query_, false, "", "");
            }
            else if(PI[ids][pAdmin])
            {
                format(stringer1, sizeof(stringer1), "Главный администратор %s снял с %s права администратора (3/3). Причина: %s", PI[playerid][pNames], PI[ids][pNames], params[1]);
                SendAdminMessage(COLOR_LIGHTRED, stringer1);
                format(stringer1, sizeof(stringer1), "снял права администратора (3/3), по причине: %s", params[1]);
                Admin_Log(playerid, stringer1, nicks, gettime());
                DelAdmin(ids);
            }
        }
        else
        {
            if(PI[ids][pLeader] && !PI[ids][pAdmin])
            {
                format(stringer1, sizeof(stringer1), "Администратор %s выдал выговор лидеру фракции %s: %s (%d/3). Причина: %s", PI[playerid][pNames], GetPlayerFracName[PI[ids][pMember]], PI[ids][pNames],PI[ids][pVigs], params[1]);
                SendClientMessageToAll(COLOR_LIGHTRED, stringer1);
                format(stringer1, sizeof(stringer1), "выдал выговор лидеру фракции %s (%d/3) по причине: %s", GetPlayerFracName[PI[ids][pMember]],PI[ids][pVigs], params[1]);
                Admin_Log(playerid, stringer1, nicks, gettime());
                format(mysql_query_, sizeof(mysql_query_), "UPDATE `"TABLE_ACCOUNT"` SET `vigs` = '%d' WHERE `pID` = '%d'",PI[ids][pVigs], PI[ids][pID]);
                mysql_function_query(connects, mysql_query_, false, "", "");
            }
            else if(PI[ids][pAdmin])
            {
                format(stringer1, sizeof(stringer1), "Главный администратор %s выдал администратору %s выговор (%d/3). Причина: %s", PI[playerid][pNames], PI[ids][pNames],PI[ids][pVigs], params[1]);
                SendAdminMessage(COLOR_LIGHTRED, stringer1);
                format(stringer1, sizeof(stringer1), "выдал выговор (%d/3), по причине: %s", PI[ids][pVigs], params[1]);
                Admin_Log(playerid, stringer1, nicks, gettime());
                format(mysql_query_, sizeof(mysql_query_), "UPDATE `"TABLE_ACCOUNT"` SET `vigs` = '%d' WHERE `pID` = '%d'",PI[ids][pVigs],PI[ids][pID]);
                mysql_function_query(connects, mysql_query_, false, "", "");
            }
        }
        return 1;
    }
    format(mysql_query_,sizeof(mysql_query_), "SELECT `pAdmin`, `pLeader`, `vigs` FROM `"TABLE_ACCOUNT"` WHERE `Name` = '%s'", nicks);
    mysql_function_query(connects, mysql_query_, true, "SetVig_Date", "dssd", playerid,nicks,params[1],2);
    return 1;
}
cmd:aunvig(playerid, params[])
{
    if(PI[playerid][pAdmin] < 7) return 1;
    new nicks[24];
    if(sscanf(params, "s[24]s[100]",nicks, params[1])) return SendClientMessage(playerid, COLOR_WHITE, "Используйте: /aunvig [ник] [причина]");



    new ids = GetPlayerID(nicks);
    if(IsPlayerConnected(ids))
    {
        if(!PI[ids][pLeader] && !PI[ids][pAdmin]) return SendClientMessage(playerid, COLOR_WHITE, "Игрок не является администратором / лидером");
        if(playerid == ids && !FD(playerid)) return SendClientMessage(playerid, COLOR_WHITE, "Невозможно снять выговор самому себе.");
        if(PI[ids][pVigs] <= 0) return SendClientMessage(playerid, COLOR_WHITE, "Выговоры отсутствуют.");
        PI[ids][pVigs] --;
        if(PI[ids][pLeader] && !PI[ids][pAdmin])
        {
            format(stringer1, sizeof(stringer1), "Администратор %s снял выговор лидеру фракции %s: %s (%d/3). Причина: %s", PI[playerid][pNames], GetPlayerFracName[PI[ids][pMember]], PI[ids][pNames],PI[ids][pVigs], params[1]);
            SendClientMessageToAll(COLOR_LIGHTRED, stringer1);
            format(stringer1, sizeof(stringer1), "снял выговор лидеру фракции (%s) (%d/3) по причине: %s", GetPlayerFracName[PI[ids][pMember]],PI[ids][pVigs], params[1]);
            Admin_Log(playerid, stringer1, nicks, gettime());
            format(mysql_query_, sizeof(mysql_query_), "UPDATE `"TABLE_ACCOUNT"` SET `vigs` = '%d' WHERE `pID` = '%d'",PI[ids][pVigs], PI[ids][pID]);
            mysql_function_query(connects, mysql_query_, false, "", "");
        }
        else if(PI[ids][pAdmin])
        {
            format(stringer1, sizeof(stringer1), "Главный администратор %s снял администратору %s выговор (%d/3). Причина: %s", PI[playerid][pNames], PI[ids][pNames],PI[ids][pVigs], params[1]);
            SendAdminMessage(COLOR_LIGHTRED, stringer1);
            format(stringer1, sizeof(stringer1), "снял выговор (%d/3), по причине: %s", PI[ids][pVigs], params[1]);
            Admin_Log(playerid, stringer1, nicks, gettime());
            format(mysql_query_, sizeof(mysql_query_), "UPDATE `"TABLE_ACCOUNT"` SET `vigs` = '%d' WHERE `pID` = '%d'",PI[ids][pVigs],PI[ids][pID]);
            mysql_function_query(connects, mysql_query_, false, "", "");
        }
        return 1;
    }
    format(mysql_query_,sizeof(mysql_query_), "SELECT `pAdmin`, `pLeader`, `vigs` FROM `"TABLE_ACCOUNT"` WHERE `Name` = '%s'", nicks);
    mysql_function_query(connects, mysql_query_, true, "SetVig_Date", "dssd", playerid,nicks,params[1],1);
    return 1;
}
cmd:fvig(playerid, params[])
{
    if(!PI[playerid][pMember]) return 1;
    if(PI[playerid][pRank] < gFractionRank[PI[playerid][pMember]-1][3]) return SendMes(playerid, -1, "Доступно с %i ранга", gFractionRank[PI[playerid][pMember]-1][3]);
    if(sscanf(params, "ds[100]",params[0],params[1])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /fvig [id] [причина]");
    if(!IsPlayerConnected(params[0])) return 1;
    if(PI[params[0]][pMember] != PI[playerid][pMember] || PI[params[0]][pAdmin] || PI[params[0]][pRank] >= gFractionRank[PI[playerid][pMember]-1][3]) return SendClientMessage(playerid,COLOR_GREY,"Игрок не в вашей фракции / администратор / лидер / зам");
    PI[params[0]][pVigs] ++;
    if(PI[params[0]][pVigs] >= 3)
    {
        format(stringer1, sizeof(stringer1), "[F] %s уволил игрока %s (3/3). Причина: %s", PI[playerid][pNames], PI[params[0]][pNames], params[1]);
        SendFamilyMessage(PI[playerid][pMember],COLOR_LIGHTRED, stringer1);
        SetPlayerSkinEx(params[0], PI[params[0]][pChar], true);
        SetPlayerArmour(params[0],0);
        SetPlayerHealth(params[0],100);
        ResetWeapons(params[0]);
        PI[params[0]][pMember] = 0;
        PI[params[0]][pRank] = 0;
        PI[params[0]][pVigs] = 0;
        PI[params[0]][pOdet] = 0;
        SetPlayerToTeamColor(params[0]);
        format(mysql_query_, sizeof(mysql_query_), "UPDATE `"TABLE_ACCOUNT"` SET `pMember` = '0', `pRank` = '0', `odet` = '0' , `vigs` = '0' WHERE `pID` = '%d'",PI[params[0]][pID]);
        mysql_function_query(connects, mysql_query_, false, "", "");
    }
    format(mysql_query_, sizeof(mysql_query_), "UPDATE `"TABLE_ACCOUNT"` SET `vigs` = '%d' WHERE `pID` = '%d'",PI[params[0]][pVigs],PI[params[0]][pID]);
    mysql_function_query(connects, mysql_query_, false, "", "");
    format(stringer1, sizeof(stringer1), "[F] %s выдал выговор игроку %s (%d/3). Причина: %s", PI[playerid][pNames], PI[params[0]][pNames], PI[params[0]][pVigs], params[1]);
    SendFamilyMessage(PI[playerid][pMember],COLOR_LIGHTRED, stringer1);
    return 1;
}
cmd:funvig(playerid, params[])
{
    if(!PI[playerid][pMember]) return 1;
    if(PI[playerid][pRank] < gFractionRank[PI[playerid][pMember]-1][3]) return SendMes(playerid, -1, "Доступно с %i ранга", gFractionRank[PI[playerid][pMember]-1][3]);
    if(sscanf(params, "ds[100]",params[0],params[1])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /funvig [id] [причина]");
    if(!IsPlayerConnected(params[0])) return 1;
    if(PI[params[0]][pMember] != PI[playerid][pMember] || PI[params[0]][pAdmin] || PI[params[0]][pRank] >= gFractionRank[PI[playerid][pMember]-1][3]) return SendClientMessage(playerid,COLOR_GREY,"Игрок не в вашей фракции / администратор / лидер / зам");
    if(PI[params[0]][pVigs] <= 0) return 1;
    PI[params[0]][pVigs] --;
    format(mysql_query_, sizeof(mysql_query_), "UPDATE `"TABLE_ACCOUNT"` SET `vigs` = '%d' WHERE `pID` = '%d'",PI[params[0]][pVigs],PI[params[0]][pID]);
    mysql_function_query(connects, mysql_query_, false, "", "");
    format(stringer1, sizeof(stringer1), "[F] %s снял выговор игроку %s (%d/3). Причина: %s", PI[playerid][pNames], PI[params[0]][pNames], PI[params[0]][pVigs], params[1]);
    SendFamilyMessage(PI[playerid][pMember],COLOR_LIGHTRED, stringer1);
    return 1;
}
alias:jail("prison");
cmd:jail(playerid, params[])
{
    if(PI[playerid][pAdmin] < 4) return 1;
    if(PI[playerid][pVigs] > 2) return SendClientMessage(playerid, -1, "{FF6600}На вашем аккаунте выговор, некоторые команды были заблокированы. Снимите выговор — {FFFFFF}/adonate, {FF6600}чтобы снять блокировку команд.");
    if(sscanf(params, "dds[128]",params[0],params[1],params[2])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /jail [id] [минуты] [причина]");
    if(!IsPlayerConnected(params[0])) return 1;
    if(params[1] > 70 || params[1] <= 0) return SendClientMessage(playerid,COLOR_WHITE,"Ошибка: Нельзя меньше 1 и больше 70");
   // if((FD(params[0]) && !FD(playerid)) || IsIpFound(params[2])) return 1;
    //RemoveBadWords(params[2]);
    ResetWeapons(params[0]);
    SetPlayerInteriorEx(params[0], 0);
    switch(random(2)) {
    case 0: SetPPos(params[0],524.0659,333.5089,1201.8594), SetPlayerFacingAngle(params[0], 120.3213);
    case 1: SetPPos(params[0],523.9069,339.0927,1201.8594), SetPlayerFacingAngle(params[0], 102.7743);
    }
    TogglePlayerControllable(params[0], false);
    SetTimerEx("UnFreez", 2900, 0, "d", params[0]);
    PI[params[0]][pJailed] = 4;
    PI[params[0]][pMestoJail] = 0;
    PI[params[0]][pJailTime] = gettime() + params[1]*60;
    PI[playerid][pJailAdm]++;
    format(stringer1, sizeof(stringer1), "Администратор %s посадил вас в деморган на %i минут(ы). Причина: %s",PI[playerid][pNames],params[1], params[2]);
    SendClientMessage(params[0], COLOR_LIGHTRED, stringer1);
    format(stringer1, sizeof(stringer1), "[A] %s посадил игрока %s в деморган на %i минут(ы). Причина: %s", PI[playerid][pNames],PI[params[0]][pNames], params[1], params[2]);
    SendAdminMessage(COLOR_GREY,stringer1);
    Admin_Log(playerid, stringer1, PI[params[0]][pNames], gettime());
    return 1;