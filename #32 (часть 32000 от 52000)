stock youtube_message(text[])
{
    foreach(new i : Player)
    {
        if ( PI[i][pYtADM] == 0 )continue;
        SendClientMessage ( i, 0xCD5C5CFF, text ) ;
    }
    return 1;
}

CMD:yt(playerid, params[])
{
    if(!PI[playerid][pYtADM])return SendClientMessage(playerid, -1, "Вы не блогер!");
    if( sscanf(params, "s[128]", params [0]))return SendClientMessage(playerid, -1, "Введите: /yt [текст]" ) ;
    new text_string [ 144 ];
    format(text_string,sizeof(text_string),"[YT-%d] %s[%d]: %s", PI[playerid][pYtADM], PI[playerid][pNames], playerid, params[0]);
    youtube_message(text_string);
    return 1;
}

CMD:makeyt(playerid, params[])
{
    if(!FD(playerid)) return 1;
    if(sscanf(params, "dd", params[0], params[1])) return SendClientMessage(playerid, COLOR_WHITE, !"Используйте: /makeyt [id игрока] [уровень]");
    if(!IsPlayerConnected(params[0])) return SendClientMessage(playerid, COLOR_WHITE, !"Игрок не найден.");
    if(params[1] > 2 || params[1] < 0) return SendClientMessage(playerid, COLOR_WHITE, !"Уровень может быть от 0 до 2.");

    new scm_string[144];
    if(params[1] == 0)
    {
        format(scm_string, sizeof(scm_string), "%s[%d] отобрал у Вас блогерскую адмиинку.", PI[playerid][pNames], playerid);
        SendClientMessage(params[0], COLOR_WHITE, scm_string);

        PI[params[0]][pYtADM] = 0;
        UpdatePlayerData(params[0], "pYTADM", PI[params[0]][pYtADM]);
    }
    else
    {
        format(scm_string, sizeof(scm_string), "%s[%d] назначил Вас блогером (Уровень: %d).", PI[playerid][pNames], playerid, params[1]);
        SendClientMessage(params[0], COLOR_WHITE, scm_string);

        PI[params[0]][pYtADM] = params[1];
        UpdatePlayerData(params[0], "pYTADM", PI[params[0]][pYtADM]);
    }
    return 1;
}

cmd:namestore(playerid, params[])
{
    if(PI[playerid][pAdmin] < 5) return 1;
    if(sscanf(params, "d", params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /namestore [ID]");
    if(params[0] <= 0) return 1;
    format(mysql_query_, sizeof(mysql_query_), "SELECT * FROM  `setname` WHERE `account` = '%d'", PI[params[0]][pID]);
    mysql_function_query(connects, mysql_query_, true, "Logs", "ddd", playerid, 5, 0);
    return 1;
}

