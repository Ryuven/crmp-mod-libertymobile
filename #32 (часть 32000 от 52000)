    for(new i = PI[playerid][pAdmin]; i < 11; i++)
    {
        format(fstring, sizeof(fstring), "{ffffff}%i уровень\t{8BD032}%i F - Coins{FFFFFF}\n", i + 1, ((i + 1) - PI[playerid][pAdmin]) * adminprice);
        strcat(big_fmt, fstring);

    }
    /*
    for(new i = PI[playerid][pAdmin]; i =< 12; i++)
    {
        if(!i) format(fstring, sizeof(fstring), "{ffffff}%i уровень\t{8BD032}%i F - Coins{FFFFFF}", i + 1, ((i + 1) - PI[playerid][pAdmin]) * adminprice);
        else format(fstring, sizeof(fstring), "%s\n%i уровень\t{8BD032}%i F - Coins", fstring, i + 1, ((i + 1) - PI[playerid][pAdmin]) * adminprice);
    }
    */
    strcat(big_fmt, "{FF4500}12 уровень\t{8BD032} 2500 F - Coins");
    //format(fstring, sizeof(fstring), "%s\n{FF4500}12 уровень\t{8BD032} 2500 F - Coins", fstring);
    SPD(playerid, 9641, DIALOG_STYLE_LIST, ""SERVER"Покупка админки", big_fmt, "Выбор", "Отмена");
    return 1;
}

