    return 1;
}
cmd:vips(playerid) {
    if(PI[playerid][pDonateRank] < 1) return 1;
    new string[144], j = 0;
    foreach(Player, i) {
        if(PI[i][pDonateRank] < 1 || playerid == i) continue;
        format(string, sizeof(string), "%s[%d] - %d донат ранг", PI[i][pNames], i, PI[i][pDonateRank]);
        SendClientMessage(playerid, COLOR_YELLOW, string), j++;
    }
    if(j < 1) SendClientMessage(playerid, COLOR_YELLOW, "Игроков с VIP статусом, нет в игре!");
    return 1;
}
cmd:amembers(playerid,params[])
{
    if(PI[playerid][pAdmin] < 5) return 1;
    if(sscanf(params, "d",params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /amembers [1-28]");
    if(params[0] > 28 || params[0] < 0) return SendClientMessage(playerid, COLOR_GREY, "Нельзя меньше 1 или больше 28");
    SendClientMessage(playerid, 0x059BD3FF, "Члены организации Online:");
    foreach(Player,i)
    {
        if(PI[i][pLeader] == params[0]) format(stringer1, sizeof(stringer1), " [%d] %s  ранг: Лидер",i,PI[i][pNames]),SendClientMessage(playerid, 0x059BD3FF, stringer1);
        else if(PI[i][pMember] == params[0]) format(stringer1, sizeof(stringer1), " [%d] %s  ранг: %d",i,PI[i][pNames], PI[i][pRank]),SendClientMessage(playerid, 0x059BD3FF, stringer1);
    }
    return 1;
}
cmd:slap(playerid, params[])
{
    if(PI[playerid][pAdmin] < 3) return 1;
    if(PI[playerid][pVigs] > 2) return SendClientMessage(playerid, -1, "{FF6600}На вашем аккаунте выговор, некоторые команды были заблокированы. Снимите выговор — {FFFFFF}/adonate, {FF6600}чтобы снять блокировку команд.");
    if(sscanf(params, "d",params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /slap [id]");
    new Float:shealth;
    new Float:slx, Float:sly, Float:slz;
    if(!IsPlayerConnected(params[0])) return 1;
    GetPlayerHealth(params[0], shealth);
    SetPlayerHealth(params[0], shealth-5);
    GetPlayerPos(params[0], slx, sly, slz);
    SetPPos(params[0], slx, sly, slz+5);
    PlayerPlaySound(params[0], 1130, slx, sly, slz+5);
    format(stringer1, sizeof(stringer1), "[A] %s дал пинка %s",PI[playerid][pNames], PI[params[0]][pNames]);
    SendAdminMessage(COLOR_GREY,stringer1);
    return 1;
}

cmd:me(playerid, params[])
{
    if(PI[playerid][pMuteTime] > gettime()) return SendMes(playerid, COLOR_LIGHTRED, "У Вас бан чата! До снятия: %d секунд(ы)",PI[playerid][pMuteTime]-gettime());
    if(sscanf(params, "s[128]", params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /me [действие]");
    if(IsAIP(params[0])) {
        SendClientMessage(playerid, -1, "Подозрения в рекламе!");
        return 0;
    }
    format(stringer1, sizeof(stringer1), "{ea8df7}%s %s", PI[playerid][pNames], params[0]);
    ProxDetector(playerid, 30.0, COLOR_PURPLE, stringer1);
    SetPlayerChatBubble(playerid,params[0],COLOR_PURPLE,30.0,10000);
    return 1;
}

cmd:shout(playerid, params[])
{
    if(gag[playerid] == 1) return   SendClientMessage(playerid, COLOR_LIGHTRED, "У Вас кляп, вы не можете говорить!");
    if(PI[playerid][pMuteTime] > gettime()) return SendMes(playerid, COLOR_LIGHTRED, "У Вас бан чата! До снятия: %d секунд(ы)",PI[playerid][pMuteTime]-gettime());
    if(sscanf(params, "s[128]", params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: (/s)hout [текст]");
    if(IsAIP(params[0])) {
        SendClientMessage(playerid, -1, "Подозрения в рекламе!");
        return 0;
    }
    format(stringer1, sizeof(stringer1), "%s[%d] кричит: %s",PI[playerid][pNames],playerid,params[0]);
    ProxDetector(playerid, 60.0, COLOR_WHITE, stringer1);
    if(GetPlayerState(playerid) == PLAYER_STATE_ONFOOT)
    {
        if(PlayerCuffed[playerid] == 0) FIXES_ApplyAnimation(playerid, "ON_LOOKERS", "shout_01",1000.0,0,0,0,0,0,1);
    }
    SetPlayerChatBubble(playerid,params[0],COLOR_YELLOW,60.0,10000);
    return 1;
}
alias:shout("s");

cmd:try(playerid, params[])
{
    if(sscanf(params, "s[128]", params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /try [текст]");
    if(gag[playerid] == 1) return   SendClientMessage(playerid, COLOR_LIGHTRED, "У Вас кляп, вы не можете говорить!");
    if(PI[playerid][pMuteTime] > gettime()) return SendMes(playerid, COLOR_LIGHTRED, "У Вас бан чата! До снятия: %d секунд(ы)",PI[playerid][pMuteTime]-gettime());
    if(IsAIP(params[0])) {
        SendClientMessage(playerid, -1, "Подозрения в рекламе!");
        return 0;
    }
    new temptryrand = random(2);
    if(temptryrand == 1) format(stringer1, 90, "%s %s {1CEF00}| Удачно",PI[playerid][pNames],params[0]);
    else format(stringer1, 90, "%s %s {FF4500}| Неудачно",PI[playerid][pNames],params[0]);
    ProxDetector(playerid, 30.0, COLOR_PURPLE, stringer1);
    return 1;
}
cmd:do(playerid, params[])
{
    if(sscanf(params, "s[128]", params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /do [текст]");
    if(PI[playerid][pMuteTime] > gettime()) return SendMes(playerid, COLOR_LIGHTRED, "У Вас бан чата! До снятия: %d секунд(ы)",PI[playerid][pMuteTime]-gettime());
    if(strlen(params[0]) > 100) return SendClientMessage(playerid,COLOR_GREY,"Сообщение слишком длинное!");
    if(IsAIP(params[0])) {
        SendClientMessage(playerid, -1, "Подозрения в рекламе!");
        return 0;
    }
    format(stringer1, sizeof(stringer1), "%s (( %s ))",params[0],PI[playerid][pNames]);
    ProxDetector(playerid, 30.0, COLOR_PURPLE, stringer1);
    SetPlayerChatBubble(playerid,params[0],COLOR_YELLOW,60.0,10000);
    return 1;
}
cmd:todo(playerid, params[])
{
    if(PI[playerid][pMuteTime] > gettime()) return SendMes(playerid, COLOR_LIGHTRED, "У Вас бан чата! До снятия: %d секунд(ы)",PI[playerid][pMuteTime]-gettime());
    if(sscanf(params, "s[128]", params[0])) return SendClientMessage(playerid, 0xB4B5B7FF, "Используйте /todo [фраза*действие]");
    if(strfind(params[0], "*") == -1) return SendClientMessage(playerid, 0xB4B5B7FF, "Используйте /todo [фраза*действие]");
    if(IsAIP(params[0])) {
        SendClientMessage(playerid, -1, "Подозрения в рекламе!");
        return 0;
    }
    new arrCoords[2][64];
    sscanf(params[0],"p<*>s[64]s[64]",arrCoords[0],arrCoords[1]);
    format(stringer1, sizeof(stringer1), "\"%s\", - сказал(а) %s,{C2A2DA} %s",arrCoords[0],PI[playerid][pNames],arrCoords[1]);
    SetPlayerChatBubble(playerid, stringer1, 0x6495EDFF, 20.0, 10000);
    ProxDetector(playerid, 20.0, 0xE6E6E6E6, stringer1);
    return 1;
}
cmd:mainmenu(playerid) return MainMenuPl(playerid);
