cmd:advertise(playerid, params[])
{
    if(sscanf(params, "s[128]",params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: (/ad)vertise [текст]");
    new len = strlen(params[0]);
    if(len > 100) return SendClientMessage(playerid,COLOR_GREY,"Максимальная длина объявления - 100 символов");
    if(PI[playerid][pMuteTime] > gettime()) return SendClientMessage(playerid, TEAM_CYAN_COLOR, "У Вас бан чата!");
    if(gAdvertCount >= MAX_ADVERT_COUNT) return SendClientMessage(playerid,COLOR_GREY,"К сожалению, очередь на объявления занята, попробуйте позже");
    if(adverttime > gettime()) return SendClientMessage(playerid,COLOR_WHITE,"Нельзя посылать больше одного сообщения в 30 секунд!");
    if(IsAIP(params[0])) {
        SendClientMessage(playerid, -1, "Подозрения в рекламе!");
        return 0;
    }
    SetPVarString(playerid,"advert",params[0]);
    format(stringer,sizeof(stringer),"{B8B8B8}Цена объявления: {00FF5E}$%d.\n{B8B8B8}Вы действительно хотите отправить объявление?",len*addd);
    SPD(playerid,8454,0,"Объявление",stringer,"Да","Нет");
    return 1;
}

cmd:vipad(playerid, params[])
{
    if(PI[playerid][pDonateRank] == 0) return SendClientMessage(playerid,COLOR_WHITE,"У вас нет VIP аккаунта!");
    if(sscanf(params, "s[128]",params[0])) return SendClientMessage(playerid, COLOR_WHITE, !"{afafaf}Используйте: /v(ip)ad [текст]");
    new len = strlen(params[0]);
    if(len > 100) return SendClientMessage(playerid,COLOR_GREY,"Максимальная длина объявления - 100 символов");
    if(PI[playerid][pMuteTime] > gettime()) return SendClientMessage(playerid, TEAM_CYAN_COLOR, "У Вас бан чата!");
    if(gVipAdvertCount >= MAX_VIP_ADVERT_COUNT) return SendClientMessage(playerid,COLOR_GREY,"К сожалению, очередь на объявления занята, попробуйте позже");
    if(vipadverttime > gettime()) return SendClientMessage(playerid,COLOR_WHITE,"Нельзя посылать больше одного VIP-объявления в 1 минуту!");
    if(IsAIP(params[0])) {
        SendClientMessage(playerid, COLOR_WHITE, !"{afafaf}Обнаружено подозрение в рекламе!");
        return 0;
    }
    SetPVarString(playerid,"advert",params[0]);
    format(stringer,sizeof(stringer),"{FFFFFF}Текст объявления: "SERVER"%s\n{FFFFFF}Количество символов: "SERVER"%d\n\n{afafaf}Вы действительно желаете отправить объявление?", params[0], len);
    SPD(playerid, 8455, DIALOG_STYLE_MSGBOX, !""SERVER"VIP-Объявление", stringer, !"Отправить", !"Отменить");
    return 1;
}
alias:vipad("vad");

cmd:edit(playerid)
{
    if(PI[playerid][pAdmin] < 5) return SendClientMessage(playerid, COLOR_WHITE, !"{afafaf}Редактировать VIP-объявления можно с 5 уровня админки.");
    return ShowVipAdvertList(playerid);
}

stock ShowVipAdvertList(playerid)
{
    stringer[0] = EOS;
    new data[3][30],index;
    format(data[0],30,"[{33AA33}Проверено{FFFFFF}]");
    format(data[1],30,"[{FFFF00}Проверяется{FFFFFF}]");
    for(new i = 0; i < MAX_VIP_ADVERT_COUNT; i++)
    {
        if(!gVipAdvert[i][vadBusy]) format(stringer,sizeof(stringer),"%s%d. -\n",stringer,i+1);
        else
        {
            if(gVipAdvert[i][vadCheking]) index = 1;
            else if(gVipAdvert[i][vadCheked]) index = 0;
            else index = 2;
            format(stringer,sizeof(stringer),"%s%d. %s %s\n",stringer,i+1,gVipAdvert[i][vadSender],data[index]);
        }
    }
    return SPD(playerid, 11550, DIALOG_STYLE_LIST, !""SERVER"VIP-объявления",stringer, !"Далее", !"Закрыть");
}

