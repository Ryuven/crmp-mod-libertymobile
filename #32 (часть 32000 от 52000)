cmd:dadm(playerid, params[])
{
    if(GetPlayerAdmin(playerid))
    {
        extract params -> new to_player; else return SendClientMessage(playerid, 0xCECECEFF, "Используйте: /daadm [id игрока]");
        if(!IsPlayerConnected(to_player)) return 1;

        format(stringer1, sizeof(stringer1), "[%s]: %s[%d] для %s[%d]: Пополните счёт аккаунта на сайте "SERVER_WEBSITE" и купите в /mm > Дополнительно",PI[playerid][pAdmin] ? ("A"): ("H") ,PI[playerid][pNames], playerid, PI[params[0]][pNames],params[0]);
        SendClientMessage(to_player, 0xff9945FF, stringer1);
        SendAdminMessage(0xff9945FF, stringer1);
    }
    return 1;
}

