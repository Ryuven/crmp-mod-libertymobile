cmd:setcmd(playerid, params[]) {
    if(FD(playerid)) {
        new Namets[24], CMD_name[32], CMD_allow, query[144];
        if(sscanf(params, "s[24]s[32]d", Namets, CMD_name, CMD_allow)) return SendClientMessage(playerid, 0xCECECEFF, "Используйте: /setcmd [Имя] [Команда] [1/0]\n Посмотреть список команд которые можно выдать - /cmdinfo");
        //      SendClientMessage(playerid, 0xCECECEFF, "Посмотреть список команд которые можно выдать - /cmdinfo");
        new CMD_i;
        for (CMD_i = 0; CMD_i < CMD_NUM; CMD_i++)
        {
            if(!strcmp(CMD_names[CMD_i], CMD_name, true)) break;
        }
        if (CMD_i >= CMD_NUM) return SendClientMessage(playerid, -1, "{ffffff}Введёной команды не существует");
        format(query, sizeof(query), "SELECT * FROM `accounts` WHERE `Name` = '%s'", Namets);
        mysql_function_query(connects, query, true, "setcmd_SQL_callback" ,"dsdd", playerid, Namets, CMD_i, CMD_allow);
    }
    return 1;
}
