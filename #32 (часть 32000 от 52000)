{
    new str[256],sctring[2000];
    if(PI[playerid][pAdmin] >= 12) format(str,sizeof(str),"{ffffff}Команды:\n"); strcat(sctring,str);
    if(PI[playerid][pAdmin] >= 12) format(str,sizeof(str),"1. /setadmin - {FF6600}Команда для выдачи админки | Не выдавать без ведома владельца{ffffff} \n"); strcat(sctring,str);
    if(PI[playerid][pAdmin] >= 12) format(str,sizeof(str),"2. /setleader - {FF6600}Команда для выдачи лидерки{ffffff} \n"); strcat(sctring,str);
    if(PI[playerid][pAdmin] >= 12) format(str,sizeof(str),"3. /banip - {FF6600}Команда для выдачи бана по IP адресу{ffffff} \n"); strcat(sctring,str);
    if(PI[playerid][pAdmin] >= 12) format(str,sizeof(str),"4. /gzcolor - {FF6600}Команда для перекраски территорий банд{ffffff} \n"); strcat(sctring,str);
    if(PI[playerid][pAdmin] >= 12) format(str,sizeof(str),"5. /ghetto {FF6600}Команда для Запрета/Разрешения каптов бандам{ffffff} \n"); strcat(sctring,str);
    if(PI[playerid][pAdmin] >= 12) format(str,sizeof(str),"6. /avig {FF6600}Команда для выдачи Админ-Выговоров{ffffff} \n"); strcat(sctring,str);
    if(PI[playerid][pAdmin] >= 12) format(str,sizeof(str),"7. /aunvig {FF6600}Команда для снятия Админ-Выговоров{ffffff} \n"); strcat(sctring,str);
    if(PI[playerid][pAdmin] >= 12) format(str,sizeof(str),"8. /ban {FF6600}Команда для выдачи бана (Вместе с ней дается /offban){ffffff} \n"); strcat(sctring,str);
    if(PI[playerid][pAdmin] >= 12) format(str,sizeof(str),"9. /warn {FF6600}Команда для выдачи предупреждения(Вместе с ней дается /offwarn){ffffff} \n"); strcat(sctring,str);
    SPD(playerid,0000,DIALOG_STYLE_MSGBOX,"{65c8ff}Список команд которые можно выдавать | >>",sctring,"Принять","");
    return 1;
}
cmd:setadmin(playerid,params[])
{
    if(PI[playerid][pAdmin] < 7) return 1;
    new NameA[24], lvl;
    if(sscanf(params,"s[24]d",NameA,lvl)) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /setadmin [Имя] [Уровень]");
    if(!strcmp(NameA, "Paphsheia")) return SendClientMessage(playerid, COLOR_WHITE, !"{afafaf}Вы не можете изменить лвл ему.");
    if(lvl > 12 || lvl < 0) return SendClientMessage(playerid,COLOR_WHITE,"Ошибка: Нельзя меньше 1 и больше 12 ");
    new query[144];
    format(query, sizeof(query), "SELECT * FROM `accounts` WHERE `Name` = '%s' LIMIT 1", NameA);
    mysql_function_query(connects, query, true, "SetAdmin", "dds", playerid, lvl, NameA);
    return 1;
}


forward SetYouTube(playerid, lvl, name[]);
public SetYouTube(playerid, lvl, name[])
{
    new rows, fields;
    cache_get_data(rows, fields);
    if(rows)
    {
        new query[256], string[144];
        format(query, sizeof(query), "UPDATE `accounts` SET `youtube` = '%d' WHERE `Name` = '%s'",lvl, name);
        mysql_function_query(connects, query, false, "", "");
        print(query);

        PI[playerid][youtube] = lvl;

        format(string, sizeof(string), "%s был %s пост ютубера",lvl ? ("Поставлен на") : ("Снят с"), name);
        SendClientMessage(playerid, COLOR_WHITE, string);
    }
    else return SendClientMessage(playerid, COLOR_WHITE, "Данный игрок не зарегестрирован на этом сервере!");
    return 1;
}
stock YouTube(playerid)
{
    if(PI[playerid][youtube])
    return 1;
    else
    return 0;
}
forward SetAdmin(playerid, level, name[]);
public SetAdmin(playerid, level, name[]) {
    new rows, fields;
    cache_get_data(rows, fields);
    if(rows) {
        new lvl = cache_get_field_content_int(0, "pAdmin", connects);
        if(lvl < 1 && level < 1) return SendClientMessage(playerid, COLOR_WHITE, "Игрок не администратор!");
        new query[256], string[144];
        format(query, sizeof(query), "UPDATE `accounts` SET `pAdmin` = '%d' WHERE `Name` = '%s'", level, name);
        mysql_function_query(connects, query, false, "", "");
        if(lvl < 1 && level > 0) {
            format(string, sizeof(string), "%s был назначен на пост администратора %d уровня", name, level);
            SendClientMessage(playerid, COLOR_WHITE, string);
            format(string, sizeof(string), ""SERVER"%s назначил Вас администратором %d уровня. Введите - /alogin.", PI[playerid][pNames], level);
        } else if(lvl > 0 && level > 0) {
            format(string, sizeof(string), "Вы изменили уровень админ прав с %d на %d у %s", lvl, level, name);
            SendClientMessage(playerid, COLOR_WHITE, string);
            format(string, sizeof(string), ""SERVER"%s изменил Вам уровень админ прав с %d на %d.", PI[playerid][pNames], lvl, level);
        } else if(level == 0) {
            format(string, sizeof(string), "Вы сняли админ права с %s", name);
            SendClientMessage(playerid, COLOR_WHITE, string);
            format(string, sizeof(string), ""SERVER"%s снял Вас c должности администратора сервера.", PI[playerid][pNames]);
        }

        Admin_Log(playerid, string, name, gettime());

        foreach(Player, i) {
            if(!IsPlayerConnected(i)) continue;
            if(strcmp(PI[i][pNames], name, true) != 0) continue;
            if(level == 0) {
                SendClientMessage(i, COLOR_WHITE, string);
                DelAdmin(i);
                break;
            }
            PI[i][pAdmin] = level;
            SendClientMessage(i, COLOR_WHITE, string);
            break;
        }
    } else return SendClientMessage(playerid, COLOR_WHITE, "Данный игрок не зарегестрирован на этом сервере!");
    return 1;
}

