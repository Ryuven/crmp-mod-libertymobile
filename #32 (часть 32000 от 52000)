cmd:asl(playerid, params[]) {
    if(GetPlayerAdmin(playerid)) {
        if(sscanf(params, "d",params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /asl [id]");
        if(!IsPlayerConnected(params[0]) || params[0] == playerid) return 1;
        if(PI[playerid][pAdmin] > 0) format(stringer1, sizeof(stringer1), "Администратор: %s[%d] для %s[%d]: Начал работать по вашей жалобе, спасибо за помощь ! | Приятной игры на "SERVER_NAME"",PI[playerid][pNames], playerid, PI[params[0]][pNames], params[0]);
        else format(stringer1, sizeof(stringer1), "{FF9945}Ответ от %s: Начал работать по вашей жалобе, спасибо за помощь !",PI[playerid][pNames]), PI[playerid][pHotvet] ++;
        SendClientMessage(params[0], 0xff9945FF, stringer1);
        if(PI[playerid][pAdmin] > 0) { format(stringer1, sizeof(stringer1), "Администратор: %s[%d] для %s[%d]: Передал администрации проекта, ожидайте ! | Приятной игры на "SERVER_NAME"",PI[playerid][pNames], playerid, PI[params[0]][pNames], params[0]); }
        else { format(stringer1, sizeof(stringer1), "{FF9945}[H] %s: Передал администрации проекта, ожидайте ! | Для %s[%d]",PI[playerid][pNames],PI[params[0]][pNames], params[0]); }
        SendAdminMessage(0xff9945FF, stringer1);
    }
    return 1;
}
