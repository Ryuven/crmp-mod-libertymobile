cmd:mpwin(playerid, params[])
{
    if(PI[playerid][pAdmin] < 4) return 1;

    if(sscanf(params,"dd",params[0],params[1])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /mpwin [ID] [Очки]");
    if(!IsPlayerConnected(params[0]) || params[0] == playerid) return 1;
    if(params[1] > 1000 || params[1] < 50) return SendClientMessage(playerid,COLOR_WHITE,"Ошибка: Нельзя меньше 50 и больше 1000!");
    PI[params[0]][pDonatemoney2] += params[1];
    format(stringer1, sizeof(stringer1), "Администратор %s: МП окончено, победил %s! Он получает %d DonateMoney (/donate)", PI[playerid][pNames],PI[params[0]][pNames], params[1]);
    SendClientMessageToAll(0xFFCD00ff, stringer1);
    SendMes(params[0],COLOR_WHITE, "Ваш донатный счёт: %d DonatMoney {FFFFFF}(( Используйте /donate ))", PI[params[0]][pDonatemoney2]);

    return 1;
}
cmd:mpfd(playerid, params[])
{
    if(!FD(playerid)) return 1;

    if(sscanf(params,"dd",params[0],params[1])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /mpfd [ID] [Количество]");
    if(!IsPlayerConnected(params[0]) || params[0] == playerid) return 1;
    if(params[1] > 50 || params[1] < 5) return SendClientMessage(playerid,COLOR_WHITE,"Ошибка: Нельзя меньше 5 и больше 50!");
    PI[params[0]][pDonateRubNew] += params[1];
    format(stringer1, sizeof(stringer1), "Основатель %s: МП окончено, победил %s! Он получает %d F - Coins (/donaterub)", PI[playerid][pNames],PI[params[0]][pNames], params[1]);
    SendClientMessageToAll(0xFFCD00ff, stringer1);
    SendMes(params[0],COLOR_WHITE, "Ваш донатный счёт: %d F - Coins {FFFFFF}(( Используйте /donaterub ))", PI[params[0]][pDonateRubNew]);

    return 1;
}
cmd:ceject(playerid, params[])
{
    if(sscanf(params, "d",params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /ceject [id игрока]");
    if(!ProxDetectorS(8.0, playerid, params[0])) return SendClientMessage(playerid, COLOR_GREY, "Игрок не рядом с вами!");
    if(PI[playerid][pMember] == 1 || PI[playerid][pMember] == 2 || PI[playerid][pMember] == 10 || PI[playerid][pMember] == 21 || PI[playerid][pMember] == 25) {
        if(IsPlayerInRangeOfPoint(playerid, 10.0,1568.2023,-1694.1594,5.8906)) {
            SetPlayerInteriorEx(params[0],6);
            SetPPos(params[0],253.9933,86.9706,1002.4453);
            SetPlayerFacingAngle(params[0], 278.0311); }
        else if(IsPlayerInRangeOfPoint(playerid, 10.0,-1590.8289,716.0479,-5.2422)) {
            SetPlayerInteriorEx(params[0],10);
            SetPPos(params[0],217.3147,121.3382,999.0156);
            SetPlayerFacingAngle(params[0], 272.8898); }
        else if(IsPlayerInRangeOfPoint(playerid, 10.0,2295.1934,2451.8459,10.8203)) {
            SetPlayerInteriorEx(params[0],3);
            SetPlayerVirtualWorldEx(params[0], 122);
            SetPPos(params[0],238.5033,141.1578,1003.0234);
            SetPlayerFacingAngle(params[0], 359.1617); }
        else return SendClientMessage(playerid, 0x6ab1ffaa, "Вы затолкали приступника в участок."); }
    return 1;
}
cmd:withdraw(playerid, params[])
{
    if(!IsPlayerInRangeOfPoint(playerid, 10.0,2308.8201,-13.2660,26.7422))
    if(sscanf(params, "d", params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /withdraw [Сумма]");
    if(params[0] < 1 || params[0] > 10000000) { SendClientMessage(playerid, COLOR_GREY, "Нельзя снять 1 руб.а и более 10000000!"); return 1; }
    if(PI[playerid][pBank] < params[0]) return SendClientMessage(playerid, COLOR_GRAD1, "У вас нет столько денег!");
    GiveMoney(playerid, params[0]);
    PI[playerid][pBank] -= params[0];
    UpdatePlayerData(playerid, "pBank", PI[playerid][pBank]);
    format(stringer1, sizeof(stringer1), "Вы сняли %s руб.. Остаток: %s руб.",formatInt(params[0]), formatInt(PI[playerid][pBank]));
    SendClientMessage(playerid, COLOR_YELLOW, stringer1);
    return 1;
}
cmd:newswithdraw(playerid, params[])
{
    if(PI[playerid][pMember] != 16 || PI[playerid][pRank] < 12) return 1;
    if(sscanf(params, "d", params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /newswithdraw [Сумма]");
    if(params[0] < 1 || params[0] > 10000000) return SendClientMessage(playerid, COLOR_GREY, "Нельзя снять меньше 1 и более 1000000 руб.!");
    if(FracBank[0][fLsnews] < params[0]) return SendClientMessage(playerid, COLOR_GREY, "В банке Россия 24 нет столько денег");
    FracBank[0][fLsnews] -= params[0];
    GiveMoney(playerid, params[0]);
    SendMes(playerid, 0x6ab1ffaa, "Вы сняли с банка Россия 24: %s руб.", formatInt(params[0]));
    SetOtherInt("FracBank[0][fLsnews]", FracBank[0][fLsnews]);
    return 1;
}
cmd:shield(playerid)
{
    if(TeamPain[playerid] != 0 && StartPaintBall == 2) return SendClientMessage(playerid,COLOR_GREY,"Использовать щит на PaintBall запрещено.");
    if(IsACop(playerid) || IsAArm(playerid))
    {
        if(shield[playerid] == 0)
        {
            SetPlayerHealth(playerid, 999), shield[playerid] = 1;
            SendClientMessage(playerid, COLOR_WHITE, !"{afafaf}Вы надели щит. Ваше здоровье увеличено до 999.");
        }
        else { SendClientMessage(playerid, COLOR_WHITE, !"{afafaf}Вы сняли щит. Ваше здоровье уменьшено до 50."), SetPlayerHealth(playerid, 50), shield[playerid] = 0; }
    }
    else return SendClientMessage(playerid, COLOR_GREY, "Вы не Полицейский/Военный/ФБР");
    return 1;
}
cmd:gomp(playerid)
{
    if(TeleportInfo_players <= 0) return SendClientMessage(playerid, COLOR_WHITE, !"Места на мероприятие закончились!");

    if(Teleport == 0) return SendClientMessage(playerid, -1, "{AC0000}[Ошибка] {ffffff}На данный момент мероприятий нет.");
    else ShowPlayerDialog(playerid, 3333, DIALOG_STYLE_MSGBOX, "{FFCD00}Телепортация", "{ffffff}Вы уверены, что хотите попасть в место проведения мероприятия?", "Да", "Нет");
    return 1;
}
cmd:news(playerid, params[])
{
    if(gag[playerid] == 1) return   SendClientMessage(playerid, COLOR_LIGHTRED, "У Вас кляп, вы не можете говорить!");
    if(PI[playerid][pMuteTime] > gettime()) return SendClientMessage(playerid, TEAM_CYAN_COLOR, "У Вас бан чата!");
    if(sscanf(params, "s[128]", params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /news [текст]");
    if(gag[playerid] == 1) return SendClientMessage(playerid, COLOR_LIGHTRED, "У Вас кляп, вы не можете говорить!");
    switch(PI[playerid][pMember])
    {
    case 16:
        {
            new newcar = GetPlayerVehicleID(playerid);
            if(GetVehicleData(newcar, V_ACTION_TYPE) == VEHICLE_ACTION_TYPE_NEWS || IsPlayerInRangeOfPoint(playerid, 60.0, -2662.6042,-1820.9193,4.1045)) { } else { SendClientMessage(playerid, COLOR_GRAD1, "Вы не в репортёрской машине / вертолёте / офисе!"); return 1;}
            format(stringer1, sizeof(stringer1), "<  > %s[%d]: %s",PI[playerid][pNames], playerid, params[0]);
            SendClientMessageToAll(0x0073b7AA, stringer1);
        }
    }
    return 1;
}

cmd:live(playerid, params[])
{
    if(PI[playerid][pMuteTime] > gettime()) return SendClientMessage(playerid, TEAM_CYAN_COLOR, "У Вас бан чата!");
    if(sscanf(params, "d", params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /live [id/name]");
    if(!IsPlayerConnected(params[0]) || params[0] == playerid) return SendClientMessage(playerid, COLOR_WHITE, "Ошибка: Этого игрока нет, или вы указали свой ид");
    if(!ProxDetectorS(5.0, playerid, params[0])) return SendClientMessage(playerid, COLOR_GREY, "Человек далеко от вас!");
    new newcar = GetPlayerVehicleID(playerid);
    switch(PI[playerid][pMember])
    {
    case 16:
        {
            if(GetVehicleData(newcar, V_ACTION_TYPE) == VEHICLE_ACTION_TYPE_NEWS || IsPlayerInRangeOfPoint(playerid, 60.0, -2662.6042,-1820.9193,4.1045)) { } else { SendClientMessage(playerid, COLOR_GREY, "Вы не в репортёрской машине / вертолёте / офисе!"); return 1;}
            if(TalkingLivels[playerid] != INVALID_PLAYER_ID)
            {
                SendClientMessage(playerid, 0x6495EDFF, "Прямой эфир завершён");
                SendClientMessage(TalkingLivels[playerid], 0x6495EDFF, "Прямой эфир завершён");
                TogglePlayerControllable(playerid, 1);
                TogglePlayerControllable(TalkingLivels[playerid], 1);
                TalkingLivels[TalkingLivels[playerid]] = INVALID_PLAYER_ID;
                TalkingLivels[playerid] = INVALID_PLAYER_ID;
                return 1;
            }
            format(stringer1, sizeof(stringer1), "Вы предложили %s снять интервью",PI[params[0]][pNames]);
            SendClientMessage(playerid, 0x6495EDFF, stringer1);
            format(stringer1, sizeof(stringer1), "%s предлагает вам снять интервью",PI[playerid][pNames]);
            SendClientMessage(params[0], 0x6495EDFF, stringer1);
            SendClientMessage(params[0], COLOR_WHITE, "Введите /accept livels, чтобы согласиться");
            LiveOfferls[params[0]] = playerid;
        }
    }
    return 1;
}
alias:live("l");

