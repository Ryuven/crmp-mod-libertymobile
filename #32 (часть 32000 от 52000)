    if(GetPlayerAdmin(playerid)) {
        if(sscanf(params, "d",params[0])) return SendClientMessage(playerid, COLOR_WHITE, "Введите: /kk [id]");
        if(!IsPlayerConnected(params[0]) || params[0] == playerid) return 1;
        if(PI[playerid][pAdmin] > 0) format(stringer1, sizeof(stringer1), "Администратор: %s[%d] для %s[%d]: Оставьте жалобу на форуме - forum.WIPE PROJECT",PI[playerid][pNames], playerid, PI[params[0]][pNames], params[0]);
        else format(stringer1, sizeof(stringer1), "{FF9945}Ответ от %s: Оставьте жалобу на форуме - forum.WIPE PROJECT",PI[playerid][pNames]), PI[playerid][pHotvet] ++;
        SendClientMessage(params[0], 0xff9945FF, stringer1);
        if(PI[playerid][pAdmin] > 0) { format(stringer1, sizeof(stringer1), "Администратор: %s[%d] для %s[%d]: Оставьте жалобу на форуме - forum.WIPE PROJECT",PI[playerid][pNames], playerid, PI[params[0]][pNames], params[0]); }
