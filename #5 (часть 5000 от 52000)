    PlayerTextDrawBackgroundColor(playerid, autosalon_PTD[playerid][11], 255);
    PlayerTextDrawFont(playerid, autosalon_PTD[playerid][11], 2);
    PlayerTextDrawSetProportional(playerid, autosalon_PTD[playerid][11], 1);
    PlayerTextDrawSetShadow(playerid, autosalon_PTD[playerid][11], 0);

    autosalon_PTD[playerid][12] = CreatePlayerTextDraw(playerid, 594.8836, 211.4357, ">>>"); // пусто
