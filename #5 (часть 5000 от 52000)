    case 441,448,449,450,462,464,465,481,501,509,510,537,538,564,569,570,590,591,594,606,607,608,610,611: return 1;
    }
    return 0;
}
stock IsAMatsCar(carid)
{
    if(GetVehicleData(carid, V_ACTION_TYPE) == VEHICLE_ACTION_TYPE_ARMY && GetVehicleData(carid, V_ACTION_ID) != 0) return 1;
    return 0;
}
stock PutPlayerInVeh(playerid, vehicleid, seatid)
{
    VehicleID[playerid] = vehicleid;
    return PutPlayerInVehicle(playerid, vehicleid, seatid);
}
